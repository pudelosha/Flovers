  "translations": {
    "en": {
      "common_name": "Jewel Orchid",
      "description": "Ludisia discolor is a terrestrial orchid grown for its dark, velvety leaves with contrasting veins. It prefers low light, steady moisture, and warm indoor temperatures."
    },
    "pl": {
      "common_name": "Storczyk klejnot",
      "description": "Ludisia discolor to naziemny storczyk uprawiany głównie dla ozdobnych liści. Wymaga małego światła, umiarkowanego podlewania i ciepła."
    },
    "de": {
      "common_name": "Juwelenorchidee",
      "description": "Ludisia discolor ist eine terrestrische Orchidee mit dekorativen Blättern. Sie gedeiht bei wenig Licht, gleichmäßiger Feuchte und Wärme."
    },
    "fr": {
      "common_name": "Orchidée bijou",
      "description": "Ludisia discolor est une orchidée terrestre appréciée pour son feuillage. Elle aime une lumière faible et un sol légèrement humide."
    },
    "it": {
      "common_name": "Orchidea gioiello",
      "description": "Ludisia discolor è un'orchidea terrestre coltivata per le foglie decorative. Preferisce poca luce e umidità costante."
    },
    "es": {
      "common_name": "Orquídea joya",
      "description": "Ludisia discolor es una orquídea terrestre valorada por su follaje. Requiere poca luz y riego moderado."
    },
    "pt": {
      "common_name": "Orquídea joia",
      "description": "Ludisia discolor é uma orquídea terrestre cultivada pelas folhas ornamentais. Cresce bem com pouca luz e solo úmido."
    },
    "ar": {
      "common_name": "أوركيد الجوهرة",
      "description": "لوديسيا ديسكلور أوركيد أرضي يزرع لأوراقه المزخرفة. يفضل الإضاءة المنخفضة ورطوبة معتدلة."
    },
    "hi": {
      "common_name": "ज्वेल ऑर्किड",
      "description": "लुडिसिया डिस्कलर एक स्थलीय ऑर्किड है जो अपनी सजावटी पत्तियों के लिए उगाया जाता है। इसे कम रोशनी और नियमित नमी पसंद है।"
    },
    "zh": {
      "common_name": "宝石兰",
      "description": "Ludisia discolor 是一种以观叶著称的地生兰花。适合低光环境并保持基质微湿。"
    },
    "ja": {
      "common_name": "ジュエルオーキッド",
      "description": "ルディシア・ディスカラーは葉を観賞する地生ランです。弱い光と安定した湿度を好みます。"
    },
    "ko": {
      "common_name": "보석란",
      "description": "Ludisia discolor는 잎이 아름다운 지생 난초입니다. 약한 빛과 일정한 수분을 선호합니다."
    }
  },
