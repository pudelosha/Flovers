  "translations": {
    "en": {
      "common_name": "Common Sorrel",
      "description": "Rumex acetosa, or common sorrel, is a perennial herb with tangy, green leaves. It is often used in salads or soups."
    },
    "pl": {
      "common_name": "Szczaw zwyczajny",
      "description": "Rumex acetosa, czyli szczaw zwyczajny, to roślina wieloletnia o kwaśnych, zielonych liściach. Często używana w sałatkach lub zupach."
    },
    "de": {
      "common_name": "Sauerampfer",
      "description": "Rumex acetosa, oder Sauerampfer, ist ein mehrjähriges Kraut mit säuerlichen, grünen Blättern. Häufig in Salaten oder Suppen verwendet."
    },
    "fr": {
      "common_name": "Oseille commune",
      "description": "Rumex acetosa, ou oseille commune, est une plante vivace à feuilles vertes et acides. Utilisée fréquemment dans les salades ou soupes."
    },
    "it": {
      "common_name": "Acetosa comune",
      "description": "Rumex acetosa, o acetosa comune, è una pianta perenne con foglie verdi e acidule. Usata frequentemente in insalate o zuppe."
    },
    "es": {
      "common_name": "Acedera común",
      "description": "Rumex acetosa, o acedera común, es una planta perenne con hojas verdes y ácidas. Se usa comúnmente en ensaladas o sopas."
    },
    "pt": {
      "common_name": "Azedinha",
      "description": "Rumex acetosa, ou azedinha, é uma planta perene com folhas verdes e ácidas. Usada frequentemente em saladas ou sopas."
    },
    "ar": {
      "common_name": "الحنظل المشترك",
      "description": "Rumex acetosa، أو الحنظل المشترك، هو نبات معمر ذو أوراق خضراء وحمضية. يستخدم عادة في السلطات أو الحساء."
    },
    "hi": {
      "common_name": "आम ख़ट्टा",
      "description": "Rumex acetosa, या आम ख़ट्टा, एक बारहमासी पौधा है जिसमें खट्टे, हरे पत्ते होते हैं। इसका उपयोग आमतौर पर सलाद या सूप में किया जाता है."
    },
    "zh": {
      "common_name": "常见酸模",
      "description": "Rumex acetosa，或常见酸模，是一种多年生草本植物，具有酸味的绿色叶子。常用于沙拉或汤中。"
    },
    "ja": {
      "common_name": "ノコギリソウ",
      "description": "Rumex acetosa、またはノコギリソウは、酸味のある緑色の葉を持つ多年草です。サラダやスープに使われることがよくあります。"
    },
    "ko": {
      "common_name": "일반 쑥",
      "description": "Rumex acetosa, 또는 일반 쑥은 신맛이 나는 초록색 잎을 가진 다년생 허브입니다. 일반적으로 샐러드나 수프에 사용됩니다."
    }
  },
