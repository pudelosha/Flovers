  "translations": {
    "en": {
      "common_name": "Crystal Philodendron",
      "description": "A striking plant with large, glossy leaves. It thrives in medium light and warm, humid environments."
    },
    "pl": {
      "common_name": "Filodendron Kryształowy",
      "description": "Roślina o dużych, błyszczących liściach. Rośnie w średnim świetle i ciepłym, wilgotnym otoczeniu."
    },
    "de": {
      "common_name": "Kristall-Philodendron",
      "description": "Eine auffällige Pflanze mit großen, glänzenden Blättern. Sie gedeiht bei mittlerem Licht und in warmen, feuchten Umgebungen."
    },
    "fr": {
      "common_name": "Philodendron Cristal",
      "description": "Une plante saisissante avec de grandes feuilles brillantes. Elle prospère sous une lumière moyenne et dans des environnements chauds et humides."
    },
    "it": {
      "common_name": "Philodendron Cristallo",
      "description": "Una pianta sorprendente con grandi foglie lucide. Cresce in luce media e in ambienti caldi e umidi."
    },
    "es": {
      "common_name": "Filodendro Cristal",
      "description": "Una planta llamativa con grandes hojas brillantes. Prosperan con luz media y en ambientes cálidos y húmedos."
    },
    "pt": {
      "common_name": "Filodendro Cristal",
      "description": "Uma planta impressionante com grandes folhas brilhantes. Cresce em luz média e em ambientes quentes e úmidos."
    },
    "ar": {
      "common_name": "فيلوديندرون الكريستال",
      "description": "نبتة لافتة للنظر ذات أوراق لامعة وكبيرة. تنمو في الضوء المتوسط وفي بيئات دافئة ورطبة."
    },
    "hi": {
      "common_name": "क्रिस्टल फिलोडेंड्रोन",
      "description": "यह एक आकर्षक पौधा है जिसकी बड़ी, चमकदार पत्तियां होती हैं। यह मध्यम रोशनी और गर्म, नम वातावरण में बढ़ता है।"
    },
    "zh": {
      "common_name": "水晶蔓绿绳",
      "description": "这是一种引人注目的植物，具有大而光亮的叶子。它在中等光照和温暖潮湿的环境中茁壮成长。"
    },
    "ja": {
      "common_name": "クリスタルフィロデンドロン",
      "description": "大きくて光沢のある葉を持つ印象的な植物です。中程度の光と暖かく湿った環境で育ちます。"
    },
    "ko": {
      "common_name": "크리스탈 필로덴드론",
      "description": "크고 광택 있는 잎을 가진 인상적인 식물입니다. 중간 정도의 빛과 따뜻하고 습한 환경에서 자랍니다."
    }
  },
