  "translations": {
    "en": {
      "common_name": "Aconite-leaved Begonia",
      "description": "Begonia aconitifolia is a tropical begonia species native to Brazil. It is grown for its deeply cut decorative leaves and delicate pink flowers. The plant prefers bright indirect light and consistently moist soil."
    },
    "pl": {
      "common_name": "Begonia akonitowa",
      "description": "Begonia aconitifolia pochodzi z Brazylii i jest ceniona za ozdobne, głęboko klapowane liście. Wymaga jasnego, rozproszonego światła i umiarkowanie wilgotnego podłoża."
    },
    "de": {
      "common_name": "Eisenhutblättrige Begonie",
      "description": "Begonia aconitifolia stammt aus Brasilien und hat dekorative, tief gelappte Blätter. Sie bevorzugt helles, indirektes Licht und gleichmäßig feuchte Erde."
    },
    "fr": {
      "common_name": "Bégonia à feuilles d’aconit",
      "description": "Begonia aconitifolia est originaire du Brésil et appréciée pour son feuillage décoratif. Elle aime une lumière indirecte et un sol régulièrement humide."
    },
    "it": {
      "common_name": "Begonia a foglie di aconito",
      "description": "Begonia aconitifolia è una specie tropicale brasiliana con foglie ornamentali. Cresce bene con luce indiretta e terreno costantemente umido."
    },
    "es": {
      "common_name": "Begonia de hojas de acónito",
      "description": "Begonia aconitifolia es originaria de Brasil y destaca por sus hojas decorativas. Prefiere luz indirecta brillante y riego regular."
    },
    "pt": {
      "common_name": "Begônia de folhas de acônito",
      "description": "Begonia aconitifolia é nativa do Brasil e cultivada pela folhagem ornamental. Necessita luz indireta e solo sempre levemente úmido."
    },
    "ar": {
      "common_name": "بيغونيا أوراق الأقونيط",
      "description": "بيغونيا أكونيتيفوليا نبات استوائي من البرازيل يتميز بأوراقه الزخرفية. يفضل الضوء غير المباشر والتربة الرطبة."
    },
    "hi": {
      "common_name": "एकोनाइट पत्ती बेगोनिया",
      "description": "Begonia aconitifolia ब्राज़ील की उष्णकटिबंधीय प्रजाति है। इसे सजावटी पत्तियों और अप्रत्यक्ष रोशनी में उगाया जाता है।"
    },
    "zh": {
      "common_name": "乌头叶秋海棠",
      "description": "Begonia aconitifolia 原产于巴西，以其装饰性叶片著称。适合明亮散射光和持续湿润的土壤。"
    },
    "ja": {
      "common_name": "アコニティフォリア・ベゴニア",
      "description": "ベゴニア・アコニティフォリアはブラジル原産の観葉植物です。明るい間接光と湿った土を好みます。"
    },
    "ko": {
      "common_name": "아코니티폴리아 베고니아",
      "description": "Begonia aconitifolia는 브라질 원산의 열대 베고니아입니다. 밝은 간접광과 촉촉한 토양을 선호합니다."
    }
  },
