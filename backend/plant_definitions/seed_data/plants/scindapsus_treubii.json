  "translations": {
    "en": {
      "common_name": "Treubii Pothos",
      "description": "Scindapsus treubii is an epiphytic plant native to Southeast Asia. It is known for its striking dark green leaves with silver streaks."
    },
    "pl": {
      "common_name": "Pothos Treubii",
      "description": "Scindapsus treubii to epifitczna roślina pochodząca z Azji Południowo-Wschodniej. Charakteryzuje się ciemnozielonymi liśćmi z srebrnymi smugami."
    },
    "de": {
      "common_name": "Treubii Efeu",
      "description": "Scindapsus treubii ist eine epiphytische Pflanze aus Südostasien. Sie ist bekannt für ihre dunklen grünen Blätter mit silbernen Streifen."
    },
    "fr": {
      "common_name": "Pothos de Treubii",
      "description": "Scindapsus treubii est une plante épiphyte originaire du sud-est asiatique. Elle est connue pour ses feuilles vertes foncées avec des rayures argentées."
    },
    "it": {
      "common_name": "Pothos di Treubii",
      "description": "Scindapsus treubii è una pianta epifita originaria del Sud-Est asiatico. È conosciuta per le sue foglie verde scuro con strisce argentate."
    },
    "es": {
      "common_name": "Pothos de Treubii",
      "description": "Scindapsus treubii es una planta epífita originaria del sudeste asiático. Es conocida por sus hojas verde oscuras con rayas plateadas."
    },
    "pt": {
      "common_name": "Pothos Treubii",
      "description": "Scindapsus treubii é uma planta epífita nativa do Sudeste Asiático. É conhecida por suas folhas verdes escuras com listras prateadas."
    },
    "ar": {
      "common_name": "بوثوس تريوبي",
      "description": "Scindapsus treubii هو نبات ميثي موطنه جنوب شرق آسيا. ويشتهر بأوراقه الخضراء الداكنة ذات الخطوط الفضية."
    },
    "hi": {
      "common_name": "ट्रेबुई पोटहोस",
      "description": "Scindapsus treubii एक एपिफाइटिक पौधा है जो दक्षिण-पूर्व एशिया का मूल निवासी है। इसके गहरे हरे पत्तों पर चांदी की धारियाँ होती हैं।"
    },
    "zh": {
      "common_name": "特鲁比金钱树",
      "description": "Scindapsus treubii 是一种原产于东南亚的附生植物。以其深绿色的叶子和银色条纹而闻名。"
    },
    "ja": {
      "common_name": "トレウビイポトス",
      "description": "Scindapsus treubii は東南アジア原産の着生植物です。濃い緑色の葉に銀色のストライプが特徴です。"
    },
    "ko": {
      "common_name": "트레우비 포토스",
      "description": "Scindapsus treubii는 동남아시아 원산의 기생 식물입니다. 짙은 녹색의 잎과 은색 줄무늬로 유명합니다."
    }
  },
