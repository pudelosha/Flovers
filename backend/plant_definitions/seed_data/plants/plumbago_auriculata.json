  "translations": {
    "en": {
      "common_name": "Cape plumbago",
      "description": "Plumbago auriculata is a shrub known for its blue flowers. It thrives in full sun and moderate watering."
    },
    "pl": {
      "common_name": "Plumbago Cape",
      "description": "Plumbago auriculata to krzew o niebieskich kwiatach. Preferuje pełne słońce i umiarkowane podlewanie."
    },
    "de": {
      "common_name": "Plumbago",
      "description": "Plumbago auriculata ist ein Strauch mit blauen Blumen. Er gedeiht in voller Sonne und bei mäßiger Bewässerung."
    },
    "fr": {
      "common_name": "Plumbago",
      "description": "Plumbago auriculata est un arbuste aux fleurs bleues. Il préfère le plein soleil et un arrosage modéré."
    },
    "it": {
      "common_name": "Plumbago",
      "description": "Plumbago auriculata è un arbusto con fiori blu. Cresce meglio al sole pieno con annaffiature moderate."
    },
    "es": {
      "common_name": "Plumbago",
      "description": "Plumbago auriculata es un arbusto con flores azules. Prefiere pleno sol y riego moderado."
    },
    "pt": {
      "common_name": "Plumbago",
      "description": "Plumbago auriculata é um arbusto com flores azuis. Prefere pleno sol e rega moderada."
    },
    "ar": {
      "common_name": "بلومباجو",
      "description": "Plumbago auriculata هو شجيرة ذات زهور زرقاء. يفضل أشعة الشمس الكاملة والري المعتدل."
    },
    "hi": {
      "common_name": "प्लंबैगो",
      "description": "Plumbago auriculata एक झाड़ी है जिसमें नीले फूल होते हैं। यह पूर्ण सूर्य और मध्यम पानी में उगता है।"
    },
    "zh": {
      "common_name": "蓝花龙船花",
      "description": "Plumbago auriculata 是一种有蓝色花朵的灌木。它喜欢充足的阳光和适中的浇水。"
    },
    "ja": {
      "common_name": "プランバゴ",
      "description": "Plumbago auriculata は青い花を持つ低木です。直射日光と適度な水やりを好みます。"
    },
    "ko": {
      "common_name": "플럼바고",
      "description": "Plumbago auriculata는 파란색 꽃을 가진 관목입니다. 완전한 햇빛과 적당한 물주기를 선호합니다."
    }
  },
