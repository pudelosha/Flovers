  "translations": {
    "en": {
      "common_name": "Morning Glory",
      "description": "Ipomoea purpurea is a fast-growing annual vine known for its trumpet-shaped purple flowers. It blooms best in full sun and warm conditions. Commonly grown outdoors on trellises or fences."
    },
    "pl": {
      "common_name": "Wilec purpurowy",
      "description": "Ipomoea purpurea to szybko rosnące pnącze jednoroczne o fioletowych kwiatach. Najlepiej kwitnie w pełnym słońcu i cieple. Często sadzona przy podporach."
    },
    "de": {
      "common_name": "Prunkwinde",
      "description": "Ipomoea purpurea ist eine einjährige Kletterpflanze mit violetten Blüten. Sie bevorzugt volle Sonne und warme Standorte. Wird häufig an Zäunen gezogen."
    },
    "fr": {
      "common_name": "Volubilis pourpre",
      "description": "Ipomoea purpurea est une plante grimpante annuelle aux fleurs violettes. Elle aime le plein soleil et la chaleur. Souvent cultivée en extérieur."
    },
    "it": {
      "common_name": "Ipomea purpurea",
      "description": "Ipomoea purpurea è una rampicante annuale dai fiori viola. Cresce meglio al sole e con temperature calde. Ideale per pergolati."
    },
    "es": {
      "common_name": "Gloria de la mañana",
      "description": "Ipomoea purpurea es una enredadera anual de rápido crecimiento con flores púrpuras. Prefiere pleno sol y clima cálido. Muy usada en jardines."
    },
    "pt": {
      "common_name": "Glória-da-manhã",
      "description": "Ipomoea purpurea é uma trepadeira anual de flores roxas. Desenvolve-se melhor em sol pleno e calor. Comum em cercas."
    },
    "ar": {
      "common_name": "مجد الصباح",
      "description": "Ipomoea purpurea نبات متسلق سنوي بأزهار أرجوانية. يفضل الشمس الكاملة والطقس الدافئ. يزرع خارجياً غالباً."
    },
    "hi": {
      "common_name": "मॉर्निंग ग्लोरी",
      "description": "Ipomoea purpurea एक तेजी से बढ़ने वाली बेल है जिसमें बैंगनी फूल होते हैं। इसे पूरी धूप और गर्म मौसम पसंद है।"
    },
    "zh": {
      "common_name": "牵牛花",
      "description": "Ipomoea purpurea 是一年生攀援植物，开紫色花。喜全日照和温暖环境。常用于庭院栽培。"
    },
    "ja": {
      "common_name": "アサガオ",
      "description": "Ipomoea purpurea は紫色の花を咲かせる一年草のつる植物です。日当たりと暖かさを好みます。"
    },
    "ko": {
      "common_name": "나팔꽃",
      "description": "Ipomoea purpurea는 보라색 꽃을 피우는 한해살이 덩굴식물입니다. 햇빛과 따뜻한 환경을 좋아합니다."
    }
  },
