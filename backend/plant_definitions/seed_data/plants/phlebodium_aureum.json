  "translations": {
    "en": {
      "common_name": "Blue Star Fern",
      "description": "Phlebodium aureum is a tropical fern known for its distinctive blue-green, wavy fronds. It thrives in shaded, humid environments."
    },
    "pl": {
      "common_name": "Paproć niebieska gwiazda",
      "description": "Phlebodium aureum to tropikalna paproć, znana z charakterystycznych niebiesko-zielonych, falistych liści. Rośnie w zacienionych, wilgotnych środowiskach."
    },
    "de": {
      "common_name": "Blaue Sternfarn",
      "description": "Phlebodium aureum ist ein tropischer Farn, bekannt für seine auffälligen blau-grünen, gewellten Wedel. Er gedeiht in schattigen, feuchten Umgebungen."
    },
    "fr": {
      "common_name": "Fougère étoile bleue",
      "description": "Phlebodium aureum est une fougère tropicale connue pour ses frondes bleu-vert ondulées. Elle prospère dans des environnements ombragés et humides."
    },
    "it": {
      "common_name": "Felce stella blu",
      "description": "Phlebodium aureum è una felce tropicale nota per le sue fronde ondulate di colore blu-verde. Cresce in ambienti ombreggiati e umidi."
    },
    "es": {
      "common_name": "Helecho estrella azul",
      "description": "Phlebodium aureum es un helecho tropical conocido por sus frondas onduladas de color azul-verde. Crece en entornos sombreados y húmedos."
    },
    "pt": {
      "common_name": "Samambaia estrela azul",
      "description": "Phlebodium aureum é uma samambaia tropical conhecida por suas frondes onduladas azul-esverdeadas. Ela cresce em ambientes sombreados e úmidos."
    },
    "ar": {
      "common_name": "سرخس النجمة الزرقاء",
      "description": "Phlebodium aureum هو سرخس استوائي معروف بسعفه المتموجة باللون الأزرق والأخضر. ينمو في بيئات مظللة ورطبة."
    },
    "hi": {
      "common_name": "ब्लू स्टार फर्न",
      "description": "Phlebodium aureum एक उष्णकटिबंधीय फर्न है जो अपनी विशिष्ट नीली-हरे, लहराती पत्तियों के लिए जाना जाता है। यह छायादार, आर्द्र वातावरण में उगता है।"
    },
    "zh": {
      "common_name": "蓝星蕨",
      "description": "Phlebodium aureum 是一种热带蕨类植物，以其独特的蓝绿色波状叶子而闻名。它在阴湿的环境中生长。"
    },
    "ja": {
      "common_name": "ブルースターファーン",
      "description": "Phlebodium aureum は、特徴的な青緑色の波状の葉で知られる熱帯性のシダです。日陰で湿った環境で成長します。"
    },
    "ko": {
      "common_name": "블루 스타 고사리",
      "description": "Phlebodium aureum는 파란색-녹색의 물결 모양 잎으로 알려진 열대 식물입니다. 그늘지고 습한 환경에서 자랍니다."
    }
  },
