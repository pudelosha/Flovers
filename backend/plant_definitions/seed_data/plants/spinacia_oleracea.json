  "translations": {
    "en": {
      "common_name": "Spinach",
      "description": "Spinach is a fast-growing, leafy vegetable. It thrives in cooler climates with moderate sunlight."
    },
    "pl": {
      "common_name": "Szpinak",
      "description": "Szpinak to szybko rosnąca roślina liściasta. Dobrze rośnie w chłodniejszych klimatach z umiarkowanym nasłonecznieniem."
    },
    "de": {
      "common_name": "Spinat",
      "description": "Spinat ist ein schnell wachsendes, blättriges Gemüse. Er gedeiht in kühleren Klimazonen mit mäßigem Sonnenlicht."
    },
    "fr": {
      "common_name": "Épinard",
      "description": "L'épinard est une plante à feuilles qui pousse rapidement. Il prospère dans les climats plus frais avec un ensoleillement modéré."
    },
    "it": {
      "common_name": "Spinaci",
      "description": "Gli spinaci sono una pianta a crescita rapida e a foglia. Crescono bene nei climi più freschi con luce solare moderata."
    },
    "es": {
      "common_name": "Espinaca",
      "description": "La espinaca es una planta de rápido crecimiento y con hojas. Crece bien en climas frescos con luz solar moderada."
    },
    "pt": {
      "common_name": "Espinafre",
      "description": "O espinafre é uma planta de crescimento rápido e com folhas. Cresce bem em climas mais frescos com luz solar moderada."
    },
    "ar": {
      "common_name": "السبانخ",
      "description": "السبانخ هو نبات سريع النمو ذو أوراق. ينمو جيدًا في المناخات الباردة مع ضوء الشمس المعتدل."
    },
    "hi": {
      "common_name": "पालक",
      "description": "पालक एक तेजी से बढ़ने वाली पत्तेदार सब्जी है। यह ठंडी जलवायु में मध्यम धूप में अच्छी तरह से उगता है।"
    },
    "zh": {
      "common_name": "菠菜",
      "description": "菠菜是一种快速生长的叶菜类植物。它在凉爽的气候中，适度的阳光下生长良好。"
    },
    "ja": {
      "common_name": "ほうれん草",
      "description": "ほうれん草は成長が早い葉物野菜です。涼しい気候と適度な日光でよく育ちます。"
    },
    "ko": {
      "common_name": "시금치",
      "description": "시금치는 빠르게 자라는 잎채소입니다. 서늘한 기후에서 적당한 햇빛을 받으며 잘 자랍니다."
    }
  },
