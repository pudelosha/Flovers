  "translations": {
    "en": {
      "common_name": "Common Rhododendron",
      "description": "Rhododendron ponticum is a shrub with attractive flowers. It is popular in gardens for its colorful blooms."
    },
    "pl": {
      "common_name": "Różanecznik pospolity",
      "description": "Różanecznik pospolity to krzew o atrakcyjnych kwiatach. Jest popularny w ogrodach dzięki kolorowym kwiatom."
    },
    "de": {
      "common_name": "Gewöhnlicher Rhododendron",
      "description": "Rhododendron ponticum ist ein Strauch mit attraktiven Blumen. Er ist in Gärten wegen seiner bunten Blüten beliebt."
    },
    "fr": {
      "common_name": "Rhododendron commun",
      "description": "Le Rhododendron ponticum est un arbuste aux fleurs attrayantes. Il est populaire dans les jardins pour ses fleurs colorées."
    },
    "it": {
      "common_name": "Rhododendron comune",
      "description": "Il Rhododendron ponticum è un arbusto con fiori attraenti. È popolare nei giardini per i suoi fiori colorati."
    },
    "es": {
      "common_name": "Rhododendron común",
      "description": "Rhododendron ponticum es un arbusto con flores atractivas. Es popular en los jardines por sus flores coloridas."
    },
    "pt": {
      "common_name": "Rhododendron comum",
      "description": "Rhododendron ponticum é um arbusto com flores atraentes. É popular nos jardins por suas flores coloridas."
    },
    "ar": {
      "common_name": "رودودندرون شائع",
      "description": "رودودندرون بونتيكوم هو شجيرة ذات زهور جذابة. إنه شائع في الحدائق بسبب أزهاره الملونة."
    },
    "hi": {
      "common_name": "कॉमन रोडोडेंड्रॉन",
      "description": "रोडोडेंड्रॉन पॉंटिकम एक झाड़ी है जिसमें आकर्षक फूल होते हैं। यह अपने रंगीन फूलों के लिए बगीचों में लोकप्रिय है।"
    },
    "zh": {
      "common_name": "常见杜鹃",
      "description": "常见杜鹃是一个具有吸引力花朵的灌木。由于其色彩丰富的花朵，它在花园中很受欢迎。"
    },
    "ja": {
      "common_name": "普通のツツジ",
      "description": "Rhododendron ponticum は魅力的な花を持つ低木です。そのカラフルな花が庭で人気があります。"
    },
    "ko": {
      "common_name": "일반 진달래",
      "description": "Rhododendron ponticum은 매력적인 꽃을 가진 관목입니다. 그 화려한 꽃 덕분에 정원에서 인기가 있습니다."
    }
  },
