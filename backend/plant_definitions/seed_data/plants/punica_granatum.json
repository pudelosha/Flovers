  "translations": {
    "en": {
      "common_name": "Pomegranate",
      "description": "Pomegranate is a deciduous shrub or small tree. It produces bright red fruit with edible seeds inside."
    },
    "pl": {
      "common_name": "Granat",
      "description": "Granat to krzew lub małe drzewo liściaste. Wytwarza czerwone owoce z jadalnymi nasionami."
    },
    "de": {
      "common_name": "Granatapfel",
      "description": "Der Granatapfel ist ein laubabwerfender Strauch oder kleiner Baum. Er trägt rote Früchte mit essbaren Samen."
    },
    "fr": {
      "common_name": "Grenade",
      "description": "La grenade est un arbuste ou un petit arbre à feuilles caduques. Il produit des fruits rouges avec des graines comestibles."
    },
    "it": {
      "common_name": "Melograno",
      "description": "Il melograno è un arbusto o piccolo albero a foglie caduche. Produce frutti rossi con semi commestibili."
    },
    "es": {
      "common_name": "Granado",
      "description": "El granado es un arbusto o pequeño árbol de hojas caducas. Produce frutas rojas con semillas comestibles."
    },
    "pt": {
      "common_name": "Romã",
      "description": "A romã é um arbusto ou pequena árvore caducifólia. Produz frutas vermelhas com sementes comestíveis."
    },
    "ar": {
      "common_name": "رمان",
      "description": "الرمان هو شجيرة أو شجرة صغيرة متساقطة الأوراق. ينتج فواكه حمراء تحتوي على بذور صالحة للأكل."
    },
    "hi": {
      "common_name": "अनार",
      "description": "अनार एक पत्ते गिराने वाली झाड़ी या छोटा पेड़ है। यह लाल फल उत्पन्न करता है जिसमें खाने योग्य बीज होते हैं।"
    },
    "zh": {
      "common_name": "石榴",
      "description": "石榴是一种落叶灌木或小树。它会结出红色的果实，里面有可食用的种子。"
    },
    "ja": {
      "common_name": "ザクロ",
      "description": "ザクロは落葉性の低木または小さな木です。食べられる種子を含む赤い果実を生産します。"
    },
    "ko": {
      "common_name": "석류",
      "description": "석류는 낙엽성 관목 또는 작은 나무입니다. 먹을 수 있는 씨앗이 들어 있는 빨간 열매를 생산합니다."
    }
  },
