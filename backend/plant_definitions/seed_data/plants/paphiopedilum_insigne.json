  "translations": {
    "en": {
      "common_name": "Insigne Paphiopedilum",
      "description": "Paphiopedilum insigne is a species of orchid native to India and Southeast Asia. It thrives in medium light and high humidity environments."
    },
    "pl": {
      "common_name": "Paphiopedilum insigne",
      "description": "Paphiopedilum insigne to gatunek storczyka pochodzący z Indii i Azji Południowo-Wschodniej. Rośnie w średnim świetle i wysokiej wilgotności."
    },
    "de": {
      "common_name": "Insigne Paphiopedilum",
      "description": "Paphiopedilum insigne ist eine Orchideenart aus Indien und Südostasien. Es gedeiht bei mittlerem Licht und hoher Luftfeuchtigkeit."
    },
    "fr": {
      "common_name": "Paphiopedilum insigne",
      "description": "Paphiopedilum insigne est une espèce d'orchidée originaire d'Inde et d'Asie du Sud-Est. Il pousse bien en lumière moyenne et en haute humidité."
    },
    "it": {
      "common_name": "Paphiopedilum insigne",
      "description": "Paphiopedilum insigne è una specie di orchidea originaria dell'India e del Sud-Est asiatico. Cresce in ambienti con luce media e umidità elevata."
    },
    "es": {
      "common_name": "Paphiopedilum insigne",
      "description": "Paphiopedilum insigne es una especie de orquídea originaria de la India y el sudeste asiático. Crece bien en luz media y ambientes con alta humedad."
    },
    "pt": {
      "common_name": "Paphiopedilum insigne",
      "description": "Paphiopedilum insigne é uma espécie de orquídea nativa da Índia e do sudeste asiático. Ela prospera em luz média e alta umidade."
    },
    "ar": {
      "common_name": "بابيوفيديلوم إنسين",
      "description": "بابيوفيديلوم إنسين هو نوع من الأوركيد الذي ينمو في الهند وجنوب شرق آسيا. يزدهر في الضوء المتوسط والرطوبة العالية."
    },
    "hi": {
      "common_name": "पैफियोपेडिलम इंसिग्ने",
      "description": "पैफियोपेडिलम इंसिग्ने एक ऑर्किड प्रजाति है जो भारत और दक्षिण-पूर्व एशिया की मूल है। यह मध्यम प्रकाश और उच्च आर्द्रता वाले वातावरण में उगता है।"
    },
    "zh": {
      "common_name": "英俊的石斛兰",
      "description": "英俊的石斛兰是一种原产于印度和东南亚的兰花。它在中等光照和高湿度的环境中茁壮成长。"
    },
    "ja": {
      "common_name": "パフィオペディルム・インシグネ",
      "description": "パフィオペディルム・インシグネは、インドと東南アジア原産のランの一種です。中程度の光と高湿度の環境で育ちます。"
    },
    "ko": {
      "common_name": "파피오페딜럼 인시그네",
      "description": "파피오페딜럼 인시그네는 인도와 동남아시아 원산지의 난초입니다. 중간 정도의 빛과 높은 습도의 환경에서 자랍니다."
    }
  },
