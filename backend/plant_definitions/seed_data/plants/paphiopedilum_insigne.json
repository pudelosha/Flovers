  "translations": {
    "en": {
      "common_name": "Slipper Orchid",
      "description": "Paphiopedilum insigne is a species of orchid native to the eastern Himalayas. Known for its striking, slipper-shaped flowers, it thrives in shaded, humid environments."
    },
    "pl": {
      "common_name": "Storczyk pantoflowy",
      "description": "Paphiopedilum insigne to gatunek storczyka pochodzący z wschodnich Himalajów. Jest znany z wyjątkowych, pantoflowych kwiatów, rośnie w zacienionych, wilgotnych miejscach."
    },
    "de": {
      "common_name": "Schuhorchidee",
      "description": "Paphiopedilum insigne ist eine Orchideenart aus den östlichen Himalaya. Sie ist bekannt für ihre auffälligen, schuhförmigen Blüten und gedeiht in schattigen, feuchten Umgebungen."
    },
    "fr": {
      "common_name": "Orchidée sabot",
      "description": "Paphiopedilum insigne est une espèce d'orchidée des Himalayas orientaux. Elle est connue pour ses fleurs en forme de sabot et pousse dans des environnements ombragés et humides."
    },
    "it": {
      "common_name": "Orchidea scarpetta",
      "description": "Paphiopedilum insigne è una specie di orchidea proveniente dall'Himalaya orientale. È conosciuta per i suoi fiori a forma di scarpetta e cresce in ambienti ombrosi e umidi."
    },
    "es": {
      "common_name": "Orquídea zapato",
      "description": "Paphiopedilum insigne es una especie de orquídea originaria de los Himalayas orientales. Es conocida por sus flores en forma de zapato y crece en entornos sombreados y húmedos."
    },
    "pt": {
      "common_name": "Orquídea sapato",
      "description": "Paphiopedilum insigne é uma espécie de orquídea dos Himalaias orientais. Conhecida por suas flores em forma de sapato, cresce em ambientes sombreados e úmidos."
    },
    "ar": {
      "common_name": "زهرة السابوت",
      "description": "Paphiopedilum insigne هو نوع من الأوركيد موطنه هيمالايا الشرقية. يشتهر بزهوره المميزة على شكل حذاء وينمو في بيئات مظللة ورطبة."
    },
    "hi": {
      "common_name": "चप्पल ऑर्किड",
      "description": "Paphiopedilum insigne एक प्रकार का ऑर्किड है जो पूर्वी हिमालय का मूल निवासी है। यह अपनी विशिष्ट चप्पल के आकार के फूलों के लिए जाना जाता है और इसे छायादार, नम वातावरण में उगाया जाता है।"
    },
    "zh": {
      "common_name": "拖鞋兰",
      "description": "Paphiopedilum insigne 是一种原产于东喜马拉雅山的兰花。以其独特的拖鞋形状花朵而闻名，喜欢生长在阴湿的环境中。"
    },
    "ja": {
      "common_name": "スリッパラン",
      "description": "Paphiopedilum insigneは東ヒマラヤ原産のランの一種です。特徴的なスリッパ形の花を持ち、陰湿な環境で育ちます。"
    },
    "ko": {
      "common_name": "슬리퍼 난초",
      "description": "Paphiopedilum insigne은 동부 히말라야 원산의 난초입니다. 특유의 슬리퍼 모양 꽃으로 유명하며 그늘지고 습한 환경에서 자랍니다."
    }
  },
