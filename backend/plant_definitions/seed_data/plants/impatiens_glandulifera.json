  "translations": {
    "en": {
      "common_name": "Himalayan balsam",
      "description": "Impatiens glandulifera is a tall annual native to the western Himalayas. It grows along rivers and damp ground. The plant is invasive in many parts of Europe."
    },
    "pl": {
      "common_name": "Niecierpek gruczołowaty",
      "description": "Impatiens glandulifera to wysoka roślina jednoroczna z Himalajów. Rośnie nad rzekami i na wilgotnych terenach. W Europie jest gatunkiem inwazyjnym."
    },
    "de": {
      "common_name": "Drüsiges Springkraut",
      "description": "Impatiens glandulifera ist eine einjährige Pflanze aus dem Himalaya. Sie wächst an feuchten Standorten und Flussufern. In Europa gilt sie als invasiv."
    },
    "fr": {
      "common_name": "Balsamine de l’Himalaya",
      "description": "Impatiens glandulifera est une annuelle originaire de l’Himalaya. Elle pousse dans les sols humides et le long des cours d’eau. Elle est invasive en Europe."
    },
    "it": {
      "common_name": "Balsamina dell’Himalaya",
      "description": "Impatiens glandulifera è una pianta annuale dell’Himalaya. Cresce in ambienti umidi e lungo i fiumi. In molte regioni europee è invasiva."
    },
    "es": {
      "common_name": "Bálsamo del Himalaya",
      "description": "Impatiens glandulifera es una planta anual del Himalaya. Prefiere suelos húmedos y riberas. En Europa se considera invasora."
    },
    "pt": {
      "common_name": "Bálsamo-do-Himalaia",
      "description": "Impatiens glandulifera é uma planta anual do Himalaia. Desenvolve-se em solos húmidos e margens de rios. É invasora na Europa."
    },
    "ar": {
      "common_name": "بلسم الهيمالايا",
      "description": "نبات Impatiens glandulifera حولي موطنه الهيمالايا. ينمو في المناطق الرطبة وعلى ضفاف الأنهار. يعد نباتًا غازيًا في أوروبا."
    },
    "hi": {
      "common_name": "हिमालयन बालसम",
      "description": "Impatiens glandulifera हिमालय की वार्षिक वनस्पति है। यह नमी वाली भूमि और नदियों के किनारे उगती है। यूरोप में यह आक्रामक मानी जाती है।"
    },
    "zh": {
      "common_name": "喜马拉雅凤仙花",
      "description": "Impatiens glandulifera 原产喜马拉雅山。常生于湿地和河岸。该植物在欧洲具有入侵性。"
    },
    "ja": {
      "common_name": "ヒマラヤツリフネソウ",
      "description": "Impatiens glandulifera はヒマラヤ原産の一年草です。湿った土地や川沿いに生育します。ヨーロッパでは外来種です。"
    },
    "ko": {
      "common_name": "히말라야 봉선화",
      "description": "Impatiens glandulifera는 히말라야 원산의 한해살이풀입니다. 습한 토양과 강가에서 자랍니다. 유럽에서는 침입종입니다."
    }
  },
