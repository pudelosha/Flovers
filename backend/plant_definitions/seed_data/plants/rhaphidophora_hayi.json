  "translations": {
    "en": {
      "common_name": "Hayi Philodendron",
      "description": "Rhaphidophora hayi is a tropical plant with attractive heart-shaped leaves. It's commonly grown indoors as a decorative houseplant."
    },
    "pl": {
      "common_name": "Filodendron Hayi",
      "description": "Rhaphidophora hayi to roślina tropikalna o atrakcyjnych, sercowatych liściach. Jest popularnie uprawiana w pomieszczeniach jako roślina ozdobna."
    },
    "de": {
      "common_name": "Hayi-Philodendron",
      "description": "Rhaphidophora hayi ist eine tropische Pflanze mit attraktiven herzförmigen Blättern. Sie wird häufig als Zimmerpflanze angebaut."
    },
    "fr": {
      "common_name": "Philodendron Hayi",
      "description": "Rhaphidophora hayi est une plante tropicale avec de belles feuilles en forme de cœur. Elle est couramment cultivée comme plante d'intérieur décorative."
    },
    "it": {
      "common_name": "Philodendron Hayi",
      "description": "Rhaphidophora hayi è una pianta tropicale con foglie a forma di cuore. È comunemente coltivata come pianta da interno decorativa."
    },
    "es": {
      "common_name": "Filodendro Hayi",
      "description": "Rhaphidophora hayi es una planta tropical con atractivas hojas en forma de corazón. Comúnmente se cultiva en interiores como planta decorativa."
    },
    "pt": {
      "common_name": "Filodendro Hayi",
      "description": "Rhaphidophora hayi é uma planta tropical com folhas atraentes em forma de coração. Geralmente é cultivada como planta decorativa em ambientes internos."
    },
    "ar": {
      "common_name": "فيلوديندرون هايي",
      "description": "Rhaphidophora hayi هي نبات استوائي ذو أوراق جذابة على شكل قلب. غالبًا ما يتم زراعتها كنبات زخرفي داخلي."
    },
    "hi": {
      "common_name": "फिलोडेंड्रोन हायि",
      "description": "Rhaphidophora hayi एक उष्णकटिबंधीय पौधा है जिसकी आकर्षक दिल के आकार की पत्तियाँ होती हैं। इसे आमतौर पर एक सजावटी पौधे के रूप में इनडोर उगाया जाता है।"
    },
    "zh": {
      "common_name": "海伊蔓绿绒",
      "description": "Rhaphidophora hayi 是一种热带植物，具有迷人的心形叶子。它通常作为室内装饰植物种植。"
    },
    "ja": {
      "common_name": "ハイイフィロデンドロン",
      "description": "Rhaphidophora hayi は魅力的なハート型の葉を持つ熱帯植物です。一般的に室内の装飾植物として栽培されています。"
    },
    "ko": {
      "common_name": "하이이 필로덴드론",
      "description": "Rhaphidophora hayi는 매력적인 심장 모양의 잎을 가진 열대 식물입니다. 일반적으로 실내 장식 식물로 재배됩니다."
    }
  },
