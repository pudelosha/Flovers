  "translations": {
    "en": {
      "common_name": "Fiddle-leaf fig",
      "description": "Ficus lyrata is a tropical tree native to West Africa. It is valued for its large, violin-shaped leaves and is commonly grown indoors in bright light."
    },
    "pl": {
      "common_name": "Figowiec dębolistny",
      "description": "Ficus lyrata pochodzi z Afryki Zachodniej. Roślina jest ceniona za duże, dekoracyjne liście i uprawiana jako roślina domowa."
    },
    "de": {
      "common_name": "Geigenfeige",
      "description": "Ficus lyrata stammt aus Westafrika. Er wird wegen seiner großen, geigenförmigen Blätter als Zimmerpflanze geschätzt."
    },
    "fr": {
      "common_name": "Figuier lyre",
      "description": "Ficus lyrata est originaire d’Afrique de l’Ouest. Il est apprécié pour ses grandes feuilles et cultivé en intérieur lumineux."
    },
    "it": {
      "common_name": "Fico a foglia di violino",
      "description": "Ficus lyrata è originario dell’Africa occidentale. È coltivato in casa per le sue grandi foglie ornamentali."
    },
    "es": {
      "common_name": "Higuera de hoja de violín",
      "description": "Ficus lyrata es nativo de África occidental. Se cultiva en interiores por sus grandes hojas decorativas."
    },
    "pt": {
      "common_name": "Figueira-lira",
      "description": "Ficus lyrata é nativo da África Ocidental. É cultivado em interiores pelas suas folhas grandes e ornamentais."
    },
    "ar": {
      "common_name": "فيكس ليراتا",
      "description": "نبات فيكس ليراتا موطنه أفريقيا الغربية. يزرع كنبات داخلي لأوراقه الكبيرة الزخرفية."
    },
    "hi": {
      "common_name": "फिडल लीफ फिग",
      "description": "फिकस लिराटा पश्चिमी अफ्रीका का पौधा है। इसे बड़े सजावटी पत्तों के कारण घर के अंदर उगाया जाता है।"
    },
    "zh": {
      "common_name": "琴叶榕",
      "description": "琴叶榕原产于西非。因其大型观赏叶片而常作为室内植物栽培。"
    },
    "ja": {
      "common_name": "フィカス・リラータ",
      "description": "フィカス・リラータは西アフリカ原産です。大きな葉を持つ観葉植物として室内で育てられます。"
    },
    "ko": {
      "common_name": "떡갈잎고무나무",
      "description": "피쿠스 리라타는 서아프리카 원산입니다. 큰 잎이 특징이며 실내 관엽식물로 재배됩니다."
    }
  },
