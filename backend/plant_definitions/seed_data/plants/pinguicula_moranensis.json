  "translations": {
    "en": {
      "common_name": "Moran's Butterwort",
      "description": "Pinguicula moranensis is a carnivorous plant native to Mexico. It has vibrant green rosettes and striking magenta flowers."
    },
    "pl": {
      "common_name": "Woskowiec Moran",
      "description": "Pinguicula moranensis to roślina mięsożerna pochodząca z Meksyku. Ma intensywnie zielone rozetki i wyraziste, purpurowe kwiaty."
    },
    "de": {
      "common_name": "Morans Butterblume",
      "description": "Pinguicula moranensis ist eine fleischfressende Pflanze aus Mexiko. Sie hat leuchtend grüne Rosetten und auffällige magentafarbene Blumen."
    },
    "fr": {
      "common_name": "Pinguicula de Moran",
      "description": "Pinguicula moranensis est une plante carnivore originaire du Mexique. Elle possède des rosettes vertes vives et des fleurs magenta frappantes."
    },
    "it": {
      "common_name": "Pinguicula di Moran",
      "description": "Pinguicula moranensis è una pianta carnivora originaria del Messico. Ha rosette verdi vivaci e fiori magenta sorprendenti."
    },
    "es": {
      "common_name": "Pinguicula de Moran",
      "description": "Pinguicula moranensis es una planta carnívora originaria de México. Tiene rosetas verdes brillantes y flores magenta llamativas."
    },
    "pt": {
      "common_name": "Pinguicula de Moran",
      "description": "Pinguicula moranensis é uma planta carnívora nativa do México. Possui rosetas verdes vibrantes e flores magenta impressionantes."
    },
    "ar": {
      "common_name": "Pinguicula موران",
      "description": "Pinguicula moranensis هي نبات آكل اللحوم موطنه المكسيك. لها وردات خضراء زاهية وأزهار مغنتا لافتة."
    },
    "hi": {
      "common_name": "पिंग्विकुला मोरानेंसिस",
      "description": "पिंग्विकुला मोरानेंसिस एक मांसाहारी पौधा है जो मेक्सिको का मूल निवासी है। इसमें हरे रंग की रोसेट और चमकीले मैजेंटा रंग के फूल होते हैं।"
    },
    "zh": {
      "common_name": "莫兰毛虫草",
      "description": "Pinguicula moranensis 是一种原产于墨西哥的食虫植物。它具有鲜艳的绿色莲座和引人注目的品红色花朵。"
    },
    "ja": {
      "common_name": "モランのウツボカズラ",
      "description": "Pinguicula moranensis はメキシコ原産の食虫植物です。鮮やかな緑色のロゼットと目を引くマゼンタ色の花を持っています。"
    },
    "ko": {
      "common_name": "모란 벌레잡이풀",
      "description": "Pinguicula moranensis는 멕시코 원산의 식충 식물입니다. 그것은 선명한 녹색의 로제트와 눈에 띄는 자홍색 꽃을 가지고 있습니다."
    }
  },
