  "translations": {
    "en": {
      "common_name": "Curry Plant",
      "description": "Helichrysum italicum is a fragrant herb with silvery foliage. It thrives in full sun and requires moderate watering."
    },
    "pl": {
      "common_name": "Roślina curry",
      "description": "Helichrysum italicum to pachnąca roślina z srebrzystymi liśćmi. Rośnie w pełnym słońcu i wymaga umiarkowanego podlewania."
    },
    "de": {
      "common_name": "Curry-Pflanze",
      "description": "Helichrysum italicum ist ein duftendes Kraut mit silbrigen Blättern. Es gedeiht in voller Sonne und benötigt mäßige Bewässerung."
    },
    "fr": {
      "common_name": "Plante curry",
      "description": "Helichrysum italicum est une herbe parfumée avec un feuillage argenté. Elle prospère en plein soleil et nécessite un arrosage modéré."
    },
    "it": {
      "common_name": "Pianta curry",
      "description": "Helichrysum italicum è un'erba profumata con foglie argentate. Cresce al pieno sole e richiede un'annaffiatura moderata."
    },
    "es": {
      "common_name": "Planta curry",
      "description": "Helichrysum italicum es una hierba fragante con follaje plateado. Crece a pleno sol y necesita riego moderado."
    },
    "pt": {
      "common_name": "Planta de curry",
      "description": "Helichrysum italicum é uma erva perfumada com folhagem prateada. Cresce em pleno sol e requer rega moderada."
    },
    "ar": {
      "common_name": "نبات الكاري",
      "description": "Helichrysum italicum هو نبات عطري ذو أوراق فضية. ينمو في الشمس الكاملة ويتطلب الري المعتدل."
    },
    "hi": {
      "common_name": "करी पौधा",
      "description": "Helichrysum italicum एक सुगंधित हर्ब है जिसमें चांदी जैसा पत्तियां होती हैं। यह पूर्ण सूर्य में बढ़ता है और इसे मध्यम पानी की आवश्यकता होती है."
    },
    "zh": {
      "common_name": "咖喱植物",
      "description": "Helichrysum italicum 是一种具有银色叶片的芳香草本植物。它在阳光充足的地方生长，需要适度的浇水。"
    },
    "ja": {
      "common_name": "カレー植物",
      "description": "Helichrysum italicum は銀色の葉を持つ香り高いハーブです。完全な日光の下で育ち、適度な水やりが必要です。"
    },
    "ko": {
      "common_name": "커리 식물",
      "description": "Helichrysum italicum은 은색 잎을 가진 향기로운 허브입니다. 완전한 햇빛에서 자라며 적당한 물주기가 필요합니다."
    }
  },
