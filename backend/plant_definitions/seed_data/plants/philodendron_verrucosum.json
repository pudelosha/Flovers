  "translations": {
    "en": {
      "common_name": "Creeping Philodendron",
      "description": "Philodendron verrucosum is a tropical vine with velvety, heart-shaped leaves. Known for its striking veins, it thrives in humid conditions."
    },
    "pl": {
      "common_name": "Filodendron brodawkowaty",
      "description": "Filodendron verrucosum to tropikalna roślina pnąca o aksamitnych, sercowatych liściach. Charakteryzuje się wyraźnymi żyłkami i wymaga wilgotnych warunków."
    },
    "de": {
      "common_name": "Warziger Philodendron",
      "description": "Philodendron verrucosum ist eine tropische Kletterpflanze mit samtigen, herzförmigen Blättern. Es liebt feuchte Bedingungen und hat markante Adern."
    },
    "fr": {
      "common_name": "Philodendron verruqueux",
      "description": "Le Philodendron verrucosum est une plante grimpante tropicale aux feuilles en forme de cœur, veloutées et aux nervures frappantes."
    },
    "it": {
      "common_name": "Philodendron verrucoso",
      "description": "Il Philodendron verrucosum è una pianta rampicante tropicale con foglie vellutate a forma di cuore, caratterizzate da venature evidenti."
    },
    "es": {
      "common_name": "Philodendron verrucoso",
      "description": "El Philodendron verrucosum es una planta trepadora tropical con hojas aterciopeladas en forma de corazón y venas prominentes."
    },
    "pt": {
      "common_name": "Philodendron verrucoso",
      "description": "Philodendron verrucosum é uma planta trepadeira tropical com folhas aveludadas em forma de coração e veias proeminentes."
    },
    "ar": {
      "common_name": "فلودندرون فوروكوسوم",
      "description": "الفلودندرون فوروكوسوم هو نبات تسلق استوائي بأوراق مخملية على شكل قلب وعروق بارزة."
    },
    "hi": {
      "common_name": "फिलोडेंड्रोन वेरीकोसुम",
      "description": "फिलोडेंड्रोन वेरीकोसुम एक उष्णकटिबंधीय लताओं वाली पौधा है, जिसके मखमली, दिल के आकार के पत्ते होते हैं और इसकी नसें स्पष्ट होती हैं।"
    },
    "zh": {
      "common_name": "毛茸毛爬山虎",
      "description": "Philodendron verrucosum 是一种热带藤本植物，具有天鹅绒般的心形叶子，显著的脉络。"
    },
    "ja": {
      "common_name": "フィロデンドロン・ヴェルルコスム",
      "description": "フィロデンドロン・ヴェルルコスムは、ベルベットのような心形の葉と特徴的な静脈を持つ熱帯性のつる植物です。"
    },
    "ko": {
      "common_name": "필로덴드론 베루코숨",
      "description": "필로덴드론 베루코숨은 벨벳 같은 심장 모양의 잎을 가진 열대성 덩굴 식물로 뚜렷한 맥이 있습니다."
    }
  },
