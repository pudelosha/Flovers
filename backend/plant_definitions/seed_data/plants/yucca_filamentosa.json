  "translations": {
    "en": {
      "common_name": "Adam's needle",
      "description": "Yucca filamentosa is a drought-tolerant evergreen shrub. It prefers full sun and well-drained soil."
    },
    "pl": {
      "common_name": "Igła Adama",
      "description": "Yucca filamentosa jest sucholubnym zimozielonym krzewem. Woli pełne słońce i przepuszczalną glebę."
    },
    "de": {
      "common_name": "Adamsspieß",
      "description": "Yucca filamentosa ist ein dürretoleranter immergrüner Strauch. Sie bevorzugt volle Sonne und gut durchlässigen Boden."
    },
    "fr": {
      "common_name": "Yucca d’Adam",
      "description": "Yucca filamentosa est un arbuste persistant tolérant à la sécheresse. Il préfère le plein soleil et un sol drainé."
    },
    "it": {
      "common_name": "Ago di Adam",
      "description": "Yucca filamentosa è un arbusto sempreverde tollerante alla siccità. Preferisce pieno sole e terreno ben drenato."
    },
    "es": {
      "common_name": "Aguja de Adam",
      "description": "Yucca filamentosa es un arbusto perenne tolerante a la sequía. Prefiere sol pleno y suelo bien drenado."
    },
    "pt": {
      "common_name": "Agulha de Adam",
      "description": "Yucca filamentosa é um arbusto perene tolerante à seca. Prefere sol pleno e solo bem drenado."
    },
    "ar": {
      "common_name": "إبرة آدم",
      "description": "Yucca filamentosa شجيرة دائمة تتحمل الجفاف. تفضل الشمس الكاملة والتربة جيدة التصريف."
    },
    "hi": {
      "common_name": "एडम की सुई",
      "description": "Yucca filamentosa drought सहनशील सदाबहार झाड़ी है। यह पूर्ण धूप और अच्छी जलनिकासी वाली मिट्टी पसंद करती है।"
    },
    "zh": {
      "common_name": "亚当针",
      "description": "Yucca filamentosa 是耐旱常绿灌木。 它喜欢全日照和良好排水的土壤。"
    },
    "ja": {
      "common_name": "アダムの針",
      "description": "Yucca filamentosa は耐乾性常緑低木です。 直射日光と排水良好な土壌を好みます。"
    },
    "ko": {
      "common_name": "아담의 바늘",
      "description": "Yucca filamentosa 는 내건성 항상녹색 관목입니다. 햇빛과 배수 좋은 토양을 선호합니다."
    }
  },
