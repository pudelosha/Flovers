  "translations": {
    "en": {
      "common_name": "Purple Pitcher Plant",
      "description": "Sarracenia purpurea is a carnivorous plant native to North America. It thrives in acidic, wetland soils and catches prey in its tubular leaves."
    },
    "pl": {
      "common_name": "Różowa roślina dzbaneczkowa",
      "description": "Sarracenia purpurea to roślina mięsożerna pochodząca z Ameryki Północnej. Rośnie w kwaśnych, wilgotnych glebach i łapie ofiary w swoich rurkowatych liściach."
    },
    "de": {
      "common_name": "Purpurroter Schlauchpflanze",
      "description": "Sarracenia purpurea ist eine fleischfressende Pflanze, die in Nordamerika heimisch ist. Sie gedeiht in sauren, feuchten Böden und fängt Beute in ihren röhrenförmigen Blättern."
    },
    "fr": {
      "common_name": "Plante à urne pourpre",
      "description": "Sarracenia purpurea est une plante carnivore originaire d'Amérique du Nord. Elle prospère dans des sols acides et capturent des proies avec ses feuilles tubulaires."
    },
    "it": {
      "common_name": "Pianta a brocca viola",
      "description": "Sarracenia purpurea è una pianta carnivora originaria del Nord America. Cresce in suoli acidi e cattura prede con le sue foglie tubolari."
    },
    "es": {
      "common_name": "Planta jarra púrpura",
      "description": "Sarracenia purpurea es una planta carnívora originaria de América del Norte. Crece en suelos ácidos y captura presas con sus hojas tubulares."
    },
    "pt": {
      "common_name": "Planta jarra roxa",
      "description": "Sarracenia purpurea é uma planta carnívora nativa da América do Norte. Cresce em solos ácidos e captura presas com suas folhas tubulares."
    },
    "ar": {
      "common_name": "نبات الجرة الأرجوانية",
      "description": "Sarracenia purpurea هي نبات آكل لحوم النباتات الأصلي في أمريكا الشمالية. تنمو في التربة الحمضية وتلتقط الفرائس باستخدام أوراقها الأنبوبيّة."
    },
    "hi": {
      "common_name": "बैंगनी पात्र पौधा",
      "description": "Sarracenia purpurea एक मांसाहारी पौधा है जो उत्तरी अमेरिका का मूल निवासी है। यह अम्लीय मिट्टी में बढ़ता है और अपनी ट्यूब जैसी पत्तियों में शिकार करता है।"
    },
    "zh": {
      "common_name": "紫色瓶子植物",
      "description": "Sarracenia purpurea 是一种原产于北美的食肉植物。它生长在酸性土壤中，并通过其管状叶子捕捉猎物。"
    },
    "ja": {
      "common_name": "紫のピッチャープラント",
      "description": "Sarracenia purpureaは北アメリカ原産の肉食植物です。酸性土壌で育ち、管状の葉で獲物を捕らえます。"
    },
    "ko": {
      "common_name": "자주색 피처 식물",
      "description": "Sarracenia purpurea는 북미 원산의 육식 식물입니다. 산성 토양에서 자라며 튜브 모양의 잎으로 먹이를 잡습니다."
    }
  },
