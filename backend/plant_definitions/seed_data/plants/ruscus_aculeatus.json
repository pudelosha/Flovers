  "translations": {
    "en": {
      "common_name": "Butcher's Broom",
      "description": "Ruscus aculeatus is an evergreen shrub native to Europe. It is often grown as an ornamental plant."
    },
    "pl": {
      "common_name": "Ruskus ostrokończysty",
      "description": "Ruscus aculeatus to zimozielony krzew rodem z Europy. Często uprawiany jako roślina ozdobna."
    },
    "de": {
      "common_name": "Pfeilspitzenbusch",
      "description": "Ruscus aculeatus ist ein immergrüner Strauch aus Europa. Er wird oft als Zierpflanze angebaut."
    },
    "fr": {
      "common_name": "Ruscus épineux",
      "description": "Ruscus aculeatus est un arbuste à feuillage persistant originaire d'Europe. Il est souvent cultivé comme plante ornementale."
    },
    "it": {
      "common_name": "Ruscus aculeatus",
      "description": "Ruscus aculeatus è un arbusto sempreverde originario dell'Europa. Viene spesso coltivato come pianta ornamentale."
    },
    "es": {
      "common_name": "Ruscus aculeatus",
      "description": "Ruscus aculeatus es un arbusto perenne originario de Europa. Se cultiva a menudo como planta ornamental."
    },
    "pt": {
      "common_name": "Ruscus aculeatus",
      "description": "Ruscus aculeatus é um arbusto perene nativo da Europa. Frequentemente cultivado como planta ornamental."
    },
    "ar": {
      "common_name": "روسكس إيكولياتوس",
      "description": "Ruscus aculeatus هو شجيرة دائمة الخضرة موطنها أوروبا. يتم زراعته غالبًا كنبات زينة."
    },
    "hi": {
      "common_name": "रुस्कस अक्यूलियाटस",
      "description": "Ruscus aculeatus एक सदाबहार झाड़ी है जो यूरोप का मूल निवास है। इसे अक्सर सजावटी पौधे के रूप में उगाया जाता है।"
    },
    "zh": {
      "common_name": "刺果蓝花楹",
      "description": "Ruscus aculeatus 是一种常绿灌木，原产于欧洲。它通常作为观赏植物种植。"
    },
    "ja": {
      "common_name": "ルスカス・アクレアタス",
      "description": "Ruscus aculeatus はヨーロッパ原産の常緑灌木です。観賞用植物としてよく栽培されます。"
    },
    "ko": {
      "common_name": "부처의 빗자루",
      "description": "Ruscus aculeatus는 유럽 원산의 상록 관목입니다. 종종 장식용 식물로 재배됩니다."
    }
  },
