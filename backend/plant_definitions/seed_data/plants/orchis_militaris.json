  "translations": {
    "en": {
      "common_name": "Military Orchid",
      "description": "Orchis militaris is a terrestrial orchid native to Europe. It produces distinctive flowers in late spring to early summer."
    },
    "pl": {
      "common_name": "Storczyk wojskowy",
      "description": "Orchis militaris to storczyk ziemny, który występuje w Europie. Kwitnie wczesnym latem, tworząc charakterystyczne kwiaty."
    },
    "de": {
      "common_name": "Militär-Orchidee",
      "description": "Orchis militaris ist eine terrestrische Orchidee aus Europa, die im späten Frühling bis frühen Sommer auffällige Blüten bildet."
    },
    "fr": {
      "common_name": "Orchidée militaire",
      "description": "Orchis militaris est une orchidée terrestre originaire d'Europe. Elle produit des fleurs distinctives du printemps à l'été."
    },
    "it": {
      "common_name": "Orchidea militare",
      "description": "Orchis militaris è un'orchidea terrestre originaria dell'Europa. Produce fiori distintivi tra la fine della primavera e l'inizio dell'estate."
    },
    "es": {
      "common_name": "Orquídea militar",
      "description": "Orchis militaris es una orquídea terrestre originaria de Europa. Produce flores distintivas a finales de primavera y principios de verano."
    },
    "pt": {
      "common_name": "Orquídea militar",
      "description": "Orchis militaris é uma orquídea terrestre nativa da Europa. Produz flores distintas no final da primavera e no início do verão."
    },
    "ar": {
      "common_name": "زهرة الأوركيد العسكرية",
      "description": "Orchis militaris هي زهرة أوركيدية أرضية موطنها أوروبا. تنتج زهورًا مميزة في أواخر الربيع إلى أوائل الصيف."
    },
    "hi": {
      "common_name": "सैन्य ऑर्किड",
      "description": "Orchis militaris एक स्थलीय ऑर्किड है जो यूरोप का मूल निवासी है। यह वसंत के अंत से लेकर ग्रीष्मकाल के प्रारंभ तक विशिष्ट फूल पैदा करता है।"
    },
    "zh": {
      "common_name": "军事兰花",
      "description": "Orchis militaris 是一种原产于欧洲的地生兰花。它在春末到夏初开出独特的花朵。"
    },
    "ja": {
      "common_name": "ミリタリー・オーキッド",
      "description": "Orchis militaris はヨーロッパ原産の地生ランです。春の終わりから初夏にかけて特徴的な花を咲かせます。"
    },
    "ko": {
      "common_name": "군사 난초",
      "description": "Orchis militaris는 유럽 원산의 지상 난초입니다. 봄 끝에서 여름 초기에 독특한 꽃을 만듭니다."
    }
  },
