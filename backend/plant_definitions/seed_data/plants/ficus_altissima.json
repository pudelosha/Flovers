  "translations": {
    "en": {
      "common_name": "Council Tree",
      "description": "Ficus altissima is a tropical fig tree native to Southeast Asia. It is commonly grown indoors for its large, glossy leaves and tolerance of indoor conditions."
    },
    "pl": {
      "common_name": "Figowiec wyniosły",
      "description": "Ficus altissima to tropikalne drzewo figowe z Azji Południowo-Wschodniej. Uprawiany w domu ze względu na dekoracyjne liście i łatwą pielęgnację."
    },
    "de": {
      "common_name": "Riesenfeige",
      "description": "Ficus altissima ist ein tropischer Feigenbaum aus Südostasien. Er wird als Zimmerpflanze wegen seiner großen Blätter geschätzt."
    },
    "fr": {
      "common_name": "Figuier géant",
      "description": "Ficus altissima est un figuier tropical originaire d’Asie du Sud-Est. Il est cultivé en intérieur pour son feuillage décoratif."
    },
    "it": {
      "common_name": "Fico gigante",
      "description": "Ficus altissima è un albero tropicale originario del Sud-Est asiatico. È apprezzato come pianta da interno per le sue grandi foglie."
    },
    "es": {
      "common_name": "Higuera gigante",
      "description": "Ficus altissima es una higuera tropical del sudeste asiático. Se cultiva en interiores por sus hojas grandes y brillantes."
    },
    "pt": {
      "common_name": "Figueira gigante",
      "description": "Ficus altissima é uma figueira tropical do Sudeste Asiático. É cultivada em interiores pelo seu valor ornamental."
    },
    "ar": {
      "common_name": "فيكس عملاق",
      "description": "فيكس ألتيسيما شجرة تين استوائية موطنها جنوب شرق آسيا. تُزرع كنبات داخلي لأوراقها الكبيرة."
    },
    "hi": {
      "common_name": "विशाल फाइकस",
      "description": "फिकस अल्टिसिमा दक्षिण पूर्व एशिया का उष्णकटिबंधीय पेड़ है। इसे बड़े पत्तों के लिए घर में उगाया जाता है।"
    },
    "zh": {
      "common_name": "高榕",
      "description": "高榕原产于东南亚的热带地区。因其大型光亮叶片而作为室内观叶植物栽培。"
    },
    "ja": {
      "common_name": "アルティッシマゴムノキ",
      "description": "フィカス・アルティッシマは東南アジア原産の熱帯性イチジクです。観葉植物として人気があります。"
    },
    "ko": {
      "common_name": "알티시마 고무나무",
      "description": "피쿠스 알티시마는 동남아시아 원산의 열대 무화과나무입니다. 실내 관엽식물로 재배됩니다."
    }
