  "translations": {
    "en": {
      "common_name": "Arrowhead Vine",
      "description": "Syngonium wendlandii is a beautiful tropical vine. It is known for its lush, heart-shaped leaves with striking white veins."
    },
    "pl": {
      "common_name": "Winorośl strzałkowa",
      "description": "Syngonium wendlandii to piękna tropikalna winorośl. Znana jest z bujnych, sercowatych liści z wyrazistymi białymi żyłkami."
    },
    "de": {
      "common_name": "Pfeilblattrebe",
      "description": "Syngonium wendlandii ist eine wunderschöne tropische Kletterpflanze. Sie ist bekannt für ihre üppigen, herzförmigen Blätter mit markanten weißen Adern."
    },
    "fr": {
      "common_name": "Vigne à flèche",
      "description": "Syngonium wendlandii est une belle vigne tropicale. Elle est connue pour ses feuilles en forme de cœur, avec de belles nervures blanches."
    },
    "it": {
      "common_name": "Vite a freccia",
      "description": "Syngonium wendlandii è una bellissima vite tropicale. È nota per le sue lussureggianti foglie a forma di cuore con venature bianche."
    },
    "es": {
      "common_name": "Vid de flecha",
      "description": "Syngonium wendlandii es una hermosa vid tropical. Es conocida por sus hojas en forma de corazón con venas blancas marcadas."
    },
    "pt": {
      "common_name": "Videira de flecha",
      "description": "Syngonium wendlandii é uma bela videira tropical. É conhecida por suas folhas em forma de coração com veios brancos marcantes."
    },
    "ar": {
      "common_name": "عنقود السهم",
      "description": "Syngonium wendlandii هي كرمة استوائية جميلة. معروفة بأوراقها على شكل قلب مع عروق بيضاء مميزة."
    },
    "hi": {
      "common_name": "तीर बेल",
      "description": "Syngonium wendlandii एक सुंदर उष्णकटिबंधीय बेल है। यह अपनी हृदय के आकार की पत्तियों और सफेद नसों के लिए जानी जाती है।"
    },
    "zh": {
      "common_name": "箭头藤",
      "description": "Syngonium wendlandii 是一种美丽的热带藤本植物。它以其心形的叶子和显著的白色脉络而闻名。"
    },
    "ja": {
      "common_name": "矢印のつる植物",
      "description": "Syngonium wendlandii は美しい熱帯性のツル植物です。ハート型の葉と特徴的な白い脈が特徴です。"
    },
    "ko": {
      "common_name": "화살표 덩굴",
      "description": "Syngonium wendlandii는 아름다운 열대 덩굴 식물입니다. 그것은 하트 모양의 잎과 눈에 띄는 흰색 정맥으로 유명합니다."
    }
  },
