  "translations": {
    "en": {
      "common_name": "Starfish Flower",
      "description": "Stapelia variegata is a succulent native to Southern Africa. Known for its star-shaped flowers with striking patterns."
    },
    "pl": {
      "common_name": "Kwiat rozgwiazdy",
      "description": "Stapelia variegata to sukulent pochodzący z Afryki Południowej. Znany z gwiaździstych kwiatów o wyrazistych wzorach."
    },
    "de": {
      "common_name": "Sternblumen",
      "description": "Stapelia variegata ist eine Sukkulente aus Südafrika. Bekannt für ihre sternförmigen Blumen mit markanten Mustern."
    },
    "fr": {
      "common_name": "Fleur étoilée",
      "description": "Stapelia variegata est une succulente originaire d'Afrique du Sud. Connue pour ses fleurs en forme d'étoile avec des motifs frappants."
    },
    "it": {
      "common_name": "Fiore a stella",
      "description": "Stapelia variegata è una succulenta originaria del Sudafrica. Conosciuta per i suoi fiori a forma di stella con motivi distintivi."
    },
    "es": {
      "common_name": "Flor Estrella",
      "description": "Stapelia variegata es una suculenta originaria del sur de África. Conocida por sus flores en forma de estrella con patrones distintivos."
    },
    "pt": {
      "common_name": "Flor Estrela",
      "description": "Stapelia variegata é uma suculenta nativa do sul da África. Conhecida por suas flores em forma de estrela com padrões marcantes."
    },
    "ar": {
      "common_name": "زهرة النجمة",
      "description": "ستابيليا فاريويجاتا هي نبتة عصارية موطنها جنوب أفريقيا. معروفة بأزهارها على شكل نجمة مع أنماط لافتة."
    },
    "hi": {
      "common_name": "तारा फूल",
      "description": "स्टेपेलिया वेरिगाटा एक रसीला पौधा है जो दक्षिणी अफ्रीका का मूल निवासी है। यह अपने तारे के आकार के फूलों के लिए प्रसिद्ध है, जो आकर्षक पैटर्नों के साथ होते हैं।"
    },
    "zh": {
      "common_name": "星形花",
      "description": "斯塔佩利亚·瓦里戈塔是一种原产于南非的多肉植物。以其星形的花朵和独特的图案而闻名。"
    },
    "ja": {
      "common_name": "星形花",
      "description": "スタペリア・ヴァリエガータは南アフリカ原産の多肉植物です。星形の花と特徴的な模様で知られています。"
    },
    "ko": {
      "common_name": "별꽃",
      "description": "스태펠리아 바리에가타는 남아프리카 원산의 다육 식물입니다. 별 모양의 꽃과 독특한 패턴으로 유명합니다."
    }
  },
