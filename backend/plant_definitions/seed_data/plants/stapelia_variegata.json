  "translations": {
    "en": {
      "common_name": "Variegated Stapelia",
      "description": "Stapelia variegata is a succulent plant known for its star-shaped flowers and unique appearance. It thrives in well-drained soil with moderate watering."
    },
    "pl": {
      "common_name": "Variegowana Stapelia",
      "description": "Stapelia variegata to sukulentna roślina o gwiaździstych kwiatach. Wymaga dobrze przepuszczalnej gleby i umiarkowanego podlewania."
    },
    "de": {
      "common_name": "Variegierte Stapelia",
      "description": "Stapelia variegata ist eine Sukkulente mit sternförmigen Blüten und einzigartigem Aussehen. Sie gedeiht in gut durchlässigem Boden und mäßigem Gießen."
    },
    "fr": {
      "common_name": "Stapélia panachée",
      "description": "Stapelia variegata est une plante succulente avec des fleurs en forme d'étoile. Elle préfère un sol bien drainé et un arrosage modéré."
    },
    "it": {
      "common_name": "Stapelia variegata",
      "description": "Stapelia variegata è una pianta succulenta con fiori a forma di stella. Preferisce terreni ben drenati e annaffiature moderate."
    },
    "es": {
      "common_name": "Stapelia variegata",
      "description": "Stapelia variegata es una planta suculenta conocida por sus flores en forma de estrella. Crece mejor en suelos bien drenados con riego moderado."
    },
    "pt": {
      "common_name": "Stapelia variegata",
      "description": "Stapelia variegata é uma planta suculenta com flores em forma de estrela. Prefere solo bem drenado e rega moderada."
    },
    "ar": {
      "common_name": "ستابيليا متنوع",
      "description": "Stapelia variegata هي نبات عصاري معروف بأزهاره على شكل نجمة. ينمو في تربة جيدة التصريف مع ري معتدل."
    },
    "hi": {
      "common_name": "वैरिगेटेड स्टेपेलिया",
      "description": "Stapelia variegata एक सुगंधित पौधा है जिसमें तारा आकार के फूल होते हैं। यह अच्छे जल निकासी वाली मिट्टी में और मध्यम पानी में उगता है।"
    },
    "zh": {
      "common_name": "变种 Stapelia",
      "description": "Stapelia variegata 是一种多肉植物，具有星形的花朵和独特的外观。它喜欢排水良好的土壤和适度的浇水。"
    },
    "ja": {
      "common_name": "バリエガタ・ステープリア",
      "description": "Stapelia variegata は星形の花を持つ多肉植物です。排水の良い土壌と適度な水やりが必要です。"
    },
    "ko": {
      "common_name": "변종 스테이펠리아",
      "description": "Stapelia variegata는 별 모양의 꽃을 가진 다육 식물입니다. 잘 배수되는 토양과 적당한 물주기가 필요합니다."
    }
  },
