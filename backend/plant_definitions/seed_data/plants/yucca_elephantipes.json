  "translations": {
    "en": {
      "common_name": "Spineless Yucca",
      "description": "Yucca elephantipes is a slow-growing, tree-like plant. It is commonly grown indoors as a decorative plant."
    },
    "pl": {
      "common_name": "Juka bezkolcowa",
      "description": "Yucca elephantipes to powoli rosnąca roślina przypominająca drzewo. Często uprawiana w pomieszczeniach jako roślina ozdobna."
    },
    "de": {
      "common_name": "Riesen-Juka",
      "description": "Yucca elephantipes ist eine langsam wachsende, baumähnliche Pflanze. Sie wird häufig als Zierpflanze in Innenräumen gezüchtet."
    },
    "fr": {
      "common_name": "Yucca sans épines",
      "description": "Yucca elephantipes est une plante à croissance lente en forme d'arbre. Elle est couramment cultivée à l'intérieur comme plante décorative."
    },
    "it": {
      "common_name": "Yucca senza spine",
      "description": "Yucca elephantipes è una pianta a crescita lenta simile a un albero. Viene comunemente coltivata come pianta decorativa in ambienti interni."
    },
    "es": {
      "common_name": "Yucca sin espinas",
      "description": "Yucca elephantipes es una planta de crecimiento lento que se asemeja a un árbol. Se cultiva comúnmente como planta decorativa en interiores."
    },
    "pt": {
      "common_name": "Yucca sem espinhos",
      "description": "Yucca elephantipes é uma planta de crescimento lento semelhante a uma árvore. É comumente cultivada em ambientes internos como planta decorativa."
    },
    "ar": {
      "common_name": "يوكا بدون شوك",
      "description": "يوكا إلفانتايب هي نبتة بطيئة النمو تشبه الشجرة. تزرع عادة كزرع زخرفي في الأماكن المغلقة."
    },
    "hi": {
      "common_name": "स्पाइनलेस युका",
      "description": "युका एलीफेंटिप्स एक धीमी वृद्धि करने वाली पौधा है, जो पेड़ जैसा होता है। इसे आमतौर पर आंतरिक सजावटी पौधे के रूप में उगाया जाता है।"
    },
    "zh": {
      "common_name": "无刺尤卡",
      "description": "尤卡象牙树是一种生长缓慢、像树一样的植物。它通常作为室内装饰植物栽培。"
    },
    "ja": {
      "common_name": "スパインレスユッカ",
      "description": "ユッカエレファンティペスは成長が遅く、木のような形の植物です。主に室内の装飾植物として栽培されます。"
    },
    "ko": {
      "common_name": "가시 없는 유카",
      "description": "유카 엘레판티페스는 성장 속도가 느린 나무처럼 생긴 식물입니다. 주로 실내 장식용 식물로 재배됩니다."
    }
  },
