  "translations": {
    "en": {
      "common_name": "Spineless Yucca",
      "description": "Yucca elephantipes is a hardy evergreen plant native to Central America. It tolerates bright sun, dry air, and infrequent watering, making it suitable for indoor environments."
    },
    "pl": {
      "common_name": "Jukka bezkolcowa",
      "description": "Yucca elephantipes to wytrzymała roślina pochodząca z Ameryki Środkowej. Dobrze znosi jasne światło, suche powietrze i rzadkie podlewanie."
    },
    "de": {
      "common_name": "Riesen-Palmlilie",
      "description": "Yucca elephantipes stammt aus Mittelamerika und ist sehr robust. Sie benötigt viel Licht, wenig Wasser und ist pflegeleicht."
    },
    "fr": {
      "common_name": "Yucca pied d’éléphant",
      "description": "Yucca elephantipes est originaire d’Amérique centrale. Elle supporte bien la lumière vive, l’air sec et des arrosages espacés."
    },
    "it": {
      "common_name": "Yucca senza spine",
      "description": "Yucca elephantipes è una pianta resistente dell’America Centrale. Predilige luce intensa e annaffiature poco frequenti."
    },
    "es": {
      "common_name": "Yuca sin espinas",
      "description": "Yucca elephantipes es originaria de América Central. Tolera bien la luz intensa, el aire seco y el riego escaso."
    },
    "pt": {
      "common_name": "Iúca sem espinhos",
      "description": "Yucca elephantipes é uma planta robusta da América Central. Cresce bem com luz forte e pouca água."
    },
    "ar": {
      "common_name": "يوكا بدون أشواك",
      "description": "نبات يوكا إيليفانتبيس موطنه أمريكا الوسطى. يتحمل الضوء القوي والري القليل والهواء الجاف."
    },
    "hi": {
      "common_name": "स्पाइनलेस युक्का",
      "description": "युक्का एलीफैनटाइप्स मध्य अमेरिका का पौधा है। यह तेज रोशनी, कम पानी और शुष्क हवा में अच्छा रहता है।"
    },
    "zh": {
      "common_name": "无刺丝兰",
      "description": "Yucca elephantipes 原产于中美洲。它耐强光、干燥空气和少量浇水，适合室内种植。"
    },
    "ja": {
      "common_name": "スピンレスユッカ",
      "description": "ユッカ・エレファンティペスは中米原産の丈夫な植物です。明るい光と控えめな水やりを好みます。"
    },
    "ko": {
      "common_name": "무가시 유카",
      "description": "Yucca elephantipes는 중앙아메리카 원산의 강한 식물입니다. 밝은 빛과 적은 물에서도 잘 자랍니다."
    }
