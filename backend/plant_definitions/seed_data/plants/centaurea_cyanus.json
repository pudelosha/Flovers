  "translations": {
    "en": {
      "common_name": "Cornflower",
      "description": "Centaurea cyanus is an annual wildflower native to Europe, commonly found in open fields and meadows. It produces bright blue flowers and prefers full sun with well-drained soil."
    },
    "pl": {
      "common_name": "Chaber bławatek",
      "description": "Centaurea cyanus to jednoroczna roślina łąkowa pochodząca z Europy. Kwitnie na niebiesko i najlepiej rośnie w pełnym słońcu na przepuszczalnej glebie."
    },
    "de": {
      "common_name": "Kornblume",
      "description": "Centaurea cyanus ist eine einjährige Wildblume aus Europa. Sie wächst auf offenen Flächen und bevorzugt sonnige Standorte mit gut durchlässigem Boden."
    },
    "fr": {
      "common_name": "Bleuet des champs",
      "description": "Centaurea cyanus est une plante annuelle originaire d’Europe. Elle pousse dans les champs ouverts et apprécie le plein soleil et les sols drainés."
    },
    "it": {
      "common_name": "Fiordaliso",
      "description": "Centaurea cyanus è una pianta annuale europea. Cresce bene in pieno sole e in terreni ben drenati, producendo fiori blu intensi."
    },
    "es": {
      "common_name": "Aciano",
      "description": "Centaurea cyanus es una planta anual originaria de Europa. Se encuentra en campos abiertos y necesita sol pleno y suelo bien drenado."
    },
    "pt": {
      "common_name": "Centáurea-azul",
      "description": "Centaurea cyanus é uma planta anual europeia. Desenvolve-se em áreas abertas, com sol pleno e solo bem drenado."
    },
    "ar": {
      "common_name": "زهرة الذرة",
      "description": "Centaurea cyanus نبات سنوي موطنه أوروبا. ينمو في الحقول المفتوحة ويفضل الشمس الكاملة والتربة جيدة التصريف."
    },
    "hi": {
      "common_name": "कॉर्नफ्लावर",
      "description": "Centaurea cyanus यूरोप की एक वार्षिक जंगली फूल वाली पौधा है। यह खुली जगहों में उगता है और पूरी धूप पसंद करता है।"
    },
    "zh": {
      "common_name": "矢车菊",
      "description": "Centaurea cyanus 是原产于欧洲的一年生野花。常见于开阔地带，喜全日照和良好排水的土壤。"
    },
    "ja": {
      "common_name": "ヤグルマギク",
      "description": "Centaurea cyanus はヨーロッパ原産の一年草です。日当たりと水はけの良い土壌を好みます。"
    },
    "ko": {
      "common_name": "수레국화",
      "description": "Centaurea cyanus는 유럽 원산의 한해살이 꽃입니다. 햇빛이 잘 드는 배수가 좋은 토양에서 잘 자랍니다."
    }
  },
