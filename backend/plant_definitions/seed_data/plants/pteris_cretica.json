  "translations": {
    "en": {
      "common_name": "Cretan Brake Fern",
      "description": "Pteris cretica is a fern native to tropical regions. It thrives in moderate light and humid environments."
    },
    "pl": {
      "common_name": "Paproć Kreta",
      "description": "Pteris cretica to paproć pochodząca z tropikalnych regionów. Wymaga umiarkowanego światła i wilgotnego środowiska."
    },
    "de": {
      "common_name": "Kretische Schwertfarn",
      "description": "Pteris cretica ist ein Farn, der in tropischen Regionen beheimatet ist. Er gedeiht bei mäßigem Licht und hoher Luftfeuchtigkeit."
    },
    "fr": {
      "common_name": "Fougère de Crète",
      "description": "Pteris cretica est une fougère originaire des régions tropicales. Elle prospère dans des environnements à lumière modérée et à forte humidité."
    },
    "it": {
      "common_name": "Felce di Creta",
      "description": "Pteris cretica è una felce originaria delle regioni tropicali. Cresce in ambienti con luce moderata e umidità elevata."
    },
    "es": {
      "common_name": "Helecho de Creta",
      "description": "Pteris cretica es un helecho originario de regiones tropicales. Crece en ambientes de luz moderada y alta humedad."
    },
    "pt": {
      "common_name": "Samambaia de Creta",
      "description": "Pteris cretica é uma samambaia originária de regiões tropicais. Ela cresce em ambientes de luz moderada e alta umidade."
    },
    "ar": {
      "common_name": "سرخس كريت",
      "description": "Pteris cretica هو سرخس موطنه المناطق الاستوائية. ينمو في بيئات ذات إضاءة معتدلة ورطوبة عالية."
    },
    "hi": {
      "common_name": "क्रीट ब्रेक फर्न",
      "description": "Pteris cretica एक फर्न है जो उष्णकटिबंधीय क्षेत्रों में पाया जाता है। यह मध्यम रोशनी और उच्च आर्द्रता वाले वातावरण में उगता है।"
    },
    "zh": {
      "common_name": "克里特岛蕨",
      "description": "Pteris cretica 是一种原产于热带地区的蕨类植物。它在中等光照和高湿度的环境中生长。"
    },
    "ja": {
      "common_name": "クレタ島のブレークシダ",
      "description": "Pteris cretica は熱帯地域に自生するシダ植物です。中程度の光と高い湿度の環境で育ちます。"
    },
    "ko": {
      "common_name": "크레타 브레이크 고사리",
      "description": "Pteris cretica는 열대 지역 원산의 고사리입니다. 중간 정도의 빛과 높은 습도의 환경에서 자랍니다."
    }
  },
