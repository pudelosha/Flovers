  "translations": {
    "en": {
      "common_name": "Spearmint",
      "description": "Spearmint is a fragrant herb used in cooking and for medicinal purposes. It prefers moderate sunlight and well-drained soil."
    },
    "pl": {
      "common_name": "Mięta zielona",
      "description": "Mięta zielona to pachnące zioło stosowane w kuchni i medycynie. Preferuje umiarkowane światło i dobrze przepuszczalną glebę."
    },
    "de": {
      "common_name": "Grüne Minze",
      "description": "Grüne Minze ist ein duftendes Kraut, das in der Küche und für medizinische Zwecke verwendet wird. Sie bevorzugt mäßiges Sonnenlicht und gut durchlässigen Boden."
    },
    "fr": {
      "common_name": "Menthe verte",
      "description": "La menthe verte est une herbe parfumée utilisée en cuisine et à des fins médicinales. Elle préfère une lumière modérée et un sol bien drainé."
    },
    "it": {
      "common_name": "Menta verde",
      "description": "La menta verde è un'erba profumata utilizzata in cucina e per scopi medicinali. Preferisce la luce moderata e un terreno ben drenato."
    },
    "es": {
      "common_name": "Menta verde",
      "description": "La menta verde es una hierba aromática utilizada en la cocina y con fines medicinales. Prefiere luz moderada y un suelo bien drenado."
    },
    "pt": {
      "common_name": "Hortelã verde",
      "description": "A hortelã verde é uma erva aromática usada na culinária e para fins medicinais. Prefere luz moderada e solo bem drenado."
    },
    "ar": {
      "common_name": "نعناع أخضر",
      "description": "النعناع الأخضر هو عشب عطري يستخدم في الطهي ولأغراض طبية. يفضل الضوء المعتدل والتربة جيدة التصريف."
    },
    "hi": {
      "common_name": "हरी पुदीना",
      "description": "हरी पुदीना एक सुगंधित पौधा है जो खाना पकाने और औषधीय उपयोग के लिए प्रयोग किया जाता है। यह मध्यम प्रकाश और अच्छी जल निकासी वाली मिट्टी पसंद करता है।"
    },
    "zh": {
      "common_name": "绿薄荷",
      "description": "绿薄荷是一种芳香的草本植物，用于烹饪和药用。它喜欢中等阳光和排水良好的土壤。"
    },
    "ja": {
      "common_name": "グリーンミント",
      "description": "グリーンミントは料理や薬用に使われる香り高いハーブです。中程度の光と排水性の良い土壌を好みます。"
    },
    "ko": {
      "common_name": "그린 민트",
      "description": "그린 민트는 요리와 의약 용도로 사용되는 향긋한 허브입니다. 중간 정도의 빛과 배수가 좋은 토양을 선호합니다."
    }
  },
