  "translations": {
    "en": {
      "common_name": "Corn",
      "description": "Maize is a tall annual cereal grass grown for grain. It requires full sun and consistent water during growth."
    },
    "pl": {
      "common_name": "Kukurydza",
      "description": "Kukurydza to wysoka jednoroczna trawa uprawna. Wymaga pełnego słońca i stałej wilgotności."
    },
    "de": {
      "common_name": "Mais",
      "description": "Mais ist ein hohes einjähriges Getreidegras. Es braucht volle Sonne und gleichmäßige Feuchte."
    },
    "fr": {
      "common_name": "Maïs",
      "description": "Le maïs est une grande herbe annuelle cultivée pour ses grains. Il nécessite plein soleil et humidité constante."
    },
    "it": {
      "common_name": "Mais",
      "description": "Il mais è un alta erba annuale coltivata per i semi. Richiede pieno sole e umidità costante."
    },
    "es": {
      "common_name": "Maíz",
      "description": "El maíz es un alto pasto anual cultivado por sus granos. Requiere sol pleno y humedad constante."
    },
    "pt": {
      "common_name": "Milho",
      "description": "O milho é uma gramínea anual alta cultivada pelo grão. Requer sol pleno e umidade constante."
    },
    "ar": {
      "common_name": "الذرة",
      "description": "الذرة نبات سنوي عالي يُزرع من أجل الحبوب. يحتاج شمس كاملة ورطوبة منتظمة."
    },
    "hi": {
      "common_name": "मक्का",
      "description": "मक्का अनाज के लिए उगाई जाने वाली लंबी एकवर्षीय घास है. यह पूर्ण सूर्य और नमी चाहता है."
    },
    "zh": {
      "common_name": "玉米",
      "description": "玉米是一种高大一年生禾草，种植以收获谷物。它需要充足阳光和持续湿润。"
    },
    "ja": {
      "common_name": "トウモロコシ",
      "description": "トウモロコシは穀物用に栽培される高い一年草です。日光と安定した水分が必要です。"
    },
    "ko": {
      "common_name": "옥수수",
      "description": "옥수수는 곡물용으로 재배되는 큰 일년생 풀입니다. 직사광선과 일정한 수분이 필요합니다."
    }
  },
