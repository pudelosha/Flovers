  "translations": {
    "en": {
      "common_name": "Rosemary",
      "description": "Rosemary is a fragrant, woody herb. It thrives in full sun and well-drained soil."
    },
    "pl": {
      "common_name": "Rozmaryn",
      "description": "Rozmaryn to pachnąca roślina z rodziny jasnotowatych. Preferuje pełne słońce i dobrze przepuszczalną glebę."
    },
    "de": {
      "common_name": "Rosmarin",
      "description": "Rosmarin ist ein duftendes, holziges Kraut. Es gedeiht in voller Sonne und gut durchlässigem Boden."
    },
    "fr": {
      "common_name": "Romarin",
      "description": "Le romarin est une herbe aromatique et ligneuse. Il prospère au soleil et dans un sol bien drainé."
    },
    "it": {
      "common_name": "Rosmarino",
      "description": "Il rosmarino è un'erba fragrante e legnosa. Cresce bene al sole pieno e in terreni ben drenati."
    },
    "es": {
      "common_name": "Romero",
      "description": "El romero es una hierba aromática y leñosa. Crece bien a pleno sol y en suelos bien drenados."
    },
    "pt": {
      "common_name": "Alecrim",
      "description": "O alecrim é uma erva aromática e lenhosa. Desenvolve-se bem ao sol e em solos bem drenados."
    },
    "ar": {
      "common_name": "إكليل الجبل",
      "description": "إكليل الجبل هو عشب عطري خشبي. ينمو بشكل جيد في الشمس الكاملة والتربة جيدة التصريف."
    },
    "hi": {
      "common_name": "रोजमेरी",
      "description": "रोजमेरी एक सुगंधित, लकड़ी का पौधा है। यह पूरी धूप और अच्छे जल निकासी वाली मिट्टी में अच्छी तरह से बढ़ता है।"
    },
    "zh": {
      "common_name": "迷迭香",
      "description": "迷迭香是一种芳香的木本草药。它在阳光充足、排水良好的土壤中茁壮成长。"
    },
    "ja": {
      "common_name": "ローズマリー",
      "description": "ローズマリーは芳香のある木本植物です。完全な日光と排水の良い土壌で成長します。"
    },
    "ko": {
      "common_name": "로즈마리",
      "description": "로즈마리는 향기로운 나무풀입니다. 완전한 햇빛과 배수가 좋은 토양에서 잘 자랍니다."
    }
  },
