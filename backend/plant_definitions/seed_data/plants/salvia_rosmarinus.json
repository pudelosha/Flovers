  "translations": {
    "en": {
      "common_name": "Rosemary",
      "description": "Rosemary is a fragrant herb native to the Mediterranean. It thrives in full sunlight and well-drained soil."
    },
    "pl": {
      "common_name": "Rozmaryn",
      "description": "Rozmaryn to aromatyczna roślina pochodząca z rejonu Morza Śródziemnego. Rośnie najlepiej w pełnym słońcu i dobrze przepuszczalnej glebie."
    },
    "de": {
      "common_name": "Rosmarin",
      "description": "Rosmarin ist ein duftendes Kraut, das im Mittelmeerraum heimisch ist. Es gedeiht in voller Sonne und gut durchlässigem Boden."
    },
    "fr": {
      "common_name": "Romarin",
      "description": "Le romarin est une herbe parfumée originaire de la Méditerranée. Il prospère sous un soleil plein et dans un sol bien drainé."
    },
    "it": {
      "common_name": "Rosmarino",
      "description": "Il rosmarino è un'erba aromatica originaria del Mediterraneo. Cresce meglio al sole e in terreni ben drenati."
    },
    "es": {
      "common_name": "Romero",
      "description": "El romero es una hierba aromática originaria de la región mediterránea. Crece bien a pleno sol y en suelos bien drenados."
    },
    "pt": {
      "common_name": "Alecrim",
      "description": "O alecrim é uma erva aromática nativa da região mediterrânea. Cresce bem sob luz solar direta e solo bem drenado."
    },
    "ar": {
      "common_name": "إكليل الجبل",
      "description": "إكليل الجبل هو عشب عطري نشأ في منطقة البحر الأبيض المتوسط. ينمو جيدًا في ضوء الشمس الكامل والتربة جيدة التصريف."
    },
    "hi": {
      "common_name": "Rosemary",
      "description": "Rosemary एक खुशबूदार जड़ी-बूटी है जो भूमध्यसागरीय क्षेत्र की मूल निवासी है। यह पूर्ण सूर्य और अच्छी जल निकासी वाली मिट्टी में अच्छी तरह से बढ़ता है।"
    },
    "zh": {
      "common_name": "迷迭香",
      "description": "迷迭香是一种原产于地中海的芳香草本植物。它在充足阳光和排水良好的土壤中生长得最好。"
    },
    "ja": {
      "common_name": "ローズマリー",
      "description": "ローズマリーは地中海原産の香り高いハーブです。完全な日光と良好な排水の土壌でよく成長します。"
    },
    "ko": {
      "common_name": "로즈마리",
      "description": "로즈마리는 지중해 지역이 원산지인 향기로운 허브입니다. 완전한 햇볕과 잘 배수되는 토양에서 잘 자랍니다."
    }
  },
