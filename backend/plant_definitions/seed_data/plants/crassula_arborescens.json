  "translations": {
    "en": {
      "common_name": "Silver Jade Plant",
      "description": "Crassula arborescens is a small succulent with thick, fleshy leaves. It is easy to care for and thrives in moderate light."
    },
    "pl": {
      "common_name": "Srebrna roślina jadesowa",
      "description": "Crassula arborescens to mały sukulent o grubych, mięsistych liściach. Jest łatwa w pielęgnacji i rośnie w umiarkowanym świetle."
    },
    "de": {
      "common_name": "Silber-Jadepflanze",
      "description": "Crassula arborescens ist eine kleine Sukkulente mit dicken, fleischigen Blättern. Sie ist pflegeleicht und gedeiht bei mittlerem Licht."
    },
    "fr": {
      "common_name": "Plante Jade Argentée",
      "description": "Crassula arborescens est une petite succulente aux feuilles épaisses et charnues. Elle est facile à entretenir et prospère dans une lumière modérée."
    },
    "it": {
      "common_name": "Pianta di Giada Argentata",
      "description": "Crassula arborescens è una piccola succulenta con foglie spesse e carnose. È facile da curare e cresce in luce moderata."
    },
    "es": {
      "common_name": "Planta de Jade Plateada",
      "description": "Crassula arborescens es una pequeña suculenta con hojas gruesas y carnosas. Es fácil de cuidar y crece con luz moderada."
    },
    "pt": {
      "common_name": "Planta de Jade Prateada",
      "description": "Crassula arborescens é uma pequena suculenta com folhas grossas e carnudas. É fácil de cuidar e cresce com luz moderada."
    },
    "ar": {
      "common_name": "نبات اليشم الفضي",
      "description": "Crassula arborescens هو نبات عصاري صغير ذو أوراق سميكة ولحمية. سهل العناية وينمو في الضوء المعتدل."
    },
    "hi": {
      "common_name": "चांदी का जेड पौधा",
      "description": "Crassula arborescens एक छोटा सा रसीला पौधा है जिसकी पत्तियाँ मोटी और मांसल होती हैं। यह देखभाल में आसान है और मध्यम रोशनी में उगता है।"
    },
    "zh": {
      "common_name": "银玉植物",
      "description": "Crassula arborescens 是一种小型的多肉植物，具有厚实的肉质叶子。它容易护理，在中等光照下生长。"
    },
    "ja": {
      "common_name": "シルバージェイドプラント",
      "description": "Crassula arborescens は小さな多肉植物で、厚くて肉質の葉を持っています。手入れが簡単で、適度な光で成長します。"
    },
    "ko": {
      "common_name": "실버 제이드 식물",
      "description": "Crassula arborescens는 두껍고 육즙이 많은 잎을 가진 작은 다육 식물입니다. 관리가 쉽고 적당한 빛에서 자랍니다."
    }
  },
