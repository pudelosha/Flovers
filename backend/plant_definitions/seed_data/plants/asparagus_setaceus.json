  "translations": {
    "en": {
      "common_name": "Asparagus Fern",
      "description": "Asparagus setaceus is an ornamental plant with fine, fern-like foliage. It prefers bright indirect light and moist, well-drained soil."
    },
    "pl": {
      "common_name": "Szparag pierzasty",
      "description": "Asparagus setaceus to roślina ozdobna o delikatnych liściach. Wymaga jasnego światła rozproszonego i wilgotnej, dobrze przepuszczalnej gleby."
    },
    "de": {
      "common_name": "Spargelfarn",
      "description": "Asparagus setaceus ist eine Zierpflanze mit fein farnartigem Laub. Sie bevorzugt helles indirektes Licht und feuchte, gut durchlässige Erde."
    },
    "fr": {
      "common_name": "Fougère asperge",
      "description": "Asparagus setaceus est une plante ornementale aux fines feuilles. Elle aime la lumière indirecte et un sol humide bien drainé."
    },
    "it": {
      "common_name": "Asparago felce",
      "description": "Asparagus setaceus è una pianta ornamentale con foglie delicate. Preferisce luce indiretta e terreno umido ben drenato."
    },
    "es": {
      "common_name": "Helecho de espárrago",
      "description": "Asparagus setaceus es una planta ornamental de follaje fino. Prefiere luz indirecta y suelo húmedo bien drenado."
    },
    "pt": {
      "common_name": "Aspargo samambaia",
      "description": "Asparagus setaceus é uma planta ornamental de folhagem fina. Prefere luz indireta e solo úmido bem drenado."
    },
    "ar": {
      "common_name": "سرخس الهليون",
      "description": "Asparagus setaceus نبات زينة بأوراق دقيقة. يفضل الضوء غير المباشر وتربة رطبة جيدة التصريف."
    },
    "hi": {
      "common_name": "एस्पैरेगस फर्न",
      "description": "Asparagus setaceus एक सजावटी पौधा है जिसमें नाज़ुक पत्तियाँ होती हैं। यह परोक्ष रोशनी और नम मिट्टी पसंद करता है."
    },
    "zh": {
      "common_name": "芦笋蕨",
      "description": "Asparagus setaceus 是观赏植物，具有细蕨状叶片。它喜欢明亮散射光和湿润排水良好的土壤."
    },
    "ja": {
      "common_name": "アスパラガスファーン",
      "description": "Asparagus setaceus は細かい葉の観葉植物です。明るい間接光と湿った排水良い土壌を好みます。"
    },
    "ko": {
      "common_name": "아스파라거스 펀",
      "description": "Asparagus setaceus 는 섬세한 잎이 있는 관상용 식물입니다. 밝은 간접광과 촉촉한 배수토를 선호합니다."
    }
  },
