  "translations": {
    "en": {
      "common_name": "Common Lilac",
      "description": "Syringa vulgaris is a fragrant flowering shrub. It thrives in sunny spots and grows up to 5 meters in height."
    },
    "pl": {
      "common_name": "Lilak pospolity",
      "description": "Syringa vulgaris to pachnący krzew ozdobny. Rośnie najlepiej w słonecznych miejscach i osiąga wysokość do 5 metrów."
    },
    "de": {
      "common_name": "Gemeiner Flieder",
      "description": "Syringa vulgaris ist ein duftender Strauch. Er gedeiht an sonnigen Standorten und erreicht eine Höhe von bis zu 5 Metern."
    },
    "fr": {
      "common_name": "Lilas commun",
      "description": "Syringa vulgaris est un arbuste à fleurs parfumées. Il prospère dans des endroits ensoleillés et atteint jusqu'à 5 mètres de hauteur."
    },
    "it": {
      "common_name": "Lillà comune",
      "description": "Syringa vulgaris è un arbusto fiorito profumato. Cresce bene in posti soleggiati e raggiunge i 5 metri di altezza."
    },
    "es": {
      "common_name": "Lila común",
      "description": "Syringa vulgaris es un arbusto con flores fragantes. Crece bien en lugares soleados y alcanza hasta 5 metros de altura."
    },
    "pt": {
      "common_name": "Lilás comum",
      "description": "Syringa vulgaris é um arbusto florido e perfumado. Cresce bem em locais ensolarados e pode alcançar até 5 metros de altura."
    },
    "ar": {
      "common_name": "الليلاك العادي",
      "description": "سيرينغا فولغاريس هو شجيرة مزهرة ذات رائحة عطرة. يزدهر في الأماكن المشمسة ويمكن أن يصل ارتفاعه إلى 5 أمتار."
    },
    "hi": {
      "common_name": "सामान्य लाइलैक",
      "description": "Syringa vulgaris एक सुगंधित फूलदार झाड़ी है। यह धूप वाली जगहों में अच्छी तरह से बढ़ता है और 5 मीटर तक ऊँचा हो सकता है।"
    },
    "zh": {
      "common_name": "普通紫丁香",
      "description": "Syringa vulgaris 是一种芳香的开花灌木。它在阳光充足的地方生长，最高可达 5 米。"
    },
    "ja": {
      "common_name": "コモンライラック",
      "description": "Syringa vulgaris は香りのよい花の低木です。日当たりの良い場所でよく育ち、最大で 5 メートルの高さになります。"
    },
    "ko": {
      "common_name": "일반 라일락",
      "description": "Syringa vulgaris는 향기로운 꽃이 피는 관목입니다. 햇볕이 잘 드는 곳에서 잘 자라며 최대 5미터까지 자랍니다."
    }
  },
