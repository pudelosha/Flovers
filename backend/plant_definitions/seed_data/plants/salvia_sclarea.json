  "translations": {
    "en": {
      "common_name": "Clary Sage",
      "description": "Clary sage is an aromatic herb known for its medicinal properties. It thrives in well-draining soil and moderate light."
    },
    "pl": {
      "common_name": "Szałwia muszkatołowa",
      "description": "Szałwia muszkatołowa to aromatyczne zioło znane ze swoich właściwości leczniczych. Rośnie w dobrze przepuszczalnej glebie i umiarkowanym świetle."
    },
    "de": {
      "common_name": "Muskatellersalbei",
      "description": "Muskatellersalbei ist ein aromatisches Kraut, das für seine medizinischen Eigenschaften bekannt ist. Es gedeiht in gut durchlässigem Boden und mittlerem Licht."
    },
    "fr": {
      "common_name": "Sauge sclarée",
      "description": "La sauge sclarée est une herbe aromatique reconnue pour ses propriétés médicinales. Elle prospère dans un sol bien drainé et une lumière modérée."
    },
    "it": {
      "common_name": "Salvia sclarea",
      "description": "La salvia sclarea è un'erba aromatica nota per le sue proprietà medicinali. Cresce in un terreno ben drenato e con luce moderata."
    },
    "es": {
      "common_name": "Salvia esclarea",
      "description": "La salvia esclarea es una hierba aromática conocida por sus propiedades medicinales. Crece en suelos bien drenados y con luz moderada."
    },
    "pt": {
      "common_name": "Sálvia esclarecida",
      "description": "A sálvia esclarecida é uma erva aromática conhecida pelas suas propriedades medicinais. Cresce em solo bem drenado e luz moderada."
    },
    "ar": {
      "common_name": "المريمية المسكية",
      "description": "المريمية المسكية هي عشبة عطرية معروفة بخصائصها الطبية. تنمو في التربة جيدة التصريف والضوء المعتدل."
    },
    "hi": {
      "common_name": "क्लेरी सैज",
      "description": "क्लेरी सैज एक सुगंधित औषधीय पौधा है। यह अच्छे जल निकासी वाली मिट्टी और मध्यम रोशनी में बढ़ता है।"
    },
    "zh": {
      "common_name": "克拉里鼠尾草",
      "description": "克拉里鼠尾草是一种具有药用特性的芳香草本植物。它在排水良好的土壤和中等光照下生长。"
    },
    "ja": {
      "common_name": "クラリーセージ",
      "description": "クラリーセージはその薬用特性で知られる香りの良いハーブです。良好な排水性の土壌と中程度の光で育ちます。"
    },
    "ko": {
      "common_name": "클라리 세이지",
      "description": "클라리 세이지는 약용 특성으로 알려진 향기로운 허브입니다. 배수가 좋은 토양과 중간 정도의 빛에서 자랍니다."
    }
  },
