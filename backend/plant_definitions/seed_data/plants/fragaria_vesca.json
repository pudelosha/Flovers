  "translations": {
    "en": {
      "common_name": "Wild Strawberry",
      "description": "A small, perennial plant that produces sweet, red berries. Often found in wild gardens or as a ground cover."
    },
    "pl": {
      "common_name": "Truskawka leśna",
      "description": "Mała, wieloletnia roślina produkująca słodkie, czerwone jagody. Często spotykana w ogrodach dzikich lub jako pokrycie ziemi."
    },
    "de": {
      "common_name": "Wilde Erdbeere",
      "description": "Eine kleine, mehrjährige Pflanze, die süße, rote Beeren produziert. Oft in wilden Gärten oder als Bodenbedeckung zu finden."
    },
    "fr": {
      "common_name": "Fraise des bois",
      "description": "Une petite plante vivace qui produit des baies rouges et sucrées. Souvent trouvée dans les jardins sauvages ou comme couverture du sol."
    },
    "it": {
      "common_name": "Fragola selvatica",
      "description": "Una piccola pianta perenne che produce bacche rosse e dolci. Spesso trovata nei giardini selvatici o come copertura del terreno."
    },
    "es": {
      "common_name": "Fresa silvestre",
      "description": "Una pequeña planta perenne que produce bayas rojas y dulces. Se encuentra a menudo en jardines silvestres o como cobertura del suelo."
    },
    "pt": {
      "common_name": "Morango silvestre",
      "description": "Uma pequena planta perene que produz frutos vermelhos e doces. Frequentemente encontrada em jardins silvestres ou como cobertura do solo."
    },
    "ar": {
      "common_name": "الفراولة البرية",
      "description": "نبات معمر صغير ينتج توتًا أحمر حلو. يوجد غالبًا في الحدائق البرية أو كغطاء للأرض."
    },
    "hi": {
      "common_name": "जंगली स्ट्रॉबेरी",
      "description": "एक छोटा, द्विवार्षिक पौधा जो मीठे, लाल बेरियां पैदा करता है। यह अक्सर जंगली बागों में या भूमि आवरण के रूप में पाया जाता है।"
    },
    "zh": {
      "common_name": "野草莓",
      "description": "一种小型多年生植物，结出甜美的红色浆果。常见于野生花园或作为地面覆盖。"
    },
    "ja": {
      "common_name": "野いちご",
      "description": "小さな多年草で、甘くて赤いベリーを生産します。野生の庭や地面カバーとしてよく見られます。"
    },
    "ko": {
      "common_name": "야생 딸기",
      "description": "작고 다년생 식물로 달콤한 빨간 열매를 맺습니다. 종종 야생 정원이나 땅 덮개로 발견됩니다."
    }
  },
