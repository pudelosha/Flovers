  "translations": {
    "en": {
      "common_name": "Valerian",
      "description": "Valerian is a perennial herb known for its calming effects. It is commonly used in herbal medicine to treat insomnia and anxiety."
    },
    "pl": {
      "common_name": "Waleriana",
      "description": "Waleriana to roślina wieloletnia, znana ze swoich uspokajających właściwości. Często używana w medycynie ziołowej do leczenia bezsenności i lęków."
    },
    "de": {
      "common_name": "Baldrian",
      "description": "Baldrian ist ein mehrjähriges Kraut, das für seine beruhigenden Eigenschaften bekannt ist. Es wird häufig in der Kräutermedizin verwendet, um Schlaflosigkeit und Angst zu behandeln."
    },
    "fr": {
      "common_name": "Valériane",
      "description": "La valériane est une plante vivace reconnue pour ses effets apaisants. Elle est couramment utilisée en phytothérapie pour traiter l'insomnie et l'anxiété."
    },
    "it": {
      "common_name": "Valeriana",
      "description": "La valeriana è una pianta perenne conosciuta per i suoi effetti calmanti. È comunemente usata in medicina erboristica per trattare l'insonnia e l'ansia."
    },
    "es": {
      "common_name": "Valeriana",
      "description": "La valeriana es una planta perenne conocida por sus efectos calmantes. Se utiliza comúnmente en la medicina herbal para tratar el insomnio y la ansiedad."
    },
    "pt": {
      "common_name": "Valeriana",
      "description": "A valeriana é uma planta perene conhecida por seus efeitos calmantes. É comumente usada na medicina herbal para tratar insônia e ansiedade."
    },
    "ar": {
      "common_name": "فاليريان",
      "description": "الفاليريان هو نبات معمر معروف بتأثيراته المهدئة. يتم استخدامه عادة في الطب العشبي لعلاج الأرق والقلق."
    },
    "hi": {
      "common_name": "वैलेरियाना",
      "description": "वैलेरियाना एक वार्षिक जड़ी-बूटी है जिसे शांतिपूर्ण प्रभावों के लिए जाना जाता है। इसका उपयोग अक्सर हर्बल दवाइयों में अनिद्रा और चिंता का इलाज करने के लिए किया जाता है."
    },
    "zh": {
      "common_name": "缬草",
      "description": "缬草是一种多年生草本植物，以其镇静效果而闻名。它通常用于草药治疗失眠和焦虑。"
    },
    "ja": {
      "common_name": "バレリアン",
      "description": "バレリアンは鎮静効果で知られる多年草です。不眠症や不安の治療にハーブ療法としてよく使用されます。"
    },
    "ko": {
      "common_name": "발레리안",
      "description": "발레리안은 진정 효과로 잘 알려진 다년생 허브입니다. 불면증과 불안을 치료하기 위해 허브 의학에서 흔히 사용됩니다."
    }
  },
