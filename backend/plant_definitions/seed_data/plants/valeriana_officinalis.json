  "translations": {
    "en": {
      "common_name": "Valerian",
      "description": "Valeriana officinalis is a perennial herb native to Europe and parts of Asia. It prefers moist soil and partial sun and is traditionally grown outdoors."
    },
    "pl": {
      "common_name": "Kozłek lekarski",
      "description": "Valeriana officinalis to bylina pochodząca z Europy i Azji. Preferuje wilgotną glebę i półcień, zwykle uprawiana jest na zewnątrz."
    },
    "de": {
      "common_name": "Baldrian",
      "description": "Valeriana officinalis ist eine mehrjährige Pflanze aus Europa und Asien. Sie wächst am besten in feuchtem Boden und Halbschatten im Freien."
    },
    "fr": {
      "common_name": "Valériane",
      "description": "Valeriana officinalis est une plante vivace originaire d’Europe et d’Asie. Elle préfère un sol humide et une exposition mi-ombragée."
    },
    "it": {
      "common_name": "Valeriana",
      "description": "Valeriana officinalis è una pianta perenne originaria di Europa e Asia. Cresce bene in terreno umido e mezz'ombra, soprattutto all'aperto."
    },
    "es": {
      "common_name": "Valeriana",
      "description": "Valeriana officinalis es una planta perenne nativa de Europa y Asia. Prefiere suelos húmedos y semisombra en cultivo exterior."
    },
    "pt": {
      "common_name": "Valeriana",
      "description": "Valeriana officinalis é uma planta perene nativa da Europa e Ásia. Desenvolve-se melhor em solo úmido e meia-sombra ao ar livre."
    },
    "ar": {
      "common_name": "الناردين",
      "description": "نبات Valeriana officinalis عشبي معمر موطنه أوروبا وآسيا. يفضل التربة الرطبة والضوء الجزئي وينمو عادة في الخارج."
    },
    "hi": {
      "common_name": "वैलेरियन",
      "description": "Valeriana officinalis यूरोप और एशिया की एक बहुवर्षीय जड़ी-बूटी है। यह नम मिट्टी और आंशिक धूप में बाहर अच्छी तरह उगती है।"
    },
    "zh": {
      "common_name": "缬草",
      "description": "Valeriana officinalis 原产于欧洲和亚洲，是多年生草本植物。适合在湿润土壤和半日照的户外环境生长。"
    },
    "ja": {
      "common_name": "セイヨウカノコソウ",
      "description": "Valeriana officinalis はヨーロッパとアジア原産の多年草です。湿った土壌と半日陰の屋外環境を好みます。"
    },
    "ko": {
      "common_name": "서양쥐오줌풀",
      "description": "Valeriana officinalis는 유럽과 아시아 원산의 다년생 식물입니다. 습한 토양과 반그늘의 야외 환경을 선호합니다."
    }
  },
