  "translations": {
    "en": {
      "common_name": "Wallis' Adelonema",
      "description": "Adelonema wallisii is a tropical plant, native to Southeast Asia. It thrives in humid environments and prefers moderate light and watering."
    },
    "pl": {
      "common_name": "Adelonema Wallisii",
      "description": "Adelonema wallisii to roślina tropikalna pochodząca z Azji Południowo-Wschodniej. Wymaga wilgotnego środowiska i średniego nasłonecznienia oraz podlewania."
    },
    "de": {
      "common_name": "Wallis' Adelonema",
      "description": "Adelonema wallisii ist eine tropische Pflanze, die in Südostasien beheimatet ist. Sie gedeiht in feuchten Umgebungen und bevorzugt mittleres Licht und Bewässerung."
    },
    "fr": {
      "common_name": "Adelonéma de Wallis",
      "description": "Adelonema wallisii est une plante tropicale originaire du Sud-Est asiatique. Elle prospère dans des environnements humides et préfère une lumière et un arrosage modérés."
    },
    "it": {
      "common_name": "Adelonema Wallisii",
      "description": "Adelonema wallisii è una pianta tropicale originaria del Sud-Est asiatico. Cresce bene in ambienti umidi e preferisce luce e annaffiature moderate."
    },
    "es": {
      "common_name": "Adelonema de Wallis",
      "description": "Adelonema wallisii es una planta tropical originaria del sudeste asiático. Prefiere ambientes húmedos y requiere luz y riego moderados."
    },
    "pt": {
      "common_name": "Adelonema Wallisii",
      "description": "Adelonema wallisii é uma planta tropical originária do Sudeste Asiático. Ela cresce bem em ambientes úmidos e prefere luz e rega moderadas."
    },
    "ar": {
      "common_name": "أديلونما ووليسي",
      "description": "أديلونما ووليسي هي نبات استوائي من جنوب شرق آسيا. تنمو في بيئات رطبة وتفضل الضوء والري المعتدل."
    },
    "hi": {
      "common_name": "वॉलिस का एडेलोनिमा",
      "description": "एडेलोनिमा वालिसी एक उष्णकटिबंधीय पौधा है जो दक्षिण-पूर्व एशिया का मूल निवासी है। यह नम वातावरण में बढ़ता है और मध्यम प्रकाश और पानी देना पसंद करता है।"
    },
    "zh": {
      "common_name": "瓦利斯亚德罗内马",
      "description": "Adelonema wallisii 是一种原产于东南亚的热带植物。它喜欢湿润的环境，适合中等光照和浇水。"
    },
    "ja": {
      "common_name": "ワリスアデロネマ",
      "description": "Adelonema wallisii は東南アジア原産の熱帯植物で、湿気の多い環境を好み、中程度の光と水やりを好みます。"
    },
    "ko": {
      "common_name": "왈리스 아델로네마",
      "description": "Adelonema wallisii는 동남아시아 원산의 열대 식물입니다. 습한 환경에서 잘 자라며, 중간 정도의 빛과 물주기를 선호합니다."
    }
  },
