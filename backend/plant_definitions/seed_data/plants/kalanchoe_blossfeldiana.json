  "translations": {
    "en": {
      "common_name": "Flaming Katy",
      "description": "Kalanchoe blossfeldiana is a compact succulent known for long-lasting colorful flowers. It prefers bright light and infrequent watering."
    },
    "pl": {
      "common_name": "Kalanchoe",
      "description": "Kalanchoe blossfeldiana to sukulent o barwnych kwiatach. Wymaga jasnego stanowiska i rzadkiego podlewania."
    },
    "de": {
      "common_name": "Flammendes Käthchen",
      "description": "Kalanchoe blossfeldiana ist eine blühende Sukkulente mit dichter Form. Sie mag viel Licht und wenig Wasser."
    },
    "fr": {
      "common_name": "Kalanchoé",
      "description": "Kalanchoe blossfeldiana est une plante grasse à floraison durable. Elle aime la lumière vive et peu d’arrosage."
    },
    "it": {
      "common_name": "Calancola",
      "description": "Kalanchoe blossfeldiana è una succulenta ornamentale compatta. Richiede luce intensa e annaffiature moderate."
    },
    "es": {
      "common_name": "Calanchoe",
      "description": "Kalanchoe blossfeldiana es una suculenta de floración vistosa. Prefiere luz brillante y riego escaso."
    },
    "pt": {
      "common_name": "Calanchoê",
      "description": "Kalanchoe blossfeldiana é uma suculenta compacta e florífera. Gosta de luz intensa e pouca água."
    },
    "ar": {
      "common_name": "كالانشو",
      "description": "نبات كالانشو بلوسفيلديانا عصاري مزهر. يفضل الضوء الساطع والري القليل."
    },
    "hi": {
      "common_name": "कैलनकोई",
      "description": "कैलनकोई ब्लॉसफेल्डियाना एक फूलदार रसीला पौधा है। इसे तेज रोशनी और कम पानी चाहिए।"
    },
    "zh": {
      "common_name": "长寿花",
      "description": "长寿花是一种紧凑型多肉植物，花期长。喜欢明亮光照和少量浇水。"
    },
    "ja": {
      "common_name": "カランコエ",
      "description": "カランコエは花が長持ちする多肉植物です。明るい光と控えめな水やりを好みます。"
    },
    "ko": {
      "common_name": "칼랑코에",
      "description": "칼랑코에는 꽃이 오래가는 다육식물입니다. 밝은 빛과 적은 물을 선호합니다."
    }
  },
