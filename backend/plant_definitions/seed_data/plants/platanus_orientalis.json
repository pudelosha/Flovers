  "translations": {
    "en": {
      "common_name": "Oriental Plane",
      "description": "Platanus orientalis is a deciduous tree native to southeastern Europe and western Asia. It is commonly found in parks and along roadsides."
    },
    "pl": {
      "common_name": "Platany orientalny",
      "description": "Platanus orientalis to drzewo liściaste pochodzące z południowo-wschodniej Europy i zachodniej Azji. Jest powszechnie spotykane w parkach i wzdłuż dróg."
    },
    "de": {
      "common_name": "Orientalische Platane",
      "description": "Die Platanus orientalis ist ein Laubbaum, der ursprünglich aus Südosteuropa und Westasien stammt. Sie ist häufig in Parks und an Straßenrändern zu finden."
    },
    "fr": {
      "common_name": "Platane orientale",
      "description": "La Platanus orientalis est un arbre à feuilles caduques originaire du sud-est de l'Europe et de l'Asie occidentale. On la trouve couramment dans les parcs et le long des routes."
    },
    "it": {
      "common_name": "Platano orientale",
      "description": "Platanus orientalis è un albero deciduo originario dell'Europa sud-orientale e dell'Asia occidentale. Si trova comunemente nei parchi e lungo le strade."
    },
    "es": {
      "common_name": "Plátano oriental",
      "description": "Platanus orientalis es un árbol caducifolio originario del sureste de Europa y el oeste de Asia. Se encuentra comúnmente en parques y a lo largo de las carreteras."
    },
    "pt": {
      "common_name": "Plátano oriental",
      "description": "Platanus orientalis é uma árvore caducifólia nativa do sudeste da Europa e do oeste da Ásia. É comumente encontrada em parques e ao longo das estradas."
    },
    "ar": {
      "common_name": "بلوط شرقي",
      "description": "Platanus orientalis هو شجرة نفضية موطنها جنوب شرق أوروبا وغرب آسيا. توجد عادة في الحدائق وعلى جوانب الطرق."
    },
    "hi": {
      "common_name": "ओरिएंटल प्लेन",
      "description": "Platanus orientalis एक पर्णपाती पेड़ है जो दक्षिण-पूर्वी यूरोप और पश्चिमी एशिया का मूल निवासी है। इसे सामान्यतः पार्कों और सड़कों के किनारे पाया जाता है."
    },
    "zh": {
      "common_name": "东方悬铃木",
      "description": "Platanus orientalis 是一种落叶树，原产于东南欧和西亚。常见于公园和道路旁边。"
    },
    "ja": {
      "common_name": "オリエンタルプラタナス",
      "description": "Platanus orientalis は、南東ヨーロッパおよび西アジア原産の落葉樹です。公園や道路脇でよく見かけます。"
    },
    "ko": {
      "common_name": "동양 플라타너스",
      "description": "Platanus orientalis는 동남유럽과 서아시아 원산지인 낙엽수입니다. 공원과 도로변에서 자주 볼 수 있습니다."
    }
  },
