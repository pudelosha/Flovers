  "translations": {
    "en": {
      "common_name": "Impatiens",
      "description": "Impatiens walleriana is a popular flowering plant known for its bright, colorful blooms. It thrives in shaded areas with high humidity."
    },
    "pl": {
      "common_name": "Niecierpek",
      "description": "Impatiens walleriana to popularna roślina kwitnąca, znana ze swoich jasnych, kolorowych kwiatów. Dobrze rośnie w cieniu i w wilgotnym środowisku."
    },
    "de": {
      "common_name": "Fleißiges Lieschen",
      "description": "Impatiens walleriana ist eine beliebte Blühpflanze, die für ihre leuchtenden, bunten Blüten bekannt ist. Sie gedeiht in schattigen Bereichen mit hoher Luftfeuchtigkeit."
    },
    "fr": {
      "common_name": "Impatiens",
      "description": "Impatiens walleriana est une plante à fleurs populaire, connue pour ses fleurs colorées et lumineuses. Elle préfère les zones ombragées et l'humidité élevée."
    },
    "it": {
      "common_name": "Impatiens",
      "description": "Impatiens walleriana è una pianta da fiore popolare, nota per i suoi fiori vivaci e colorati. Cresce bene in zone ombreggiate e in ambienti umidi."
    },
    "es": {
      "common_name": "Impaciencia",
      "description": "Impatiens walleriana es una planta de flores popular, conocida por sus flores coloridas. Prefiere áreas sombreadas y alta humedad."
    },
    "pt": {
      "common_name": "Impatiens",
      "description": "Impatiens walleriana é uma planta de flores popular, conhecida pelas suas flores coloridas. Prefere áreas sombreadas e alta umidade."
    },
    "ar": {
      "common_name": "النعناع",
      "description": "Impatiens walleriana هي نبتة زهور شهيرة، معروفة بأزهارها الملونة. تفضل الأماكن المظللة والرطوبة العالية."
    },
    "hi": {
      "common_name": "इम्पेशन्स",
      "description": "Impatiens walleriana एक लोकप्रिय फूलों वाली पौधा है, जो अपने चमकीले रंग-बिरंगे फूलों के लिए जाना जाता है। यह छायादार क्षेत्रों और उच्च आर्द्रता में अच्छे से बढ़ता है।"
    },
    "zh": {
      "common_name": "凤仙花",
      "description": "Impatiens walleriana 是一种受欢迎的开花植物，因其明亮多彩的花朵而闻名。它喜欢阴凉的地方和高湿度环境。"
    },
    "ja": {
      "common_name": "インパチエンス",
      "description": "Impatiens walleriana は、鮮やかな色の花が特徴の人気のある開花植物です。日陰の場所と高い湿度の環境でよく育ちます。"
    },
    "ko": {
      "common_name": "임페이셔스",
      "description": "Impatiens walleriana는 화려한 색상의 꽃으로 유명한 인기 있는 꽃 식물입니다. 그늘에서 잘 자라며 높은 습도를 선호합니다."
    }
  },
