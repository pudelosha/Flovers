  "translations": {
    "en": {
      "common_name": "Jerusalem Cherry",
      "description": "The Jerusalem Cherry is a small, ornamental plant with vibrant red berries. It is grown mainly for its decorative value."
    },
    "pl": {
      "common_name": "Wiśnia Jerozolimska",
      "description": "Wiśnia Jerozolimska to mała roślina ozdobna o jaskrawoczerwonych jagodach. Jest uprawiana głównie ze względu na walory dekoracyjne."
    },
    "de": {
      "common_name": "Jerusalems-Kirsche",
      "description": "Die Jerusalems-Kirsche ist eine kleine Zierpflanze mit lebendig roten Beeren. Sie wird hauptsächlich wegen ihres dekorativen Wertes angebaut."
    },
    "fr": {
      "common_name": "Cerisier de Jérusalem",
      "description": "Le Cerisier de Jérusalem est une petite plante ornementale aux baies rouges vives. Elle est cultivée principalement pour sa valeur décorative."
    },
    "it": {
      "common_name": "Ciliegio di Gerusalemme",
      "description": "Il Ciliegio di Gerusalemme è una piccola pianta ornamentale con bacche rosse vivaci. Viene coltivata principalmente per il suo valore decorativo."
    },
    "es": {
      "common_name": "Cerezo de Jerusalén",
      "description": "El Cerezo de Jerusalén es una pequeña planta ornamental con bayas rojas brillantes. Se cultiva principalmente por su valor decorativo."
    },
    "pt": {
      "common_name": "Cerejeira de Jerusalém",
      "description": "A Cerejeira de Jerusalém é uma pequena planta ornamental com frutos vermelhos vibrantes. É cultivada principalmente pelo seu valor decorativo."
    },
    "ar": {
      "common_name": "شجرة الكرز في القدس",
      "description": "شجرة الكرز في القدس هي نبات زينة صغير مع توت أحمر لامع. تزرع أساساً لقيمتها الزخرفية."
    },
    "hi": {
      "common_name": "यरूशलेम चेरी",
      "description": "यरूशलेम चेरी एक छोटी, सजावटी पौधा है जिसमें चमकदार लाल बेर होते हैं। इसे मुख्य रूप से इसकी सजावटी कीमत के लिए उगाया जाता है।"
    },
    "zh": {
      "common_name": "耶路撒冷樱桃",
      "description": "耶路撒冷樱桃是一种小型观赏植物，具有鲜艳的红色浆果。它主要因其装饰价值而被种植。"
    },
    "ja": {
      "common_name": "エルサレムチェリー",
      "description": "エルサレムチェリーは、小さな観賞用植物で、鮮やかな赤い実をつけます。主に装飾的な価値のために栽培されます。"
    },
    "ko": {
      "common_name": "예루살렘 체리",
      "description": "예루살렘 체리는 작은 장식용 식물로, 선명한 빨간색 열매를 가지고 있습니다. 주로 장식적 가치 때문에 재배됩니다."
    }
  },
