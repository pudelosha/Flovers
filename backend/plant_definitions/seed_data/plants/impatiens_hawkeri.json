  "translations": {
    "en": {
      "common_name": "New Guinea Impatiens",
      "description": "Impatiens hawkeri is a flowering plant native to New Guinea. It is valued for its colorful blooms and glossy leaves. It grows well indoors with bright indirect light."
    },
    "pl": {
      "common_name": "Niecierpek nowogwinejski",
      "description": "Impatiens hawkeri pochodzi z Nowej Gwinei. Ceniony jest za barwne kwiaty i błyszczące liście. Dobrze rośnie w jasnych wnętrzach."
    },
    "de": {
      "common_name": "Neuguinea-Fleißiges Lieschen",
      "description": "Impatiens hawkeri stammt aus Neuguinea. Die Pflanze hat auffällige Blüten und glänzende Blätter. Sie eignet sich gut für helle Innenräume."
    },
    "fr": {
      "common_name": "Impatiens de Nouvelle-Guinée",
      "description": "Impatiens hawkeri est originaire de Nouvelle-Guinée. Il est apprécié pour ses fleurs colorées. Il pousse bien en intérieur lumineux."
    },
    "it": {
      "common_name": "Impatiens della Nuova Guinea",
      "description": "Impatiens hawkeri proviene dalla Nuova Guinea. È apprezzata per i suoi fiori vivaci. Si adatta bene agli ambienti interni luminosi."
    },
    "es": {
      "common_name": "Impatiens de Nueva Guinea",
      "description": "Impatiens hawkeri es originaria de Nueva Guinea. Destaca por sus flores coloridas. Crece bien en interiores luminosos."
    },
    "pt": {
      "common_name": "Impatiens-da-Nova-Guiné",
      "description": "Impatiens hawkeri é nativa da Nova Guiné. É valorizada pelas flores vistosas. Desenvolve-se bem em interiores claros."
    },
    "ar": {
      "common_name": "إمباتينس غينيا الجديدة",
      "description": "نبات Impatiens hawkeri موطنه غينيا الجديدة. يتميز بأزهاره الملونة. مناسب للزراعة داخل المنزل."
    },
    "hi": {
      "common_name": "न्यू गिनी इम्पेशंस",
      "description": "Impatiens hawkeri न्यू गिनी का पौधा है। इसके फूल रंगीन होते हैं। यह घर के अंदर अच्छी तरह बढ़ता है।"
    },
    "zh": {
      "common_name": "新几内亚凤仙花",
      "description": "Impatiens hawkeri 原产于新几内亚。以鲜艳花朵著称。适合明亮的室内环境。"
    },
    "ja": {
      "common_name": "ニューギニアインパチェンス",
      "description": "Impatiens hawkeri はニューギニア原産です。鮮やかな花が特徴です。明るい室内で育ちます。"
    },
    "ko": {
      "common_name": "뉴기니아 임파첸스",
      "description": "Impatiens hawkeri는 뉴기니아 원산입니다. 화려한 꽃이 특징입니다. 실내 재배에 적합합니다."
    }
  },
