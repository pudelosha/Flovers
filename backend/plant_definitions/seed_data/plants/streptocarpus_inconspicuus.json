  "translations": {
    "en": {
      "common_name": "Uluguru African Violet",
      "description": "A rare African violet species native to the Uluguru Mountains of Tanzania. It prefers bright, indirect light and moderate watering, avoiding waterlogged soil."
    },
    "pl": {
      "common_name": "Fiołek afrykański Uluguru",
      "description": "Rzadki fiołek afrykański z gór Uluguru w Tanzanii. Woli jasne, pośrednie światło i umiarkowane podlewanie."
    },
    "de": {
      "common_name": "Uluguru Afrikanisches Veilchen",
      "description": "Seltene afrikanische Veilchenart aus den Uluguru-Bergen in Tansania. Bevorzugt indirektes Licht und mäßiges Gießen."
    },
    "fr": {
      "common_name": "Violette d'Afrique d'Uluguru",
      "description": "Espèce rare de violette d'Afrique des montagnes Uluguru en Tanzanie. Elle aime la lumière indirecte et un arrosage modéré."
    },
    "it": {
      "common_name": "Violetta Africana di Uluguru",
      "description": "Specie rara di violetta africana delle montagne Uluguru in Tanzania. Preferisce luce indiretta e annaffiature moderate."
    },
    "es": {
      "common_name": "Violeta Africana Uluguru",
      "description": "Especie rara de violeta africana de los Montes Uluguru en Tanzania. Prefiere luz indirecta y riego moderado."
    },
    "pt": {
      "common_name": "Violeta Africana de Uluguru",
      "description": "Espécie rara de violeta africana das Montanhas Uluguru na Tanzânia. Prefere luz indireta e rega moderada."
    },
    "ar": {
      "common_name": "بنفسج أفريقي أولوجورو",
      "description": "نوع نادر من البنفسج الأفريقي من جبال أولوجورو في تنزانيا. يفضل ضوء غير مباشر وري معتدل."
    },
    "hi": {
      "common_name": "उलुगुरू अफ्रीकी वायलेट",
      "description": "तंज़ानिया के उलुगुरू पहाड़ों का दुर्लभ अफ़्रीकी वायलेट. अप्रत्यक्ष प्रकाश और मध्यम पानी पसंद करता है."
    },
    "zh": {
      "common_name": "乌卢古鲁非洲紫罗兰",
      "description": "坦桑尼亚乌卢古鲁山脉的稀有非洲紫罗兰。喜欢明亮散射光和适量浇水。"
    },
    "ja": {
      "common_name": "ウルグル非洲スミレ",
      "description": "タンザニアのウルグル山地に自生する希少なアフリカスミレ。明るい間接光と適度な水やりを好みます。"
    },
    "ko": {
      "common_name": "울루구루 아프리카 제비꽃",
      "description": "탄자니아 울루구루산의 희귀 아프리카 제비꽃. 밝은 간접광과 적당한 물주기를 선호합니다."
    }
  },
