  "translations": {
    "en": {
      "common_name": "Tree Aeonium",
      "description": "Aeonium arboreum is a woody succulent native to the Canary Islands. It forms rosettes on branching stems and prefers bright sun and dry soil."
    },
    "pl": {
      "common_name": "Aeonium drzewiaste",
      "description": "Aeonium arboreum to sukulent z Wysp Kanaryjskich. Tworzy rozety na zdrewniałych pędach i wymaga dużo światła oraz rzadkiego podlewania."
    },
    "de": {
      "common_name": "Baum-Aeonium",
      "description": "Aeonium arboreum ist eine sukkulente Pflanze von den Kanaren. Sie bildet Rosetten auf verzweigten Stämmen und braucht viel Sonne."
    },
    "fr": {
      "common_name": "Aeonium arborescent",
      "description": "Aeonium arboreum est une plante succulente originaire des Canaries. Elle forme des rosettes sur des tiges ligneuses et aime le soleil."
    },
    "it": {
      "common_name": "Aeonium arboreo",
      "description": "Aeonium arboreum è una succulenta delle Isole Canarie. Cresce con rosette su fusti legnosi e richiede luce intensa."
    },
    "es": {
      "common_name": "Aeonio arbóreo",
      "description": "Aeonium arboreum es una suculenta originaria de Canarias. Forma rosetas en tallos leñosos y necesita sol abundante."
    },
    "pt": {
      "common_name": "Aeônio-arbóreo",
      "description": "Aeonium arboreum é uma suculenta das Ilhas Canárias. Desenvolve rosetas em caules lenhosos e prefere sol intenso."
    },
    "ar": {
      "common_name": "إيونيوم شجري",
      "description": "نبات عصاري موطنه جزر الكناري. ينمو على سيقان خشبية ويحتاج إلى ضوء شمس قوي وتربة جافة."
    },
    "hi": {
      "common_name": "ट्री ऐओनियम",
      "description": "Aeonium arboreum कैनरी द्वीप का एक रसीला पौधा है। यह लकड़ी जैसे तनों पर रोसेट बनाता है और तेज धूप पसंद करता है।"
    },
    "zh": {
      "common_name": "树状莲花掌",
      "description": "Aeonium arboreum 原产加那利群岛。它在木质茎上形成莲座状叶丛，喜强光和干燥土壤。"
    },
    "ja": {
      "common_name": "クロホウシ",
      "description": "Aeonium arboreum はカナリア諸島原産の多肉植物です。木質の茎にロゼットを形成し、強い光を好みます。"
    },
    "ko": {
      "common_name": "나무아에오니움",
      "description": "Aeonium arboreum은 카나리아 제도 원산의 다육식물입니다. 목질 줄기에 로제트를 형성하며 밝은 햇빛을 선호합니다."
    }
  },
