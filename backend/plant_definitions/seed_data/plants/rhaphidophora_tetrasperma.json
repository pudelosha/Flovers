  "translations": {
    "en": {
      "common_name": "Mini Monstera",
      "description": "Rhaphidophora tetrasperma is a small, tropical climbing plant. It is known for its fenestrated, glossy leaves and compact growth habit."
    },
    "pl": {
      "common_name": "Mini Monstera",
      "description": "Rhaphidophora tetrasperma to mała tropikalna roślina pnąca. Charakteryzuje się szczelinowanymi, błyszczącymi liśćmi i zwartym pokrojem."
    },
    "de": {
      "common_name": "Mini Monstera",
      "description": "Rhaphidophora tetrasperma ist eine kleine, tropische Kletterpflanze. Sie zeichnet sich durch ihre fenestrierten, glänzenden Blätter und kompakte Wuchsform aus."
    },
    "fr": {
      "common_name": "Mini Monstera",
      "description": "Rhaphidophora tetrasperma est une petite plante grimpante tropicale. Elle est connue pour ses feuilles fenestrées et brillantes, ainsi que sa croissance compacte."
    },
    "it": {
      "common_name": "Mini Monstera",
      "description": "Rhaphidophora tetrasperma è una piccola pianta rampicante tropicale. È conosciuta per le sue foglie fenestrate e lucide, e la sua crescita compatta."
    },
    "es": {
      "common_name": "Mini Monstera",
      "description": "Rhaphidophora tetrasperma es una pequeña planta trepadora tropical. Es conocida por sus hojas fenestradas y brillantes, y su hábito de crecimiento compacto."
    },
    "pt": {
      "common_name": "Mini Monstera",
      "description": "Rhaphidophora tetrasperma é uma pequena planta trepadeira tropical. É conhecida por suas folhas fenestradas e brilhantes, e seu crescimento compacto."
    },
    "ar": {
      "common_name": "ميني مونستيرا",
      "description": "Rhaphidophora tetrasperma هي نبات متسلق صغير استوائي. تشتهر بأوراقها المتشققة اللامعة وموضع نموها المدمج."
    },
    "hi": {
      "common_name": "मिनी मॉन्स्टेरा",
      "description": "Rhaphidophora tetrasperma एक छोटी, उष्णकटिबंधीय चढ़ाई करने वाली पौधा है। यह अपनी दरार वाली, चमकदार पत्तियों और कॉम्पैक्ट वृद्धि के लिए प्रसिद्ध है।"
    },
    "zh": {
      "common_name": "迷你怪兽",
      "description": "Rhaphidophora tetrasperma 是一种小型热带藤本植物，因其有裂纹的光泽叶子和紧凑的生长习性而闻名。"
    },
    "ja": {
      "common_name": "ミニモンステラ",
      "description": "Rhaphidophora tetraspermaは小さな熱帯のつる植物です。裂けた光沢のある葉とコンパクトな成長習性で知られています。"
    },
    "ko": {
      "common_name": "미니 몬스테라",
      "description": "Rhaphidophora tetrasperma는 작은 열대성 덩굴식물입니다. 찢어진 광택 있는 잎과 컴팩트한 성장 습성으로 잘 알려져 있습니다."
    }
  },
