  "translations": {
    "en": {
      "common_name": "Man Orchid",
      "description": "The Man Orchid is a unique plant with human-shaped flowers. It thrives in temperate regions, typically found in grassy meadows and woodlands."
    },
    "pl": {
      "common_name": "Orkidea ludzka",
      "description": "Orkidea ludzka to unikalna roślina o kwiatach w kształcie ludzkich postaci. Rośnie w umiarkowanych regionach, głównie na łąkach i w lasach."
    },
    "de": {
      "common_name": "Mannorchidee",
      "description": "Die Mannorchidee ist eine einzigartige Pflanze mit menschenähnlichen Blüten. Sie wächst in gemäßigten Regionen, hauptsächlich auf Wiesen und in Wäldern."
    },
    "fr": {
      "common_name": "Orchidée homme",
      "description": "L'Orchidée homme est une plante unique avec des fleurs en forme d'humain. Elle prospère dans les régions tempérées, souvent trouvée dans les prairies et les forêts."
    },
    "it": {
      "common_name": "Orchidea uomo",
      "description": "L'Orchidea uomo è una pianta unica con fiori a forma umana. Cresce nelle regioni temperate, solitamente in prati e boschi."
    },
    "es": {
      "common_name": "Orquídea hombre",
      "description": "La Orquídea hombre es una planta única con flores en forma de figura humana. Crece en regiones templadas, comúnmente encontrada en praderas y bosques."
    },
    "pt": {
      "common_name": "Orquídea homem",
      "description": "A Orquídea homem é uma planta única com flores em forma humana. Cresce em regiões temperadas, sendo encontrada principalmente em pradarias e bosques."
    },
    "ar": {
      "common_name": "زهرة الأوركيد البشرية",
      "description": "زهرة الأوركيد البشرية هي نبات فريد من نوعه بزهور على شكل إنسان. تنمو في المناطق المعتدلة، وتوجد عادة في المروج والغابات."
    },
    "hi": {
      "common_name": "मान ओर्किड",
      "description": "मान ओर्किड एक अनोखा पौधा है, जिसकी फूल मानव के आकार के होते हैं। यह समशीतोष्ण क्षेत्रों में उगता है, आमतौर पर घास के मैदानों और जंगलों में पाया जाता है।"
    },
    "zh": {
      "common_name": "人形兰",
      "description": "人形兰是一种独特的植物，具有类人形的花朵。它生长在温带地区，通常出现在草地和森林中。"
    },
    "ja": {
      "common_name": "人形ラン",
      "description": "人形ランは人間の形をした花を持つ独特な植物です。温帯地域に生息し、主に草地や森林で見られます。"
    },
    "ko": {
      "common_name": "인간 모양 난초",
      "description": "인간 모양 난초는 인간 모양의 꽃을 가진 독특한 식물입니다. 온대 지역에서 자생하며 주로 풀밭과 숲에서 자주 발견됩니다."
    }
  },
