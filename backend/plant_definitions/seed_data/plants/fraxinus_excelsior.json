  "translations": {
    "en": {
      "common_name": "European Ash",
      "description": "Fraxinus excelsior is a large deciduous tree native to Europe. It grows in moist, fertile soils and open woodlands. The species is valued for timber and ecological importance."
    },
    "pl": {
      "common_name": "Jesion wyniosły",
      "description": "Fraxinus excelsior to duże drzewo liściaste pochodzące z Europy. Występuje na wilgotnych, żyznych glebach. Jest ważne dla ekosystemów leśnych."
    },
    "de": {
      "common_name": "Gemeine Esche",
      "description": "Fraxinus excelsior ist ein großer Laubbaum Europas. Er wächst auf feuchten, nährstoffreichen Böden. Die Art ist ökologisch und wirtschaftlich bedeutend."
    },
    "fr": {
      "common_name": "Frêne élevé",
      "description": "Fraxinus excelsior est un grand arbre caduc d’Europe. Il préfère les sols fertiles et humides. Il joue un rôle écologique important."
    },
    "it": {
      "common_name": "Frassino maggiore",
      "description": "Fraxinus excelsior è un grande albero deciduo europeo. Cresce in terreni fertili e umidi. È importante dal punto di vista ecologico."
    },
    "es": {
      "common_name": "Fresno común",
      "description": "Fraxinus excelsior es un árbol caducifolio grande de Europa. Prefiere suelos húmedos y fértiles. Tiene gran valor ecológico."
    },
    "pt": {
      "common_name": "Freixo-europeu",
      "description": "Fraxinus excelsior é uma grande árvore caducifólia da Europa. Desenvolve-se em solos húmidos e férteis. É ecologicamente importante."
    },
    "ar": {
      "common_name": "الدردار الأوروبي",
      "description": "Fraxinus excelsior شجرة كبيرة متساقطة الأوراق موطنها أوروبا. تنمو في تربة رطبة وخصبة. لها أهمية بيئية كبيرة."
    },
    "hi": {
      "common_name": "यूरोपीय ऐश",
      "description": "Fraxinus excelsior यूरोप का एक बड़ा पर्णपाती वृक्ष है। यह नम और उपजाऊ मिट्टी में उगता है। इसका पारिस्थितिक महत्व है।"
    },
    "zh": {
      "common_name": "欧洲白蜡树",
      "description": "Fraxinus excelsior 是原产于欧洲的大型落叶乔木。适合在湿润肥沃的土壤中生长。具有重要生态价值。"
    },
    "ja": {
      "common_name": "セイヨウトネリコ",
      "description": "Fraxinus excelsior はヨーロッパ原産の大型落葉樹です。湿った肥沃な土壌を好みます。生態系で重要です。"
    },
    "ko": {
      "common_name": "유럽물푸레나무",
      "description": "Fraxinus excelsior는 유럽 원산의 큰 낙엽수입니다. 습하고 비옥한 토양에서 잘 자랍니다. 생태적으로 중요합니다."
    }
  },
