  "translations": {
    "en": {
      "common_name": "Black Locust",
      "description": "The Black Locust is a fast-growing tree native to North America. It is known for its fragrant white flowers and thorny branches."
    },
    "pl": {
      "common_name": "Robinia akacjowa",
      "description": "Robinia akacjowa to szybko rosnące drzewo rodem z Ameryki Północnej. Znane jest z pachnących białych kwiatów i kolczastych gałęzi."
    },
    "de": {
      "common_name": "Robinie",
      "description": "Die Robinie ist ein schnell wachsender Baum, der in Nordamerika beheimatet ist. Sie ist für ihre duftenden weißen Blüten und dornigen Äste bekannt."
    },
    "fr": {
      "common_name": "Robinier faux-acacia",
      "description": "Le robinier faux-acacia est un arbre à croissance rapide originaire d'Amérique du Nord. Il est connu pour ses fleurs blanches parfumées et ses branches épineuses."
    },
    "it": {
      "common_name": "Robinia",
      "description": "La Robinia è un albero a crescita rapida originario del Nord America. È nota per i suoi fiori bianchi profumati e i rami spinosi."
    },
    "es": {
      "common_name": "Robinia falsa acacia",
      "description": "La Robinia es un árbol de rápido crecimiento originario de América del Norte. Es conocida por sus flores blancas fragantes y sus ramas espinosas."
    },
    "pt": {
      "common_name": "Falsa acácia",
      "description": "A falsa acácia é uma árvore de crescimento rápido originária da América do Norte. É conhecida pelas suas flores brancas perfumadas e ramos espinhosos."
    },
    "ar": {
      "common_name": "روبينيه",
      "description": "روبينيه هو شجرة سريعة النمو موطنها أمريكا الشمالية. تعرف بأزهارها البيضاء العطرة وفروعها الشائكة."
    },
    "hi": {
      "common_name": "ब्लैक लोकेस्ट",
      "description": "ब्लैक लोकेस्ट एक तेज़ी से बढ़ने वाली पेड़ है जो उत्तरी अमेरिका की मूल निवासी है। यह अपनी सुगंधित सफेद फूलों और कांटेदार शाखाओं के लिए प्रसिद्ध है।"
    },
    "zh": {
      "common_name": "黑槐",
      "description": "黑槐是一种快速生长的树木，原产于北美。它以其芳香的白色花朵和多刺的枝条而闻名。"
    },
    "ja": {
      "common_name": "ブラックロカスト",
      "description": "ブラックロカストは北アメリカ原産の速成木です。香り高い白い花とトゲのある枝で知られています。"
    },
    "ko": {
      "common_name": "블랙 로커스트",
      "description": "블랙 로커스트는 북미 원산지의 빠르게 자라는 나무입니다. 향기로운 흰 꽃과 가시가 많은 가지로 유명합니다."
    }
  },
