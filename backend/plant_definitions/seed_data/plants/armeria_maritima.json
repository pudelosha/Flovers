  "translations": {
    "en": {
      "common_name": "Sea thrift",
      "description": "Armeria maritima is a hardy perennial native to coastal regions. It forms dense grassy clumps with pink flower heads and tolerates wind and salt."
    },
    "pl": {
      "common_name": "Zawciąg nadmorski",
      "description": "Armeria maritima to wytrzymała bylina z wybrzeży morskich. Tworzy zwarte kępy i dobrze znosi wiatr oraz zasolenie."
    },
    "de": {
      "common_name": "Grasnelke",
      "description": "Armeria maritima ist eine robuste Küstenstaude. Sie bildet dichte Horste und verträgt Wind sowie salzhaltige Luft."
    },
    "fr": {
      "common_name": "Armérie maritime",
      "description": "Armeria maritima est une plante vivace des zones côtières. Elle forme des touffes compactes et supporte le vent et le sel."
    },
    "it": {
      "common_name": "Armeria marittima",
      "description": "Armeria maritima è una perenne rustica delle coste. Cresce in ciuffi compatti e resiste al vento e alla salsedine."
    },
    "es": {
      "common_name": "Clavelina de mar",
      "description": "Armeria maritima es una planta perenne costera muy resistente. Forma matas densas y tolera el viento y la sal."
    },
    "pt": {
      "common_name": "Armeria-marítima",
      "description": "Armeria maritima é uma planta perene costeira resistente. Forma tufos densos e tolera vento e salinidade."
    },
    "ar": {
      "common_name": "أرميريا بحرية",
      "description": "نبات أرميريا ماريتيما معمر من المناطق الساحلية. يشكل كتلًا كثيفة ويتحمل الرياح والملوحة."
    },
    "hi": {
      "common_name": "सी थ्रिफ्ट",
      "description": "Armeria maritima एक तटीय बहुवर्षीय पौधा है। यह घनी गुच्छेदार वृद्धि करता है और हवा व लवणता सहन करता है।"
    },
    "zh": {
      "common_name": "海滨石竹",
      "description": "Armeria maritima 是一种原产于海岸的多年生植物。它形成紧密簇生，耐风耐盐。"
    },
    "ja": {
      "common_name": "ハマカンザシ",
      "description": "Armeria maritima は海岸原産の多年草です。密な株を作り、風や塩分に強いです。"
    },
    "ko": {
      "common_name": "갯잔디석죽",
      "description": "Armeria maritima는 해안 지역에 자라는 다년생 식물입니다. 바람과 염분에 강합니다."
    }
  },
