  "translations": {
    "en": {
      "common_name": "Avocado",
      "description": "Avocado is a tropical fruit plant known for its creamy texture. It thrives in medium sunlight and requires moderate watering."
    },
    "pl": {
      "common_name": "Awokado",
      "description": "Awokado to roślina tropikalna znana ze swojej kremowej konsystencji. Rośnie w średnim świetle i wymaga umiarkowanego podlewania."
    },
    "de": {
      "common_name": "Avocado",
      "description": "Avocado ist eine tropische Fruchtpflanze, die für ihre cremige Textur bekannt ist. Sie gedeiht bei mittlerem Sonnenlicht und benötigt mäßiges Gießen."
    },
    "fr": {
      "common_name": "Avocat",
      "description": "L'avocat est une plante fruitière tropicale connue pour sa texture crémeuse. Il prospère sous une lumière modérée et nécessite un arrosage modéré."
    },
    "it": {
      "common_name": "Avocado",
      "description": "L'avocado è una pianta tropicale conosciuta per la sua consistenza cremosa. Cresce in luce media e richiede un'annaffiatura moderata."
    },
    "es": {
      "common_name": "Aguacate",
      "description": "El aguacate es una planta tropical conocida por su textura cremosa. Crece con luz media y necesita un riego moderado."
    },
    "pt": {
      "common_name": "Abacate",
      "description": "O abacate é uma planta tropical conhecida por sua textura cremosa. Ela prospera com luz média e necessita de rega moderada."
    },
    "ar": {
      "common_name": "أفوكادو",
      "description": "الأفوكادو هو نبات استوائي معروف بقوامه الكريمي. ينمو تحت ضوء متوسط ويحتاج إلى الري المعتدل."
    },
    "hi": {
      "common_name": "एवोकाडो",
      "description": "एवोकाडो एक उष्णकटिबंधीय फल का पौधा है, जो अपनी मलाईदार बनावट के लिए जाना जाता है। यह मध्यम धूप में पनपता है और मध्यम पानी की आवश्यकता होती है।"
    },
    "zh": {
      "common_name": "鳄梨",
      "description": "鳄梨是一种热带水果植物，以其奶油般的质地而闻名。它在中等阳光下生长，需要适量的浇水。"
    },
    "ja": {
      "common_name": "アボカド",
      "description": "アボカドはクリーミーな食感で知られる熱帯果物の植物です。中くらいの日光で育ち、適度な水やりが必要です。"
    },
    "ko": {
      "common_name": "아보카도",
      "description": "아보카도는 크리미한 질감으로 잘 알려진 열대 과일 식물입니다. 중간 햇빛에서 자라며 적당한 물을 필요로 합니다."
    }
  },
