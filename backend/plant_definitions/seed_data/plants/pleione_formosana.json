  "translations": {
    "en": {
      "common_name": "Formosan Pleione",
      "description": "Pleione formosana is a type of orchid native to Taiwan. It thrives in medium light with high humidity."
    },
    "pl": {
      "common_name": "Pleione formosana",
      "description": "Pleione formosana to rodzaj storczyka pochodzącego z Tajwanu. Rośnie najlepiej przy średnim świetle i wysokiej wilgotności."
    },
    "de": {
      "common_name": "Formosanische Pleione",
      "description": "Pleione formosana ist eine Orchidee aus Taiwan. Sie gedeiht bei mittlerem Licht und hoher Luftfeuchtigkeit."
    },
    "fr": {
      "common_name": "Pleione formosana",
      "description": "Pleione formosana est une orchidée originaire de Taïwan. Elle pousse bien avec une lumière modérée et une humidité élevée."
    },
    "it": {
      "common_name": "Pleione formosana",
      "description": "Pleione formosana è un'orchidea originaria di Taiwan. Cresce bene con luce media e umidità elevata."
    },
    "es": {
      "common_name": "Pleione formosana",
      "description": "Pleione formosana es una orquídea originaria de Taiwán. Crece bien con luz media y alta humedad."
    },
    "pt": {
      "common_name": "Pleione formosana",
      "description": "Pleione formosana é uma orquídea nativa de Taiwan. Cresce bem com luz média e alta umidade."
    },
    "ar": {
      "common_name": "Pleione formosana",
      "description": "Pleione formosana هي نوع من الأوركيد موطنها تايوان. تنمو بشكل جيد تحت الضوء المتوسط والرطوبة العالية."
    },
    "hi": {
      "common_name": "Pleione formosana",
      "description": "Pleione formosana ताइवान का एक ऑर्किड है। यह मध्यम रोशनी और उच्च आर्द्रता में अच्छी तरह से बढ़ता है।"
    },
    "zh": {
      "common_name": "福尔摩沙的Pleione",
      "description": "Pleione formosana 是一种原产于台湾的兰花。它在中等光照和高湿度下生长良好。"
    },
    "ja": {
      "common_name": "フォルモサ・プレイオネ",
      "description": "Pleione formosana は台湾原産のランの一種です。中程度の光と高湿度で良く育ちます。"
    },
    "ko": {
      "common_name": "포모사 플레이오네",
      "description": "Pleione formosana는 대만 원산의 난초입니다. 중간 정도의 빛과 높은 습도에서 잘 자랍니다."
    }
  },
