  "translations": {
    "en": {
      "common_name": "Wood avens",
      "description": "Geum urbanum is a hardy perennial native to Europe, commonly found in woodlands and hedgerows. It produces small yellow flowers and thrives in partial shade with moist soil."
    },
    "pl": {
      "common_name": "Kuklik pospolity",
      "description": "Geum urbanum to bylina występująca naturalnie w lasach i zaroślach Europy. Ma drobne żółte kwiaty i najlepiej rośnie w półcieniu na wilgotnej glebie."
    },
    "de": {
      "common_name": "Echte Nelkenwurz",
      "description": "Geum urbanum ist eine ausdauernde Pflanze aus europäischen Wäldern und Hecken. Sie bevorzugt halbschattige Standorte mit feuchtem Boden."
    },
    "fr": {
      "common_name": "Benoîte commune",
      "description": "Geum urbanum est une plante vivace originaire d'Europe, fréquente dans les sous-bois. Elle apprécie les sols frais et une exposition mi-ombragée."
    },
    "it": {
      "common_name": "Cariofillata comune",
      "description": "Geum urbanum è una pianta perenne europea che cresce nei boschi e lungo le siepi. Predilige terreni umidi e posizioni di mezz'ombra."
    },
    "es": {
      "common_name": "Benoíta común",
      "description": "Geum urbanum es una planta perenne europea típica de bosques y setos. Crece mejor en suelos húmedos con sombra parcial."
    },
    "pt": {
      "common_name": "Benoíte-comum",
      "description": "Geum urbanum é uma planta perene nativa da Europa, comum em áreas florestais. Desenvolve-se bem em solo úmido e meia-sombra."
    },
    "ar": {
      "common_name": "جذر القرنفل البري",
      "description": "Geum urbanum نبات معمر أوروبي يوجد في الغابات والأحراج. يفضل التربة الرطبة والضوء الجزئي."
    },
    "hi": {
      "common_name": "वुड एवेंस",
      "description": "Geum urbanum यूरोप का एक बहुवर्षीय पौधा है जो जंगलों में पाया जाता है। यह आंशिक धूप और नम मिट्टी में अच्छी तरह बढ़ता है।"
    },
    "zh": {
      "common_name": "林地金莲花",
      "description": "Geum urbanum 原产于欧洲，常见于林地环境。它喜欢湿润土壤和半阴条件。"
    },
    "ja": {
      "common_name": "ダイコンソウ",
      "description": "Geum urbanum はヨーロッパ原産の多年草で、林床に自生します。湿った土と半日陰を好みます。"
    },
    "ko": {
      "common_name": "우드아벤스",
      "description": "Geum urbanum은 유럽 원산의 다년생 식물로 숲속에서 자랍니다. 반그늘과 촉촉한 토양을 선호합니다."
    }
  },
