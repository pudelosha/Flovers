  "translations": {
    "en": {
      "common_name": "Bishop's Cap Cactus",
      "description": "A small, spherical cactus with a woolly appearance. It is native to Mexico and requires minimal care."
    },
    "pl": {
      "common_name": "Kaktus Biskupa",
      "description": "Mały, kulisty kaktus o wełnistej powierzchni. Pochodzi z Meksyku i wymaga minimalnej opieki."
    },
    "de": {
      "common_name": "Bischofsmütze",
      "description": "Ein kleiner, kugelförmiger Kaktus mit wolliger Erscheinung. Er stammt aus Mexiko und benötigt nur wenig Pflege."
    },
    "fr": {
      "common_name": "Cactus Capuchon",
      "description": "Un petit cactus sphérique à l'apparence laineuse. Il est originaire du Mexique et nécessite peu de soins."
    },
    "it": {
      "common_name": "Cactus Cappuccio",
      "description": "Un piccolo cactus sferico con un aspetto lanuginoso. È originario del Messico e richiede poca cura."
    },
    "es": {
      "common_name": "Cactus de Capucho",
      "description": "Un pequeño cactus esférico con una apariencia lanosa. Es originario de México y requiere pocos cuidados."
    },
    "pt": {
      "common_name": "Cacto Capucho",
      "description": "Um pequeno cacto esférico com aparência lanosa. É originário do México e exige poucos cuidados."
    },
    "ar": {
      "common_name": "صبار قبعة الأسقف",
      "description": "صبار صغير كروي ذو مظهر صوفي. هو من أصل مكسيكي ويتطلب رعاية قليلة."
    },
    "hi": {
      "common_name": "बिशप्स कैप कैक्टस",
      "description": "एक छोटा, गोलाकार कैक्टस जिसमें ऊनी रूप है। यह मेक्सिको का मूल निवासी है और कम देखभाल की आवश्यकता होती है।"
    },
    "zh": {
      "common_name": "主教帽仙人掌",
      "description": "一种小型球形仙人掌，具有羊毛般的外观。它原产于墨西哥，且需要很少的护理。"
    },
    "ja": {
      "common_name": "ビショップキャップサボテン",
      "description": "小さな球形のサボテンで、毛羽立った外観を持っています。メキシコが原産で、手間のかからない植物です。"
    },
    "ko": {
      "common_name": "주교모자 선인장",
      "description": "작고 구형의 선인장으로 털처럼 보입니다. 멕시코 원산지이며 적은 관리가 필요합니다."
    }
  },
