  "translations": {
    "en": {
      "common_name": "Zebra Alocasia",
      "description": "Alocasia zebrina is a tropical aroid known for its striped stems and large arrow-shaped leaves. It prefers bright indirect light, evenly moist soil, and high humidity indoors."
    },
    "pl": {
      "common_name": "Alokazja zebra",
      "description": "Alocasia zebrina to tropikalna roślina o pasiastych ogonkach i dużych liściach. Wymaga jasnego światła pośredniego, wilgotnej gleby i wysokiej wilgotności."
    },
    "de": {
      "common_name": "Zebra-Alokasie",
      "description": "Alocasia zebrina ist eine tropische Blattpflanze mit gestreiften Stielen. Sie mag helles indirektes Licht, gleichmäßig feuchte Erde und hohe Luftfeuchtigkeit."
    },
    "fr": {
      "common_name": "Alocasia zébrée",
      "description": "Alocasia zebrina est une plante tropicale aux tiges rayées et aux grandes feuilles. Elle apprécie une lumière indirecte, un sol humide et une forte humidité."
    },
    "it": {
      "common_name": "Alocasia zebra",
      "description": "Alocasia zebrina è una pianta tropicale con steli striati decorativi. Cresce bene con luce indiretta, terreno umido e alta umidità."
    },
    "es": {
      "common_name": "Alocasia cebra",
      "description": "Alocasia zebrina es una planta tropical con tallos rayados. Necesita luz indirecta brillante, riego regular y humedad alta."
    },
    "pt": {
      "common_name": "Alocasia zebra",
      "description": "Alocasia zebrina é uma planta tropical com hastes listradas. Prefere luz indireta, solo sempre levemente úmido e alta umidade."
    },
    "ar": {
      "common_name": "ألوكاسيا زيبرا",
      "description": "الألوكاسيا زيبرا نبات استوائي بسيقان مخططة وأوراق كبيرة. يحتاج إلى ضوء غير مباشر ورطوبة عالية وتربة رطبة."
    },
    "hi": {
      "common_name": "ज़ेब्रा अलोकेशिया",
      "description": "Alocasia zebrina एक उष्णकटिबंधीय पौधा है जिसकी डंठल धारियों वाली होती हैं। इसे अप्रत्यक्ष रोशनी, नमी और आर्द्रता की आवश्यकता होती है।"
    },
    "zh": {
      "common_name": "斑马海芋",
      "description": "Alocasia zebrina 是一种热带观叶植物，以条纹茎闻名。适合明亮散射光、湿润土壤和高湿度环境。"
    },
    "ja": {
      "common_name": "ゼブラアロカシア",
      "description": "アロカシア・ゼブリナは縞模様の茎が特徴の熱帯植物です。明るい間接光と湿度の高い環境を好みます。"
    },
    "ko": {
      "common_name": "알로카시아 제브리나",
      "description": "알로카시아 제브리나는 줄기의 줄무늬가 특징인 열대 식물입니다. 밝은 간접광과 높은 습도를 선호합니다."
    }
  },
