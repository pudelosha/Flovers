  "translations": {
    "en": {
      "common_name": "Silvery Peperomia",
      "description": "Peperomia incana is a small, low-maintenance houseplant with silvery-green leaves. It thrives in medium light and moderate watering conditions."
    },
    "pl": {
      "common_name": "Peperomia srebrzysta",
      "description": "Peperomia incana to mała roślina doniczkowa o srebrzysto-zielonych liściach. Dobrze rośnie w średnim świetle i umiarkowanym podlewaniu."
    },
    "de": {
      "common_name": "Silberne Peperomie",
      "description": "Peperomia incana ist eine kleine, pflegeleichte Zimmerpflanze mit silbrig-grünen Blättern. Sie gedeiht bei mittlerem Licht und mäßigem Gießen."
    },
    "fr": {
      "common_name": "Peperomia argentée",
      "description": "Peperomia incana est une petite plante d'intérieur à feuilles vertes argentées. Elle se développe bien à la lumière moyenne et avec un arrosage modéré."
    },
    "it": {
      "common_name": "Peperomia argentata",
      "description": "Peperomia incana è una pianta da interno di piccole dimensioni con foglie verde argento. Cresce bene in luce media e con irrigazione moderata."
    },
    "es": {
      "common_name": "Peperomia plateada",
      "description": "Peperomia incana es una planta pequeña de interior con hojas verde plateadas. Se adapta bien a luz media y riego moderado."
    },
    "pt": {
      "common_name": "Peperomia prateada",
      "description": "Peperomia incana é uma planta pequena e de fácil cuidado, com folhas verde-prateadas. Cresce bem em luz média e com rega moderada."
    },
    "ar": {
      "common_name": "بيبيروميا فضية",
      "description": "Peperomia incana هي نبتة صغيرة وسهلة العناية ذات أوراق خضراء فضية. تنمو بشكل جيد في الضوء المتوسط وبالري المعتدل."
    },
    "hi": {
      "common_name": "चांदी की पेपेरोमिया",
      "description": "Peperomia incana एक छोटी, कम रखरखाव वाली हाउसप्लांट है जिसमें चांदी-हरी पत्तियाँ होती हैं। यह मध्यम प्रकाश और नियमित पानी में अच्छी तरह से बढ़ती है।"
    },
    "zh": {
      "common_name": "银叶绿萝",
      "description": "Peperomia incana 是一种小型的室内植物，具有银绿色的叶子。它在中等光照和适度浇水的条件下生长良好。"
    },
    "ja": {
      "common_name": "銀葉ペペロミア",
      "description": "Peperomia incana は小型の室内植物で、銀緑色の葉を持っています。中程度の光と適度な水やりの条件でよく育ちます。"
    },
    "ko": {
      "common_name": "은색 페페로미아",
      "description": "Peperomia incana는 은색-녹색 잎을 가진 작은 실내 식물입니다. 중간 정도의 빛과 적당한 물주기에서 잘 자랍니다."
    }
  },
