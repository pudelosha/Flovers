  "translations": {
    "en": {
      "common_name": "Pale Orchid",
      "description": "Orchis pallens is a delicate, pale yellow orchid native to temperate regions. It thrives in shaded forest environments."
    },
    "pl": {
      "common_name": "Orchidea blada",
      "description": "Orchis pallens to delikatna, jasnożółta orchidea, występująca w strefach umiarkowanych. Rośnie w zacienionych lasach."
    },
    "de": {
      "common_name": "Blasse Orchidee",
      "description": "Orchis pallens ist eine zarte, blassgelbe Orchidee, die in gemäßigten Regionen heimisch ist. Sie gedeiht in schattigen Wäldern."
    },
    "fr": {
      "common_name": "Orchidée pâle",
      "description": "Orchis pallens est une orchidée délicate, jaune pâle, originaire des régions tempérées. Elle prospère dans des forêts ombragées."
    },
    "it": {
      "common_name": "Orchidea pallida",
      "description": "Orchis pallens è un'orchidea delicata di colore giallo pallido, originaria delle regioni temperate. Cresce in ambienti forestali ombreggiati."
    },
    "es": {
      "common_name": "Orquídea pálida",
      "description": "Orchis pallens es una orquídea delicada de color amarillo pálido originaria de regiones templadas. Crece en bosques sombreados."
    },
    "pt": {
      "common_name": "Orquídea pálida",
      "description": "Orchis pallens é uma orquídea delicada de cor amarela pálida, nativa de regiões temperadas. Cresce em florestas sombreadas."
    },
    "ar": {
      "common_name": "أوركيد باهت",
      "description": "Orchis pallens هي أوركيد رقيقة ذات أزهار صفراء باهتة، تنمو في المناطق المعتدلة. تزدهر في الغابات المظللة."
    },
    "hi": {
      "common_name": "पीला ऑर्किड",
      "description": "Orchis pallens एक नाजुक, पीले रंग की ऑर्किड है जो समशीतोष्ण क्षेत्रों में पाई जाती है। यह छायादार जंगलों में उगती है।"
    },
    "zh": {
      "common_name": "淡黄兰",
      "description": "Orchis pallens 是一种细腻的淡黄色兰花，原产于温带地区。它在阴凉的森林环境中茁壮成长。"
    },
    "ja": {
      "common_name": "淡いラン",
      "description": "Orchis pallens は、温帯地域に自生する繊細な淡黄色のランです。日陰の森林環境で繁茂します。"
    },
    "ko": {
      "common_name": "옅은 난초",
      "description": "Orchis pallens는 온대 지역에 자생하는 섬세한 연노란색 난초입니다. 그늘진 숲 환경에서 자랍니다."
    }
  },
