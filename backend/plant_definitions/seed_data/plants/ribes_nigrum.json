  "translations": {
    "en": {
      "common_name": "Black Currant",
      "description": "Black currant is a deciduous shrub known for its dark purple fruits. It thrives in temperate climates with moderate light and water."
    },
    "pl": {
      "common_name": "Czarna porzeczka",
      "description": "Czarna porzeczka to krzew liściasty, znany ze swoich ciemnofioletowych owoców. Rośnie w klimatach umiarkowanych, preferując średnie światło i podlewanie."
    },
    "de": {
      "common_name": "Schwarze Johannisbeere",
      "description": "Die schwarze Johannisbeere ist ein laubabwerfender Strauch, der für seine dunkelvioletten Früchte bekannt ist. Sie gedeiht in gemäßigten Klimazonen mit mittlerem Licht und Wasser."
    },
    "fr": {
      "common_name": "Cassis",
      "description": "Le cassis est un arbuste à feuilles caduques connu pour ses fruits violets foncés. Il pousse dans des climats tempérés avec une lumière et un arrosage modérés."
    },
    "it": {
      "common_name": "Ribes nero",
      "description": "Il ribes nero è un arbusto deciduo noto per i suoi frutti viola scuro. Cresce in climi temperati con luce e annaffiature moderate."
    },
    "es": {
      "common_name": "Grosella negra",
      "description": "La grosella negra es un arbusto caducifolio conocido por sus frutos morados oscuros. Crece en climas templados con luz y riego moderados."
    },
    "pt": {
      "common_name": "Groselha preta",
      "description": "A groselha preta é um arbusto caducifólio conhecido por seus frutos roxos escuros. Cresce em climas temperados com luz e rega moderadas."
    },
    "ar": {
      "common_name": "الكشمش الأسود",
      "description": "الكشمش الأسود هو شجيرة نفضية معروفة بثمارها الأرجوانية الداكنة. ينمو في المناخات المعتدلة مع ضوء وري معتدلين."
    },
    "hi": {
      "common_name": "काले करंट",
      "description": "काले करंट एक पर्णपाती झाड़ी है जो इसके गहरे बैंगनी रंग के फलों के लिए जानी जाती है। यह मध्यम रोशनी और पानी में उगता है।"
    },
    "zh": {
      "common_name": "黑醋栗",
      "description": "黑醋栗是一种落叶灌木，以其深紫色的果实而闻名。它在温带气候中生长，需要适度的光照和浇水。"
    },
    "ja": {
      "common_name": "ブラックカラント",
      "description": "ブラックカラントはその暗紫色の果実で知られる落葉低木です。適度な光と水で温帯気候で育ちます。"
    },
    "ko": {
      "common_name": "블랙 커런트",
      "description": "블랙 커런트는 짙은 보라색 열매로 알려진 낙엽성 관목입니다. 온대 기후에서 적당한 빛과 물로 자랍니다."
    }
  },
