  "translations": {
    "en": {
      "common_name": "Mother fern",
      "description": "Asplenium bulbiferum is a lush fern known for producing small plantlets on its fronds. It prefers bright indirect light and consistently moist soil indoors."
    },
    "pl": {
      "common_name": "Zanokcica żyworodna",
      "description": "Asplenium bulbiferum to bujna paproć wytwarzająca młode rośliny na liściach. Najlepiej rośnie w jasnym, rozproszonym świetle i wilgotnym podłożu."
    },
    "de": {
      "common_name": "Brutfarn",
      "description": "Asplenium bulbiferum ist ein Farn, der kleine Jungpflanzen an den Wedeln bildet. Er mag helles, indirektes Licht und gleichmäßig feuchte Erde."
    },
    "fr": {
      "common_name": "Fougère mère",
      "description": "Asplenium bulbiferum est une fougère produisant des plantules sur ses frondes. Elle apprécie une lumière indirecte et un sol constamment humide."
    },
    "it": {
      "common_name": "Felce madre",
      "description": "Asplenium bulbiferum è una felce ornamentale che produce nuove piantine sulle fronde. Cresce bene con luce indiretta e terreno umido."
    },
    "es": {
      "common_name": "Helecho madre",
      "description": "Asplenium bulbiferum es un helecho que forma pequeñas plantas en sus hojas. Prefiere luz indirecta y sustrato siempre húmedo."
    },
    "pt": {
      "common_name": "Feto-mãe",
      "description": "Asplenium bulbiferum é uma samambaia que produz mudas nas folhas. Gosta de luz indireta e solo mantido úmido."
    },
    "ar": {
      "common_name": "سرخس الأم",
      "description": "Asplenium bulbiferum سرخس يتميز بإنتاج نباتات صغيرة على أوراقه. يفضل الضوء غير المباشر والتربة الرطبة باستمرار."
    },
    "hi": {
      "common_name": "मदर फर्न",
      "description": "Asplenium bulbiferum एक सजावटी फर्न है जो पत्तियों पर नए पौधे बनाता है। इसे अप्रत्यक्ष रोशनी और नम मिट्टी पसंद है।"
    },
    "zh": {
      "common_name": "胎生铁角蕨",
      "description": "Asplenium bulbiferum 是一种会在叶片上长出小苗的蕨类植物。适合明亮散射光和持续湿润的土壤。"
    },
    "ja": {
      "common_name": "マザーファーン",
      "description": "Asplenium bulbiferum は葉に子株を付けるシダ植物です。明るい間接光と湿った土を好みます。"
    },
    "ko": {
      "common_name": "마더 고사리",
      "description": "Asplenium bulbiferum는 잎에서 새 개체가 자라는 고사리입니다. 밝은 간접광과 촉촉한 토양을 선호합니다."
    }
  },
