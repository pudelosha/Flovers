  "translations": {
    "en": {
      "common_name": "Rosemary",
      "description": "Rosemary is a fragrant herb used in cooking and for ornamental purposes. It thrives in full sun and well-drained soil."
    },
    "pl": {
      "common_name": "Rozmaryn",
      "description": "Rozmaryn to pachnąca roślina wykorzystywana w kuchni i do ozdoby. Lubi pełne słońce i dobrze przepuszczalną glebę."
    },
    "de": {
      "common_name": "Rosmarin",
      "description": "Rosmarin ist ein duftendes Kraut, das in der Küche und als Zierpflanze verwendet wird. Es gedeiht in vollem Sonnenschein und gut durchlässigem Boden."
    },
    "fr": {
      "common_name": "Romarin",
      "description": "Le romarin est une herbe parfumée utilisée en cuisine et à des fins ornementales. Il prospère en plein soleil et dans un sol bien drainé."
    },
    "it": {
      "common_name": "Rosmarino",
      "description": "Il rosmarino è un'erba profumata utilizzata in cucina e come pianta ornamentale. Cresce al sole pieno e in terreni ben drenati."
    },
    "es": {
      "common_name": "Romero",
      "description": "El romero es una hierba fragante utilizada en la cocina y con fines ornamentales. Crece a pleno sol en suelos bien drenados."
    },
    "pt": {
      "common_name": "Alecrim",
      "description": "O alecrim é uma erva aromática usada na culinária e com fins ornamentais. Cresce sob sol pleno e em solos bem drenados."
    },
    "ar": {
      "common_name": "إكليل الجبل",
      "description": "إكليل الجبل هو عشب عطري يستخدم في الطهي ولأغراض زينة. ينمو في الشمس الكاملة والتربة جيدة التصريف."
    },
    "hi": {
      "common_name": "रोज़मैरी",
      "description": "रोज़मैरी एक सुगंधित जड़ी-बूटी है जो खाना बनाने और सजावटी उद्देश्यों के लिए उपयोग की जाती है। यह पूर्ण सूर्य और अच्छी जल निकासी वाली मिट्टी में उगता है।"
    },
    "zh": {
      "common_name": "迷迭香",
      "description": "迷迭香是一种芳香的草药，用于烹饪和装饰用途。它在充足阳光和排水良好的土壤中茁壮成长。"
    },
    "ja": {
      "common_name": "ローズマリー",
      "description": "ローズマリーは料理や装飾目的で使用される香り高いハーブです。直射日光と排水性の良い土壌で育ちます。"
    },
    "ko": {
      "common_name": "로즈마리",
      "description": "로즈마리는 요리와 장식용으로 사용되는 향기로운 허브입니다. 햇볕이 잘 드는 곳과 배수가 좋은 토양에서 잘 자랍니다."
    }
  },
