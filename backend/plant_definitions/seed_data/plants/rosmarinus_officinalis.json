  "translations": {
    "en": {
      "common_name": "Rosemary",
      "description": "Rosmarinus officinalis is a fragrant herb native to the Mediterranean. It's commonly used in cooking and has medicinal properties."
    },
    "pl": {
      "common_name": "Rozmaryn",
      "description": "Rosmarinus officinalis to aromatyczne zioło pochodzące z basenu Morza Śródziemnego. Jest szeroko stosowane w kuchni i ma właściwości lecznicze."
    },
    "de": {
      "common_name": "Rosmarin",
      "description": "Rosmarinus officinalis ist ein duftendes Kraut, das ursprünglich aus dem Mittelmeerraum stammt. Es wird häufig in der Küche verwendet und hat heilende Eigenschaften."
    },
    "fr": {
      "common_name": "Romarin",
      "description": "Rosmarinus officinalis est une herbe aromatique originaire de la Méditerranée. Il est couramment utilisé en cuisine et possède des propriétés médicinales."
    },
    "it": {
      "common_name": "Rosmarino",
      "description": "Rosmarinus officinalis è un'erba aromatica originaria del Mediterraneo. È comunemente usato in cucina e ha proprietà medicinali."
    },
    "es": {
      "common_name": "Romero",
      "description": "Rosmarinus officinalis es una hierba aromática originaria de la cuenca mediterránea. Se usa comúnmente en la cocina y tiene propiedades medicinales."
    },
    "pt": {
      "common_name": "Alecrim",
      "description": "Rosmarinus officinalis é uma erva aromática nativa da região mediterrânea. É amplamente usado na culinária e possui propriedades medicinais."
    },
    "ar": {
      "common_name": "إكليل الجبل",
      "description": "Rosmarinus officinalis هو عشب عطري موطنه منطقة البحر الأبيض المتوسط. يستخدم بشكل شائع في الطهي وله خصائص طبية."
    },
    "hi": {
      "common_name": "रोजमैरी",
      "description": "Rosmarinus officinalis एक सुगंधित पौधा है जो भूमध्यसागरीय क्षेत्र का मूल निवासी है। इसका उपयोग रसोई में सामान्य है और इसमें औषधीय गुण होते हैं।"
    },
    "zh": {
      "common_name": "迷迭香",
      "description": "Rosmarinus officinalis 是一种芳香草本植物，原产于地中海地区。它在烹饪中广泛使用，且具有药用特性。"
    },
    "ja": {
      "common_name": "ローズマリー",
      "description": "Rosmarinus officinalisは地中海地方原産の芳香性のハーブです。料理に広く使われ、薬用効果もあります。"
    },
    "ko": {
      "common_name": "로즈마리",
      "description": "Rosmarinus officinalis는 지중해 지역이 원산지인 향긋한 허브입니다. 주로 요리에 사용되며 약용 효과가 있습니다."
    }
  },
