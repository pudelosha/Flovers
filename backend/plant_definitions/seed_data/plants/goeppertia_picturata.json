  "translations": {
    "en": {
      "common_name": "Calathea picturata",
      "description": "A striking ornamental plant with dark green leaves and silver markings. Known for its attractive foliage, it thrives in humid environments and moderate light."
    },
    "pl": {
      "common_name": "Calathea picturata",
      "description": "Atrakcyjna roślina ozdobna z ciemnozielonymi liśćmi i srebrnymi wzorami. Preferuje wilgotne środowisko i umiarkowane światło."
    },
    "de": {
      "common_name": "Calathea picturata",
      "description": "Eine auffällige Zierpflanze mit dunkelgrünen Blättern und silbernen Markierungen. Sie gedeiht in feuchten Umgebungen und mittlerem Licht."
    },
    "fr": {
      "common_name": "Calathea picturata",
      "description": "Plante ornementale frappante avec des feuilles vert foncé et des motifs argentés. Elle prospère dans des environnements humides et une lumière modérée."
    },
    "it": {
      "common_name": "Calathea picturata",
      "description": "Pianta ornamentale dall'aspetto sorprendente con foglie verde scuro e motivi argentati. Cresce bene in ambienti umidi e con luce moderata."
    },
    "es": {
      "common_name": "Calathea picturata",
      "description": "Planta ornamental llamativa con hojas verde oscuro y marcas plateadas. Prospera en ambientes húmedos y con luz moderada."
    },
    "pt": {
      "common_name": "Calathea picturata",
      "description": "Planta ornamental impressionante com folhas verde-escuras e marcas prateadas. Prefere ambientes úmidos e luz moderada."
    },
    "ar": {
      "common_name": "كالاثيا بيكتوراتا",
      "description": "نبات مزخرف لافت للنظر بأوراق خضراء داكنة وعلامات فضية. يزدهر في البيئات الرطبة والإضاءة المتوسطة."
    },
    "hi": {
      "common_name": "कैलाथिया पिक्टुराटा",
      "description": "एक आकर्षक सजावटी पौधा जिसमें गहरे हरे पत्ते और चांदी के निशान होते हैं। यह नमी वाले वातावरण और मध्यम प्रकाश में अच्छा करता है।"
    },
    "zh": {
      "common_name": "彩叶喜树",
      "description": "一种引人注目的观赏植物，具有深绿色的叶子和银色的标记。它在潮湿环境和适中的光照下茁壮成长。"
    },
    "ja": {
      "common_name": "カラテア・ピクトラタ",
      "description": "深緑色の葉と銀色の模様が特徴的な観賞用植物。湿度の高い環境と中程度の光でよく育ちます。"
    },
    "ko": {
      "common_name": "칼라테아 픽투라타",
      "description": "어두운 녹색의 잎과 은색의 무늬를 가진 인상적인 관상용 식물입니다. 습한 환경과 적당한 빛에서 잘 자랍니다."
    }
  },
