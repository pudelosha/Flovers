  "translations": {
    "en": {
      "common_name": "Firecracker Plant",
      "description": "A vibrant, bushy plant with tubular red flowers. It thrives in full sun and requires moderate watering."
    },
    "pl": {
      "common_name": "Roślinna petarda",
      "description": "Roślina o intensywnych czerwonych kwiatach, rosnąca w pełnym słońcu, wymaga umiarkowanego podlewania."
    },
    "de": {
      "common_name": "Feuerwerkspflanze",
      "description": "Eine lebendige Pflanze mit röhrenförmigen roten Blüten. Sie gedeiht in voller Sonne und benötigt mäßiges Gießen."
    },
    "fr": {
      "common_name": "Plante pétard",
      "description": "Une plante vibrante avec des fleurs rouges tubulaires. Elle prospère en plein soleil et nécessite un arrosage modéré."
    },
    "it": {
      "common_name": "Pianta Fuochi d'Artificio",
      "description": "Una pianta vivace con fiori rossi tubulari. Cresce al sole pieno e richiede irrigazione moderata."
    },
    "es": {
      "common_name": "Planta Cohete",
      "description": "Una planta vibrante con flores rojas tubulares. Crece a pleno sol y requiere riego moderado."
    },
    "pt": {
      "common_name": "Planta de Foguetes",
      "description": "Uma planta vibrante com flores vermelhas tubulares. Cresce sob luz solar direta e precisa de rega moderada."
    },
    "ar": {
      "common_name": "نبات صواريخ",
      "description": "نبات حيوي ذو زهور حمراء على شكل أنابيب. يزدهر تحت الشمس الكاملة ويتطلب ري معتدل."
    },
    "hi": {
      "common_name": "पटाखा पौधा",
      "description": "एक जीवंत पौधा जिसमें ट्यूबलर लाल फूल होते हैं। यह पूरी धूप में पनपता है और इसके लिए मध्यम पानी की आवश्यकता होती है।"
    },
    "zh": {
      "common_name": "烟花植物",
      "description": "一种充满活力的植物，拥有管状红色花朵。它在阳光充足的地方茁壮成长，并需要适量浇水。"
    },
    "ja": {
      "common_name": "花火植物",
      "description": "チューブ状の赤い花を持つ活気のある植物。直射日光で成長し、適度な水やりが必要です。"
    },
    "ko": {
      "common_name": "불꽃놀이 식물",
      "description": "튜브 모양의 빨간 꽃을 가진 활기찬 식물. 직사광선에서 자라며 적당한 물 주기가 필요합니다."
    }
  },
