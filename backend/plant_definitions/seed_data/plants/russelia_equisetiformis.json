  "translations": {
    "en": {
      "common_name": "Firecracker Plant",
      "description": "The firecracker plant is a shrub with slender, green stems and tubular red flowers. It thrives in well-drained soil and moderate sunlight."
    },
    "pl": {
      "common_name": "Roślina fajerwerkowa",
      "description": "Roślina fajerwerkowa to krzew o cienkich, zielonych pędach i rurkowatych czerwonych kwiatach. Rośnie w dobrze przepuszczalnej glebie i umiarkowanym świetle."
    },
    "de": {
      "common_name": "Feuerwerksblume",
      "description": "Die Feuerwerksblume ist ein Strauch mit dünnen, grünen Stängeln und röhrenförmigen roten Blüten. Sie gedeiht in gut durchlässigem Boden und mäßigem Licht."
    },
    "fr": {
      "common_name": "Plante Feu d'Artifice",
      "description": "La plante feu d'artifice est un arbuste aux tiges vertes fines et aux fleurs rouges tubulaires. Elle prospère dans un sol bien drainé et une lumière modérée."
    },
    "it": {
      "common_name": "Pianta Fuochi d'Artificio",
      "description": "La pianta fuochi d'artificio è un arbusto con steli verdi sottili e fiori rossi tubolari. Cresce in terreno ben drenato e luce moderata."
    },
    "es": {
      "common_name": "Planta Cohete",
      "description": "La planta cohete es un arbusto con tallos delgados y verdes y flores rojas en forma de tubo. Se desarrolla en suelos bien drenados y luz moderada."
    },
    "pt": {
      "common_name": "Planta Foguete",
      "description": "A planta foguete é um arbusto com hastes finas e verdes e flores vermelhas em forma de tubo. Cresce em solo bem drenado e luz moderada."
    },
    "ar": {
      "common_name": "نبات الألعاب النارية",
      "description": "نبات الألعاب النارية هو شجيرة ذات سيقان رقيقة وخضراء وأزهار حمراء أنبوبية. يزدهر في التربة جيدة التصريف وأشعة الشمس المعتدلة."
    },
    "hi": {
      "common_name": "आतिशबाज़ी पौधा",
      "description": "आतिशबाज़ी पौधा एक झाड़ी है जिसमें पतले, हरे तने और लाल रंग के ट्यूब जैसी फूल होते हैं। यह अच्छी तरह से जल निकासी वाली मिट्टी और मध्यम धूप में उगता है।"
    },
    "zh": {
      "common_name": "烟花植物",
      "description": "烟花植物是一种灌木，具有细长的绿色茎和红色管状花朵。它在排水良好的土壤和适中的阳光下茁壮成长。"
    },
    "ja": {
      "common_name": "花火の植物",
      "description": "花火の植物は細長い緑の茎と赤い筒状の花を持つ低木です。排水性の良い土壌と適度な光で成長します。"
    },
    "ko": {
      "common_name": "불꽃놀이 식물",
      "description": "불꽃놀이 식물은 가늘고 초록색 줄기와 빨간색 튜브 모양의 꽃을 가진 관목입니다. 배수가 좋은 토양과 적당한 햇빛에서 잘 자랍니다."
    }
  },
