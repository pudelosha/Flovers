  "translations": {
    "en": {
      "common_name": "Banana Plant",
      "description": "Musa acuminata is a tropical plant known for its edible bananas. It prefers bright, indirect light and high humidity."
    },
    "pl": {
      "common_name": "Roślina bananowa",
      "description": "Musa acuminata to roślina tropikalna, znana z jadalnych bananów. Wymaga jasnego, pośredniego światła i wysokiej wilgotności."
    },
    "de": {
      "common_name": "Bananenpflanze",
      "description": "Musa acuminata ist eine tropische Pflanze, die für ihre essbaren Bananen bekannt ist. Sie bevorzugt helles, indirektes Licht und hohe Luftfeuchtigkeit."
    },
    "fr": {
      "common_name": "Plante à banane",
      "description": "Musa acuminata est une plante tropicale connue pour ses bananes comestibles. Elle préfère la lumière indirecte et une humidité élevée."
    },
    "it": {
      "common_name": "Pianta di banana",
      "description": "Musa acuminata è una pianta tropicale nota per le sue banane commestibili. Preferisce la luce indiretta e un'alta umidità."
    },
    "es": {
      "common_name": "Planta de plátano",
      "description": "Musa acuminata es una planta tropical conocida por sus plátanos comestibles. Prefiere luz indirecta y alta humedad."
    },
    "pt": {
      "common_name": "Planta de banana",
      "description": "Musa acuminata é uma planta tropical conhecida por suas bananas comestíveis. Prefere luz indireta e alta umidade."
    },
    "ar": {
      "common_name": "نبتة الموز",
      "description": "Musa acuminata هي نبتة استوائية معروفة بموزها الصالح للأكل. تفضل الضوء الساطع غير المباشر والرطوبة العالية."
    },
    "hi": {
      "common_name": "केला का पौधा",
      "description": "Musa acuminata एक उष्णकटिबंधीय पौधा है जो अपनी खाने योग्य केले के लिए प्रसिद्ध है। इसे उज्ज्वल, अप्रत्यक्ष प्रकाश और उच्च आर्द्रता पसंद है।"
    },
    "zh": {
      "common_name": "香蕉植物",
      "description": "Musa acuminata 是一种热带植物，因其可食用的香蕉而闻名。它喜欢明亮的间接光和高湿度。"
    },
    "ja": {
      "common_name": "バナナ植物",
      "description": "Musa acuminata は食用バナナで知られる熱帯植物です。明るい間接光と高湿度を好みます。"
    },
    "ko": {
      "common_name": "바나나 식물",
      "description": "Musa acuminata는 먹을 수 있는 바나나로 유명한 열대 식물입니다. 밝은 간접광과 높은 습도를 선호합니다."
    }
  },
