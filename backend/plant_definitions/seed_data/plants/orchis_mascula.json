  "translations": {
    "en": {
      "common_name": "Early Purple Orchid",
      "description": "Orchis mascula, known as the Early Purple Orchid, is a hardy, perennial flower native to Europe. It blooms in spring with purple flowers."
    },
    "pl": {
      "common_name": "Storczyk wczesny fioletowy",
      "description": "Orchis mascula, znany jako storczyk wczesny fioletowy, to wieloletnia roślina kwitnąca na wiosnę fioletowymi kwiatami."
    },
    "de": {
      "common_name": "Frühzeitige Purpurorchidee",
      "description": "Orchis mascula, auch Frühzeitige Purpurorchidee genannt, ist eine mehrjährige Pflanze, die im Frühling mit violetten Blüten blüht."
    },
    "fr": {
      "common_name": "Orchidée pourpre précoce",
      "description": "L'Orchis mascula, connue sous le nom d'orchidée pourpre précoce, est une plante vivace qui fleurit au printemps avec des fleurs violettes."
    },
    "it": {
      "common_name": "Orchidea viola precoce",
      "description": "L'Orchis mascula, conosciuta come orchidea viola precoce, è una pianta perenne che fiorisce in primavera con fiori viola."
    },
    "es": {
      "common_name": "Orquídea morada temprana",
      "description": "La Orchis mascula, conocida como la orquídea morada temprana, es una planta perenne que florece en primavera con flores moradas."
    },
    "pt": {
      "common_name": "Orquídea roxa precoce",
      "description": "A Orchis mascula, conhecida como orquídea roxa precoce, é uma planta perene que floresce na primavera com flores roxas."
    },
    "ar": {
      "common_name": "زهرة الأوركيد البنفسجية المبكرة",
      "description": "أوركيس ماسكولا، المعروفة باسم زهرة الأوركيد البنفسجية المبكرة، هي نبات معمر يزهر في الربيع مع أزهار بنفسجية."
    },
    "hi": {
      "common_name": "प्रारंभिक बैंगनी आर्किड",
      "description": "ऑर्किस मास्कुला, जिसे प्रारंभिक बैंगनी आर्किड कहा जाता है, एक जीवित रहनेवाला पौधा है जो वसंत में बैंगनी फूलों के साथ खिलता है।"
    },
    "zh": {
      "common_name": "早期紫色兰花",
      "description": "Orchis mascula，又名早期紫色兰花，是一种多年生植物，春季开紫色花朵。"
    },
    "ja": {
      "common_name": "早咲きの紫色ラン",
      "description": "オーキス・マスキュラ（早咲きの紫色ラン）は春に紫色の花を咲かせる多年生植物です。"
    },
    "ko": {
      "common_name": "초기 자주색 난초",
      "description": "오르키스 마스큘라(초기 자주색 난초)는 봄에 자주색 꽃을 피우는 다년생 식물입니다."
    }
  },
