  "translations": {
    "en": {
      "common_name": "Dandelion",
      "description": "Dandelions are common wildflowers with distinctive yellow flowers. They are often found in lawns and meadows."
    },
    "pl": {
      "common_name": "Mniszek lekarski",
      "description": "Mniszek lekarski to pospolite rośliny dziko rosnące, z charakterystycznymi żółtymi kwiatami. Często spotykane na trawnikach i łąkach."
    },
    "de": {
      "common_name": "Löwenzahn",
      "description": "Löwenzähne sind weit verbreitete Wildpflanzen mit auffälligen gelben Blüten. Sie wachsen oft auf Rasenflächen und Wiesen."
    },
    "fr": {
      "common_name": "Pissenlit",
      "description": "Les pissenlits sont des fleurs sauvages communes avec des fleurs jaunes distinctives. On les trouve souvent dans les pelouses et les prairies."
    },
    "it": {
      "common_name": "Dente di leone",
      "description": "Il dente di leone è un fiore selvatico comune con fiori gialli distintivi. Si trova spesso nei prati e nei campi."
    },
    "es": {
      "common_name": "Diente de león",
      "description": "El diente de león es una flor silvestre común con flores amarillas distintivas. Se encuentra a menudo en céspedes y praderas."
    },
    "pt": {
      "common_name": "Dente de leão",
      "description": "O dente de leão é uma flor selvagem comum com flores amarelas distintas. Frequentemente encontrado em gramados e prados."
    },
    "ar": {
      "common_name": "الهندباء",
      "description": "الهندباء هي أزهار برية شائعة تتميز بأزهارها الصفراء. توجد غالبًا في المروج والمراعي."
    },
    "hi": {
      "common_name": "डंडेलियन",
      "description": "डंडेलियन एक सामान्य जंगली फूल है जिसमें विशिष्ट पीले फूल होते हैं। ये आमतौर पर घास के मैदानों और घास के मैदानों में पाए जाते हैं।"
    },
    "zh": {
      "common_name": "蒲公英",
      "description": "蒲公英是一种常见的野生花卉，具有独特的黄色花朵。它们通常生长在草地和草原中。"
    },
    "ja": {
      "common_name": "タンポポ",
      "description": "タンポポは、特徴的な黄色い花を持つ一般的な野生の花です。芝生や草原に多く見られます。"
    },
    "ko": {
      "common_name": "민들레",
      "description": "민들레는 특징적인 노란 꽃을 가진 일반적인 들꽃입니다. 잔디밭과 초원에서 자주 발견됩니다."
    }
  },
