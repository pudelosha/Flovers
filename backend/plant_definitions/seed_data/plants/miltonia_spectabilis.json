  "translations": {
    "en": {
      "common_name": "Spectacular Miltonia",
      "description": "Miltonia spectabilis is a species of orchid native to Brazil. It is known for its beautiful, colorful flowers and moderate care requirements."
    },
    "pl": {
      "common_name": "Miltonia spektakularna",
      "description": "Miltonia spectabilis to gatunek storczyka pochodzący z Brazylii. Znany jest z pięknych, kolorowych kwiatów i umiarkowanych wymagań pielęgnacyjnych."
    },
    "de": {
      "common_name": "Spektakuläre Miltonia",
      "description": "Miltonia spectabilis ist eine Orchideenart aus Brasilien. Sie ist bekannt für ihre schönen, farbenfrohen Blüten und mäßige Pflegeanforderungen."
    },
    "fr": {
      "common_name": "Miltonia spectaculaire",
      "description": "Miltonia spectabilis est une espèce d'orchidée originaire du Brésil. Elle est connue pour ses belles fleurs colorées et ses besoins modérés en soins."
    },
    "it": {
      "common_name": "Miltonia spettacolare",
      "description": "Miltonia spectabilis è una specie di orchidea originaria del Brasile. È conosciuta per i suoi fiori colorati e per le esigenze di cura moderate."
    },
    "es": {
      "common_name": "Miltonia espectacular",
      "description": "Miltonia spectabilis es una especie de orquídea originaria de Brasil. Es conocida por sus hermosas flores coloridas y sus requisitos moderados de cuidado."
    },
    "pt": {
      "common_name": "Miltonia espetacular",
      "description": "Miltonia spectabilis é uma espécie de orquídea nativa do Brasil. É conhecida por suas belas flores coloridas e necessidades moderadas de cuidados."
    },
    "ar": {
      "common_name": "ميلتونيا المدهشة",
      "description": "ميلتونيا سبكتابيليس هي نوع من الأوركيد موطنها البرازيل. تشتهر بأزهارها الجميلة الملونة ومتطلبات العناية المعتدلة."
    },
    "hi": {
      "common_name": "विचित्र मिल्टोनिया",
      "description": "मिल्टोनिया स्पेक्टैबिलिस एक प्रकार का ऑर्किड है जो ब्राजील का मूल निवासी है। यह अपनी सुंदर, रंगीन फूलों और मध्यम देखभाल आवश्यकताओं के लिए जाना जाता है।"
    },
    "zh": {
      "common_name": "壮丽的米尔托尼亚",
      "description": "米尔托尼亚·斯佩克塔比利斯是一种原产于巴西的兰花。它以美丽、多彩的花朵和适中的护理要求而闻名。"
    },
    "ja": {
      "common_name": "スペクタキュラー・ミルトニア",
      "description": "ミルトニア・スペクタビリスはブラジル原産のランの一種です。美しい色とりどりの花と、適度な世話の必要性で知られています。"
    },
    "ko": {
      "common_name": "스펙타큘러 밀토니아",
      "description": "밀토니아 스펙타빌리스는 브라질 원산의 난초입니다. 아름다운 다채로운 꽃과 적당한 관리 요구사항으로 유명합니다."
    }
  },
