  "translations": {
    "en": {
      "common_name": "Basil",
      "description": "Basil is a popular herb known for its aromatic leaves. It is used in cooking and enjoys sunny, warm conditions."
    },
    "pl": {
      "common_name": "Bazylia",
      "description": "Bazylia to popularna roślina aromatyczna, używana w kuchni. Lubi słoneczne, ciepłe warunki."
    },
    "de": {
      "common_name": "Basilikum",
      "description": "Basilikum ist ein beliebtes Kraut mit aromatischen Blättern. Es wird in der Küche verwendet und bevorzugt sonnige, warme Bedingungen."
    },
    "fr": {
      "common_name": "Basilic",
      "description": "Le basilic est une herbe populaire connue pour ses feuilles aromatiques. Elle est utilisée en cuisine et préfère des conditions ensoleillées et chaudes."
    },
    "it": {
      "common_name": "Basilico",
      "description": "Il basilico è un'erba popolare conosciuta per le sue foglie aromatiche. Viene utilizzato in cucina e ama condizioni di sole e calore."
    },
    "es": {
      "common_name": "Albahaca",
      "description": "La albahaca es una hierba popular conocida por sus hojas aromáticas. Se utiliza en la cocina y disfruta de condiciones soleadas y cálidas."
    },
    "pt": {
      "common_name": "Manjericão",
      "description": "O manjericão é uma erva popular conhecida por suas folhas aromáticas. É usado na culinária e prefere condições de sol e calor."
    },
    "ar": {
      "common_name": "ريحان",
      "description": "الريحان هو نبات عشبي شائع معروف بأوراقه العطرية. يُستخدم في الطهي ويفضل الظروف المشمسة والدافئة."
    },
    "hi": {
      "common_name": "तुलसी",
      "description": "तुलसी एक प्रसिद्ध हर्ब है, जो अपनी खुशबूदार पत्तियों के लिए जानी जाती है। इसका उपयोग रसोई में किया जाता है और यह धूप और गर्मी में बढ़ता है।"
    },
    "zh": {
      "common_name": "罗勒",
      "description": "罗勒是一种常见的香草，以其芳香的叶子闻名。它常用于烹饪，并喜欢阳光明媚和温暖的环境。"
    },
    "ja": {
      "common_name": "バジル",
      "description": "バジルは香り高い葉で知られる人気のハーブです。料理に使われ、日当たりの良い温暖な環境を好みます。"
    },
    "ko": {
      "common_name": "바질",
      "description": "바질은 향긋한 잎으로 잘 알려진 인기 있는 허브입니다. 요리에 사용되며 햇볕이 잘 드는 따뜻한 환경을 선호합니다."
    }
  },
