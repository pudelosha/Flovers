  "translations": {
    "en": {
      "common_name": "Shrubby Ragwort",
      "description": "Senecio articulatus is a shrubby succulent native to southern Africa. It is known for its branching growth habit and distinctive cylindrical leaves."
    },
    "pl": {
      "common_name": "Krzyżownik drzewiasty",
      "description": "Senecio articulatus to krzewiasty sukulent pochodzący z południowej Afryki. Charakteryzuje się rozgałęzionym wzrostem i cylindrycznymi liśćmi."
    },
    "de": {
      "common_name": "Strauch-Aster",
      "description": "Senecio articulatus ist ein strauchartiger Sukkulent aus Südafrika. Er zeichnet sich durch seine verzweigte Wuchsform und zylindrische Blätter aus."
    },
    "fr": {
      "common_name": "Ragwort arbustif",
      "description": "Senecio articulatus est une succulente arbustive originaire d'Afrique du Sud. Elle se distingue par sa croissance ramifiée et ses feuilles cylindriques."
    },
    "it": {
      "common_name": "Ragwort arbustivo",
      "description": "Senecio articulatus è una succulenta arbustiva originaria del Sudafrica. È conosciuta per la sua crescita ramificata e le sue foglie cilindriche."
    },
    "es": {
      "common_name": "Ragwort arbustivo",
      "description": "Senecio articulatus es una suculenta arbustiva originaria del sur de África. Es conocida por su hábito de crecimiento ramificado y sus hojas cilíndricas."
    },
    "pt": {
      "common_name": "Ragwort arbustivo",
      "description": "Senecio articulatus é uma suculenta arbustiva nativa do sul da África. É conhecida pelo seu hábito de crescimento ramificado e suas folhas cilíndricas."
    },
    "ar": {
      "common_name": "عشب شجيري",
      "description": "Senecio articulatus هو نبات عصاري شجيري موطنه جنوب أفريقيا. يعرف بنموه المتفرع وأوراقه الأسطوانية."
    },
    "hi": {
      "common_name": "झाड़ीदार रैगवॉर्ट",
      "description": "Senecio articulatus एक झाड़ीदार सूक्यूलेंट है जो दक्षिण अफ्रीका का मूल निवासी है। यह अपनी शाखाओं वाली वृद्धि और विशिष्ट सिलिंड्रिकल पत्तियों के लिए जाना जाता है."
    },
    "zh": {
      "common_name": "灌木状千屈菜",
      "description": "Senecio articulatus 是一种来自南非的灌木状多肉植物。它因其分枝的生长习性和独特的圆柱形叶片而闻名。"
    },
    "ja": {
      "common_name": "シュラッピーラグワート",
      "description": "Senecio articulatus は南アフリカ原産のシュラブ多肉植物です。枝分かれした成長習性と特徴的な円筒形の葉で知られています。"
    },
    "ko": {
      "common_name": "덤불 래그워트",
      "description": "Senecio articulatus는 남아프리카 출신의 덤불 다육 식물입니다. 그것은 가지가 많은 성장 습관과 독특한 원통형 잎으로 유명합니다."
    }
  },
