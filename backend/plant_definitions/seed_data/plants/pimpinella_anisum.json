  "translations": {
    "en": {
      "common_name": "Anise",
      "description": "Anise (Pimpinella anisum) is an herb known for its aromatic seeds. It is often used in cooking and for medicinal purposes."
    },
    "pl": {
      "common_name": "Anyż",
      "description": "Anyż (Pimpinella anisum) to roślina znana ze swoich aromatycznych nasion. Często używana w kuchni i do celów leczniczych."
    },
    "de": {
      "common_name": "Anis",
      "description": "Anis (Pimpinella anisum) ist ein Kraut, das für seine aromatischen Samen bekannt ist. Es wird oft in der Küche und für medizinische Zwecke verwendet."
    },
    "fr": {
      "common_name": "Anis",
      "description": "L'anis (Pimpinella anisum) est une herbe connue pour ses graines aromatiques. Elle est souvent utilisée en cuisine et à des fins médicinales."
    },
    "it": {
      "common_name": "Anice",
      "description": "L'anice (Pimpinella anisum) è un'erba nota per i suoi semi aromatici. Viene spesso utilizzato in cucina e per scopi medicinali."
    },
    "es": {
      "common_name": "Anís",
      "description": "El anís (Pimpinella anisum) es una planta conocida por sus semillas aromáticas. Se usa comúnmente en la cocina y con fines medicinales."
    },
    "pt": {
      "common_name": "Anis",
      "description": "O anis (Pimpinella anisum) é uma erva conhecida por suas sementes aromáticas. É frequentemente utilizado na culinária e para fins medicinais."
    },
    "ar": {
      "common_name": "اليانسون",
      "description": "اليانسون (Pimpinella anisum) هو نبات معروف بذوره العطرية. يُستخدم في الطهي ولأغراض طبية."
    },
    "hi": {
      "common_name": "सौंफ",
      "description": "सौंफ (Pimpinella anisum) एक जड़ी-बूटी है जो अपनी सुगंधित बीजों के लिए जानी जाती है। इसका उपयोग आमतौर पर खाना पकाने और चिकित्सा उद्देश्यों के लिए किया जाता है."
    },
    "zh": {
      "common_name": "茴香",
      "description": "茴香（Pimpinella anisum）是一种以其芳香种子而闻名的草本植物。常用于烹饪和药用目的。"
    },
    "ja": {
      "common_name": "アニス",
      "description": "アニス（Pimpinella anisum）は、その芳香性の種子で知られるハーブです。料理や医療目的でよく使用されます。"
    },
    "ko": {
      "common_name": "회향",
      "description": "회향(Pimpinella anisum)은 그 향기로운 씨앗으로 알려진 허브입니다. 요리 및 의약 목적으로 자주 사용됩니다."
    }
  },
