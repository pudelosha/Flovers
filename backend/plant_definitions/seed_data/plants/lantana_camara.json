  "translations": {
    "en": {
      "common_name": "Common lantana",
      "description": "Lantana camara is a sun-loving flowering shrub known for its colorful clustered blooms. It thrives in warm climates and tolerates drought once established."
    },
    "pl": {
      "common_name": "Lantana pospolita",
      "description": "Lantana camara to kwitnący krzew lubiący słońce. Dobrze rośnie w ciepłym klimacie i znosi okresową suszę."
    },
    "de": {
      "common_name": "Wandelröschen",
      "description": "Lantana camara ist ein sonnenliebender Blütenstrauch mit bunten Blüten. Er ist hitzetolerant und pflegeleicht."
    },
    "fr": {
      "common_name": "Lantanier",
      "description": "Lantana camara est un arbuste à fleurs aimant le soleil. Il pousse bien en climat chaud et supporte la sécheresse."
    },
    "it": {
      "common_name": "Lantana",
      "description": "Lantana camara è un arbusto ornamentale amante del sole. Fiorisce abbondantemente nei climi caldi."
    },
    "es": {
      "common_name": "Lantana",
      "description": "Lantana camara es un arbusto florido que prefiere pleno sol. Es resistente y tolera la sequía."
    },
    "pt": {
      "common_name": "Lantana",
      "description": "Lantana camara é um arbusto florífero que gosta de sol pleno. Adapta-se bem a climas quentes."
    },
    "ar": {
      "common_name": "لانتانا",
      "description": "لانتانا كامارا شجيرة مزهرة تحب الشمس. تنمو جيدًا في المناخات الدافئة وتتحمل الجفاف."
    },
    "hi": {
      "common_name": "लैंटाना",
      "description": "लैंटाना कैमारा एक फूलदार झाड़ी है जिसे धूप पसंद है। यह गर्म जलवायु में अच्छी तरह बढ़ती है।"
    },
    "zh": {
      "common_name": "马缨丹",
      "description": "马缨丹是一种喜阳的开花灌木。适合温暖气候并具有耐旱性。"
    },
    "ja": {
      "common_name": "ランタナ",
      "description": "ランタナは日当たりを好む花木です。暖かい環境でよく育ちます。"
    },
    "ko": {
      "common_name": "란타나",
      "description": "란타나는 햇빛을 좋아하는 꽃관목입니다. 따뜻한 기후에서 잘 자랍니다."
    }
  },
