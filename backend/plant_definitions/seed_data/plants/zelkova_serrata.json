  "translations": {
    "en": {
      "common_name": "Japanese Zelkova",
      "description": "Zelkova serrata is a deciduous tree native to Japan. It is known for its elegant shape and resilience in urban environments."
    },
    "pl": {
      "common_name": "Zelkowa serrata",
      "description": "Zelkova serrata to drzewo liściaste pochodzące z Japonii. Jest znane ze swojej eleganckiej formy i odporności na środowiska miejskie."
    },
    "de": {
      "common_name": "Japanische Zelkove",
      "description": "Zelkova serrata ist ein Laubbaum, der in Japan beheimatet ist. Er ist bekannt für seine elegante Form und Widerstandsfähigkeit in städtischen Umgebungen."
    },
    "fr": {
      "common_name": "Zelkova du Japon",
      "description": "Zelkova serrata est un arbre à feuilles caduques originaire du Japon. Il est connu pour sa forme élégante et sa résistance dans les environnements urbains."
    },
    "it": {
      "common_name": "Zelkova giapponese",
      "description": "Zelkova serrata è un albero deciduo originario del Giappone. È noto per la sua forma elegante e la sua resistenza negli ambienti urbani."
    },
    "es": {
      "common_name": "Zelkova japonesa",
      "description": "Zelkova serrata es un árbol caducifolio originario de Japón. Es conocido por su forma elegante y resistencia en entornos urbanos."
    },
    "pt": {
      "common_name": "Zelkova do Japão",
      "description": "Zelkova serrata é uma árvore caducifólia originária do Japão. É conhecida por sua forma elegante e resistência em ambientes urbanos."
    },
    "ar": {
      "common_name": "زيلكوفا يابانية",
      "description": "زيلكوفا سيراتا هي شجرة نفضية موطنها اليابان. تشتهر بشكلها الأنيق ومرونتها في البيئات الحضرية."
    },
    "hi": {
      "common_name": "जापानी ज़ेलकोवा",
      "description": "ज़ेलकोवा सेराटा एक पत्तियाँ गिराने वाली पेड़ है जो जापान का मूल निवासी है। यह अपनी सुंदर आकृति और शहरी वातावरण में लचीलापन के लिए जाना जाता है।"
    },
    "zh": {
      "common_name": "日本榉",
      "description": "Zelkova serrata 是一种原产于日本的落叶树。它以优雅的形状和在城市环境中的韧性而闻名。"
    },
    "ja": {
      "common_name": "日本ゼルコバ",
      "description": "Zelkova serrata は日本原産の落葉樹です。その優雅な形状と都市環境での耐久性で知られています。"
    },
    "ko": {
      "common_name": "일본 제르코바",
      "description": "Zelkova serrata는 일본 원산의 낙엽수입니다. 그 우아한 형태와 도시 환경에서의 내구성으로 잘 알려져 있습니다."
    }
  },
