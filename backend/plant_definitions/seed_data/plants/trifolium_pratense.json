  "translations": {
    "en": {
      "common_name": "Red Clover",
      "description": "Trifolium pratense is a short-lived perennial legume native to Europe and Western Asia. It is commonly found in meadows and pastures and improves soil by fixing nitrogen."
    },
    "pl": {
      "common_name": "Koniczyna łąkowa",
      "description": "Trifolium pratense to wieloletnia roślina motylkowa pochodząca z Europy. Występuje na łąkach i poprawia żyzność gleby dzięki wiązaniu azotu."
    },
    "de": {
      "common_name": "Rotklee",
      "description": "Trifolium pratense ist eine mehrjährige Leguminose aus Europa. Sie wächst auf Wiesen und bindet Stickstoff im Boden."
    },
    "fr": {
      "common_name": "Trèfle des prés",
      "description": "Trifolium pratense est une plante vivace originaire d’Europe. Elle pousse dans les prairies et enrichit le sol en azote."
    },
    "it": {
      "common_name": "Trifoglio rosso",
      "description": "Trifolium pratense è una pianta perenne originaria dell’Europa. Cresce nei prati e migliora il suolo fissando l’azoto."
    },
    "es": {
      "common_name": "Trébol rojo",
      "description": "Trifolium pratense es una planta perenne originaria de Europa. Se encuentra en praderas y enriquece el suelo con nitrógeno."
    },
    "pt": {
      "common_name": "Trevo-vermelho",
      "description": "Trifolium pratense é uma planta perene nativa da Europa. Cresce em pastagens e melhora o solo ao fixar nitrogênio."
    },
    "ar": {
      "common_name": "النفل الأحمر",
      "description": "Trifolium pratense نبات معمر موطنه أوروبا. ينمو في المروج ويساهم في تحسين التربة بتثبيت النيتروجين."
    },
    "hi": {
      "common_name": "लाल तिपतिया",
      "description": "Trifolium pratense यूरोप की बहुवर्षीय पौधा है। यह घास के मैदानों में उगता है और मिट्टी में नाइट्रोजन जोड़ता है।"
    },
    "zh": {
      "common_name": "红三叶草",
      "description": "Trifolium pratense 原产于欧洲，是一种多年生豆科植物。常见于草地，可通过固氮改善土壤。"
    },
    "ja": {
      "common_name": "アカツメクサ",
      "description": "Trifolium pratense はヨーロッパ原産の多年草です。牧草地に生育し、土壌を改良します。"
    },
    "ko": {
      "common_name": "붉은토끼풀",
      "description": "Trifolium pratense는 유럽 원산의 다년생 식물입니다. 초지에서 자라며 질소를 고정합니다."
    }
  },
