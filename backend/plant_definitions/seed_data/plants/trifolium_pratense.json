  "translations": {
    "en": {
      "common_name": "Red Clover",
      "description": "Red clover is a fast-growing, herbaceous perennial plant. It is commonly used for its medicinal and soil-enhancing properties."
    },
    "pl": {
      "common_name": "Koniczyna czerwona",
      "description": "Koniczyna czerwona to szybko rosnąca roślina wieloletnia. Często stosowana w medycynie i do poprawy jakości gleby."
    },
    "de": {
      "common_name": "Rote Klee",
      "description": "Roter Klee ist eine schnell wachsende, mehrjährige Pflanze. Sie wird häufig wegen ihrer heilenden und bodenverbessernden Eigenschaften verwendet."
    },
    "fr": {
      "common_name": "Trèfle rouge",
      "description": "Le trèfle rouge est une plante vivace à croissance rapide. Il est couramment utilisé pour ses propriétés médicinales et d'amélioration du sol."
    },
    "it": {
      "common_name": "Trifoglio rosso",
      "description": "Il trifoglio rosso è una pianta perenne a crescita rapida. È comunemente usato per le sue proprietà medicinali e per migliorare il terreno."
    },
    "es": {
      "common_name": "Trébol rojo",
      "description": "El trébol rojo es una planta perenne de crecimiento rápido. Se utiliza comúnmente por sus propiedades medicinales y para mejorar el suelo."
    },
    "pt": {
      "common_name": "Trevo vermelho",
      "description": "O trevo vermelho é uma planta perene de crescimento rápido. É amplamente utilizado por suas propriedades medicinais e de melhoria do solo."
    },
    "ar": {
      "common_name": "عشب البرسيم الأحمر",
      "description": "البرسيم الأحمر هو نبات معمر ينمو بسرعة. يتم استخدامه بشكل شائع لخواصه الطبية وتحسين التربة."
    },
    "hi": {
      "common_name": "लाल त्रिफला",
      "description": "लाल त्रिफला एक तेजी से बढ़ने वाला बारहमासी पौधा है। इसे आमतौर पर इसके औषधीय और मृदा सुधार गुणों के लिए इस्तेमाल किया जाता है."
    },
    "zh": {
      "common_name": "红三叶草",
      "description": "红三叶草是一种快速生长的多年生植物。通常用于其药用和改善土壤的特性。"
    },
    "ja": {
      "common_name": "レッドクローバー",
      "description": "レッドクローバーは急速に成長する多年草です。その薬用特性と土壌改良特性でよく使用されます。"
    },
    "ko": {
      "common_name": "레드 클로버",
      "description": "레드 클로버는 빠르게 자라는 다년생 식물입니다. 약용 및 토양 개선 특성으로 일반적으로 사용됩니다."
    }
  },
