  "translations": {
    "en": {
      "common_name": "Whorled Plectranthus",
      "description": "A low-maintenance indoor plant with trailing stems and attractive foliage. Tolerates moderate light and occasional watering."
    },
    "pl": {
      "common_name": "Plektrantus okółkowy",
      "description": "Roślina doniczkowa o zwisających łodygach i ozdobnych liściach. Dobrze rośnie w średnim świetle i wymaga umiarkowanego podlewania."
    },
    "de": {
      "common_name": "Kreis-Plectranthus",
      "description": "Eine pflegeleichte Zimmerpflanze mit hängenden Stängeln und dekorativen Blättern. Verträgt mittleres Licht und gelegentliches Gießen."
    },
    "fr": {
      "common_name": "Plectranthus verticillé",
      "description": "Plante d'intérieur facile à entretenir avec des tiges pendantes et un feuillage décoratif. Tolère la lumière modérée et un arrosage occasionnel."
    },
    "it": {
      "common_name": "Plectranthus verticillato",
      "description": "Pianta d'appartamento a bassa manutenzione con steli pendenti e foglie decorative. Tollera luce moderata e annaffiature occasionali."
    },
    "es": {
      "common_name": "Plectranthus verticillatus",
      "description": "Planta de interior de bajo mantenimiento con tallos colgantes y follaje decorativo. Tolera luz moderada y riego ocasional."
    },
    "pt": {
      "common_name": "Plectranthus verticillatus",
      "description": "Planta de interior de fácil manutenção com caules pendentes e folhagem decorativa. Tolera luz moderada e rega ocasional."
    },
    "ar": {
      "common_name": "بليكترا نتس فيرتيسيلاتوس",
      "description": "نبات داخلي سهل الصيانة ذو سيقان متدلية وأوراق مزخرفة. يتحمل الضوء المعتدل والري العرضي."
    },
    "hi": {
      "common_name": "व्होर्ल्ड प्लीक्ट्रांथस",
      "description": "कम रखरखाव वाला इनडोर पौधा जिसमें लटकते तने और आकर्षक पत्तियाँ होती हैं। यह मध्यम प्रकाश और कभी-कभी पानी देने को सहन करता है।"
    },
    "zh": {
      "common_name": "旋生白千层",
      "description": "一种低维护的室内植物，具有下垂的茎和具有装饰性的叶子。耐受中等光照和偶尔浇水。"
    },
    "ja": {
      "common_name": "環状プレクトランサス",
      "description": "垂れた茎と装飾的な葉を持つ、手間のかからない室内植物。中程度の光と時々の水やりに耐えます。"
    },
    "ko": {
      "common_name": "둘레플렉트란투스",
      "description": "떨어지는 줄기와 장식적인 잎을 가진 손쉬운 실내 식물. 중간 정도의 빛과 가끔 물을 주는 것을 견딥니다."
    }
  },
