  "translations": {
    "en": {
      "common_name": "Oxeye Daisy",
      "description": "Leucanthemum vulgare is a hardy perennial daisy native to Europe. It grows in meadows and grasslands and produces white flowers with yellow centers in summer."
    },
    "pl": {
      "common_name": "Złocień właściwy",
      "description": "Leucanthemum vulgare to wytrzymała bylina pochodząca z Europy. Rośnie na łąkach i pastwiskach, kwitnąc latem białymi kwiatami."
    },
    "de": {
      "common_name": "Margerite",
      "description": "Leucanthemum vulgare ist eine robuste Staude aus Europa. Sie wächst auf Wiesen und bildet im Sommer weiße Blüten."
    },
    "fr": {
      "common_name": "Marguerite commune",
      "description": "Leucanthemum vulgare est une vivace rustique originaire d’Europe. Elle pousse dans les prairies et fleurit en été."
    },
    "it": {
      "common_name": "Margherita comune",
      "description": "Leucanthemum vulgare è una perenne rustica europea. Cresce in prati e fiorisce in estate con fiori bianchi."
    },
    "es": {
      "common_name": "Margarita común",
      "description": "Leucanthemum vulgare es una planta perenne resistente de Europa. Habita praderas y florece en verano."
    },
    "pt": {
      "common_name": "Margarida-dos-prados",
      "description": "Leucanthemum vulgare é uma planta perene resistente da Europa. Cresce em prados e floresce no verão."
    },
    "ar": {
      "common_name": "الأقحوان البري",
      "description": "Leucanthemum vulgare نبات معمر قوي موطنه أوروبا. ينمو في المروج ويزهر صيفًا بأزهار بيضاء."
    },
    "hi": {
      "common_name": "जंगली डेज़ी",
      "description": "Leucanthemum vulgare यूरोप की एक मजबूत बारहमासी पौधा है। यह घास के मैदानों में उगता है और गर्मियों में फूल देता है।"
    },
    "zh": {
      "common_name": "牛眼菊",
      "description": "Leucanthemum vulgare 是原产欧洲的多年生植物。常见于草地，夏季开白色花。"
    },
    "ja": {
      "common_name": "フランスギク",
      "description": "Leucanthemum vulgare はヨーロッパ原産の多年草です。草原に生え、夏に白い花を咲かせます。"
    },
    "ko": {
      "common_name": "불란서국화",
      "description": "Leucanthemum vulgare는 유럽 원산의 다년생 식물입니다. 초원에서 자라며 여름에 흰 꽃을 피웁니다."
    }
  },
