  "translations": {
    "en": {
      "common_name": "Lavender",
      "description": "Lavender is a popular herb known for its fragrant flowers. It thrives in full sun and well-drained soil."
    },
    "pl": {
      "common_name": "Lawenda",
      "description": "Lawenda to popularne zioło, znane z pachnących kwiatów. Rośnie najlepiej w pełnym słońcu i dobrze przepuszczalnej glebie."
    },
    "de": {
      "common_name": "Lavendel",
      "description": "Lavendel ist ein beliebtes Kraut, bekannt für seine duftenden Blüten. Er gedeiht am besten in voller Sonne und gut durchlässigem Boden."
    },
    "fr": {
      "common_name": "Lavande",
      "description": "La lavande est une plante aromatique populaire, connue pour ses fleurs parfumées. Elle pousse bien en plein soleil et dans un sol bien drainé."
    },
    "it": {
      "common_name": "Lavanda",
      "description": "La lavanda è un'erba popolare, conosciuta per i suoi fiori profumati. Cresce bene in pieno sole e in un terreno ben drenato."
    },
    "es": {
      "common_name": "Lavanda",
      "description": "La lavanda es una hierba popular conocida por sus flores fragantes. Crece bien a pleno sol y en suelos bien drenados."
    },
    "pt": {
      "common_name": "Lavanda",
      "description": "A lavanda é uma planta popular conhecida pelas suas flores fragrantes. Cresce bem ao sol pleno e em solo bem drenado."
    },
    "ar": {
      "common_name": "لافندر",
      "description": "اللافندر هو نبات عشبي شائع معروف بأزهاره المعطرة. ينمو بشكل جيد في الشمس الكاملة والتربة جيدة الصرف."
    },
    "hi": {
      "common_name": "लैवेंडर",
      "description": "लैवेंडर एक लोकप्रिय जड़ी-बूटी है जो अपनी खुशबूदार फूलों के लिए जानी जाती है। यह पूर्ण सूर्य और अच्छे जल निकासी वाली मिट्टी में अच्छी तरह से बढ़ता है।"
    },
    "zh": {
      "common_name": "薰衣草",
      "description": "薰衣草是一种受欢迎的草本植物，以其芳香的花朵而闻名。它在全日照和排水良好的土壤中生长得最好。"
    },
    "ja": {
      "common_name": "ラベンダー",
      "description": "ラベンダーは香りの良い花で知られる人気のあるハーブです。完全な日光と良好な排水性の土壌でよく育ちます。"
    },
    "ko": {
      "common_name": "라벤더",
      "description": "라벤더는 향기로운 꽃으로 알려진 인기 있는 허브입니다. 완전한 햇볕과 잘 배수되는 토양에서 잘 자랍니다."
    }
  },
