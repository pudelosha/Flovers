  "translations": {
    "en": {
      "common_name": "Rose-scented Geranium",
      "description": "Pelargonium graveolens is a fragrant plant, known for its rose-like scent. It is commonly grown for ornamental and aromatic uses."
    },
    "pl": {
      "common_name": "Geranium o zapachu róży",
      "description": "Pelargonium graveolens to roślina o przyjemnym zapachu przypominającym różę. Często uprawiana jako roślina ozdobna i aromatyczna."
    },
    "de": {
      "common_name": "Rosenduft-Geranie",
      "description": "Pelargonium graveolens ist eine duftende Pflanze, bekannt für ihren rosigen Duft. Sie wird häufig als Zier- und Aromapflanze angebaut."
    },
    "fr": {
      "common_name": "Géranium parfumé à la rose",
      "description": "Pelargonium graveolens est une plante parfumée, connue pour son odeur de rose. Elle est souvent cultivée à des fins ornementales et aromatiques."
    },
    "it": {
      "common_name": "Geranio profumato alla rosa",
      "description": "Pelargonium graveolens è una pianta profumata, conosciuta per il suo odore di rosa. Viene comunemente coltivata per usi ornamentali e aromatici."
    },
    "es": {
      "common_name": "Geranio perfumado a rosa",
      "description": "Pelargonium graveolens es una planta fragante, conocida por su aroma a rosa. Se cultiva comúnmente con fines ornamentales y aromáticos."
    },
    "pt": {
      "common_name": "Gerânio perfumado a rosa",
      "description": "Pelargonium graveolens é uma planta perfumada, conhecida pelo seu aroma a rosa. Frequentemente cultivada para fins ornamentais e aromáticos."
    },
    "ar": {
      "common_name": "الجيرانيوم المعطر بالورود",
      "description": "يعد Pelargonium graveolens من النباتات العطرية المعروفة برائحتها المشابهة للورد. غالبًا ما يتم زراعتها لأغراض الزينة والعطور."
    },
    "hi": {
      "common_name": "गुलाब-सुगंधित गेरियम",
      "description": "Pelargonium graveolens एक सुगंधित पौधा है, जिसे गुलाब जैसी खुशबू के लिए जाना जाता है। इसे आमतौर पर सजावटी और सुगंधित उपयोग के लिए उगाया जाता है."
    },
    "zh": {
      "common_name": "玫瑰香天竺葵",
      "description": "Pelargonium graveolens 是一种芳香植物，以玫瑰般的香气闻名。它通常用于观赏和香料用途。"
    },
    "ja": {
      "common_name": "ローズの香りのゼラニウム",
      "description": "Pelargonium graveolens は、バラのような香りで知られる芳香植物です。観賞用および香料用に栽培されることが多いです。"
    },
    "ko": {
      "common_name": "장미향 제라늄",
      "description": "Pelargonium graveolens는 장미와 같은 향기로 알려진 향기로운 식물입니다. 일반적으로 장식용 및 향료용으로 재배됩니다."
    }
  },
