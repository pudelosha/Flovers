  "translations": {
    "en": {
      "common_name": "Living Stone Plant",
      "description": "A succulent known for its stone-like appearance. Thrives in bright light and dry conditions."
    },
    "pl": {
      "common_name": "Roślina kamienna",
      "description": "Sukulenty o wyglądzie kamienia. Lubi jasne światło i suche warunki."
    },
    "de": {
      "common_name": "Lebende Steinpflanze",
      "description": "Eine Sukkulente mit steinartigem Aussehen. Bevorzugt helles Licht und trockene Bedingungen."
    },
    "fr": {
      "common_name": "Plante pierre vivante",
      "description": "Une succulente ayant l'apparence d'une pierre. Préfère la lumière vive et des conditions sèches."
    },
    "it": {
      "common_name": "Pianta pietra vivente",
      "description": "Una succulenta con aspetto di pietra. Cresce bene con molta luce e condizioni asciutte."
    },
    "es": {
      "common_name": "Planta piedra viva",
      "description": "Una suculenta con aspecto de piedra. Le gusta la luz brillante y las condiciones secas."
    },
    "pt": {
      "common_name": "Planta pedra viva",
      "description": "Uma suculenta com aparência de pedra. Prefere luz intensa e condições secas."
    },
    "ar": {
      "common_name": "نبتة الحجر الحية",
      "description": "نبتة عصارية ذات مظهر يشبه الحجر. تنمو بشكل جيد في الضوء الساطع والظروف الجافة."
    },
    "hi": {
      "common_name": "लिविंग स्टोन पौधा",
      "description": "एक मांसल पौधा जो पत्थर जैसे रूप में होता है। इसे उज्जवल रोशनी और शुष्क स्थितियाँ पसंद हैं।"
    },
    "zh": {
      "common_name": "活石植物",
      "description": "一种外观像石头的多肉植物。喜欢明亮的光线和干燥的环境。"
    },
    "ja": {
      "common_name": "リビングストーンプラント",
      "description": "石のような外観を持つ多肉植物。明るい光と乾燥した条件で育ちます。"
    },
    "ko": {
      "common_name": "리빙스톤 식물",
      "description": "돌처럼 보이는 다육식물. 밝은 빛과 건조한 조건에서 잘 자랍니다."
    }
  },
