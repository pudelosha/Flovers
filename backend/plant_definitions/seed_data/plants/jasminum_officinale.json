  "translations": {
    "en": {
      "common_name": "Common Jasmine",
      "description": "Jasminum officinale is a fragrant climbing plant known for its white star-shaped flowers. It prefers bright sunlight and regular watering during active growth."
    },
    "pl": {
      "common_name": "Jaśmin lekarski",
      "description": "Jasminum officinale to pachnące pnącze o białych kwiatach. Najlepiej rośnie w jasnym miejscu i wymaga regularnego podlewania."
    },
    "de": {
      "common_name": "Echter Jasmin",
      "description": "Jasminum officinale ist eine duftende Kletterpflanze mit weißen Blüten. Sie benötigt viel Licht und gleichmäßige Wassergaben."
    },
    "fr": {
      "common_name": "Jasmin officinal",
      "description": "Jasminum officinale est une plante grimpante parfumée aux fleurs blanches. Elle aime le soleil et un arrosage régulier."
    },
    "it": {
      "common_name": "Gelsomino comune",
      "description": "Jasminum officinale è un rampicante profumato con fiori bianchi. Richiede luce intensa e annaffiature regolari."
    },
    "es": {
      "common_name": "Jazmín común",
      "description": "Jasminum officinale es una planta trepadora aromática con flores blancas. Necesita sol abundante y riego regular."
    },
    "pt": {
      "common_name": "Jasmim-comum",
      "description": "Jasminum officinale é uma trepadeira perfumada com flores brancas. Prefere sol forte e regas regulares."
    },
    "ar": {
      "common_name": "الياسمين البلدي",
      "description": "Jasminum officinale نبات متسلق عطري بأزهار بيضاء. يحتاج إلى شمس جيدة وري منتظم."
    },
    "hi": {
      "common_name": "साधारण चमेली",
      "description": "Jasminum officinale सुगंधित लता है जिसमें सफेद फूल होते हैं। इसे तेज धूप और नियमित पानी पसंद है।"
    },
    "zh": {
      "common_name": "素馨花",
      "description": "Jasminum officinale 是一种芳香的攀援植物，开白色花。它需要充足阳光和定期浇水。"
    },
    "ja": {
      "common_name": "ソケイ",
      "description": "Jasminum officinale は香りのあるつる性植物です。明るい日光と定期的な水やりを好みます。"
    },
    "ko": {
      "common_name": "재스민",
      "description": "Jasminum officinale 는 향기로운 덩굴식물입니다. 충분한 햇빛과 규칙적인 물주기가 필요합니다."
    }
  },
