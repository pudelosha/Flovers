  "translations": {
    "en": {
      "common_name": "Provincial Orchid",
      "description": "Orchis provincialis is a species of orchid native to southern Europe. It thrives in well-drained soil with moderate watering."
    },
    "pl": {
      "common_name": "Orchidea prowincjonalna",
      "description": "Orchis provincialis to gatunek orchidei występujący w południowej Europie. Preferuje dobrze przepuszczalną glebę i umiarkowane podlewanie."
    },
    "de": {
      "common_name": "Provinzialorchidee",
      "description": "Orchis provincialis ist eine Orchideenart aus Südeuropa. Sie gedeiht in gut durchlässigem Boden mit mäßiger Bewässerung."
    },
    "fr": {
      "common_name": "Orchidée provinciale",
      "description": "L'Orchis provincialis est une espèce d'orchidée originaire du sud de l'Europe. Elle prospère dans un sol bien drainé avec un arrosage modéré."
    },
    "it": {
      "common_name": "Orchidea provinciale",
      "description": "Orchis provincialis è una specie di orchidea originaria del sud Europa. Cresce bene in un terreno ben drenato con annaffiature moderate."
    },
    "es": {
      "common_name": "Orquídea provincial",
      "description": "Orchis provincialis es una especie de orquídea originaria del sur de Europa. Crece bien en suelos bien drenados con riego moderado."
    },
    "pt": {
      "common_name": "Orquídea provincial",
      "description": "Orchis provincialis é uma espécie de orquídea nativa do sul da Europa. Cresce bem em solo bem drenado com rega moderada."
    },
    "ar": {
      "common_name": "زهرة الأوركيد الإقليمية",
      "description": "Orchis provincialis هو نوع من الأوركيد موطنه جنوب أوروبا. يزدهر في التربة جيدة الصرف مع الري المعتدل."
    },
    "hi": {
      "common_name": "प्रांतीय ऑर्किड",
      "description": "Orchis provincialis एक प्रकार की ऑर्किड है जो दक्षिणी यूरोप का मूल निवासी है। यह अच्छी जल निकासी वाली मिट्टी में और मध्यम पानी के साथ बढ़ता है।"
    },
    "zh": {
      "common_name": "省级兰花",
      "description": "Orchis provincialis 是一种原产于南欧的兰花。它在排水良好的土壤中生长，浇水适中。"
    },
    "ja": {
      "common_name": "地方オーキッド",
      "description": "Orchis provincialis は南ヨーロッパ原産のオーキッド種です。排水性の良い土壌で、適度な水やりが必要です。"
    },
    "ko": {
      "common_name": "지방 난초",
      "description": "Orchis provincialis는 남유럽 원산지의 난초 종류입니다. 배수가 좋은 토양에서 적당한 물주기로 잘 자랍니다."
    }
  },
