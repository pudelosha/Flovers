  "translations": {
    "en": {
      "common_name": "Madagascar periwinkle",
      "description": "Catharanthus roseus is a sun-loving flowering plant native to Madagascar. It is widely grown as an ornamental for its long-lasting blooms and tolerance of heat and drought."
    },
    "pl": {
      "common_name": "Katarantus różowy",
      "description": "Catharanthus roseus to roślina kwitnąca pochodząca z Madagaskaru. Ceniona jest za długie kwitnienie i odporność na upały."
    },
    "de": {
      "common_name": "Madagaskar-Immergrün",
      "description": "Catharanthus roseus stammt aus Madagaskar und ist eine beliebte Zierpflanze. Sie blüht lange und verträgt Sonne sowie Trockenheit."
    },
    "fr": {
      "common_name": "Pervenche de Madagascar",
      "description": "Catharanthus roseus est originaire de Madagascar. Elle est cultivée pour ses fleurs abondantes et sa résistance à la chaleur."
    },
    "it": {
      "common_name": "Pervinca del Madagascar",
      "description": "Catharanthus roseus è una pianta ornamentale originaria del Madagascar. Fiorisce a lungo e tollera bene il caldo."
    },
    "es": {
      "common_name": "Vinca de Madagascar",
      "description": "Catharanthus roseus es originaria de Madagascar. Se cultiva por su floración prolongada y su resistencia al calor."
    },
    "pt": {
      "common_name": "Vinca de Madagascar",
      "description": "Catharanthus roseus é nativa de Madagascar. É valorizada pela floração contínua e pela tolerância ao calor."
    },
    "ar": {
      "common_name": "ونكا مدغشقر",
      "description": "نبات Catharanthus roseus موطنه مدغشقر. يزرع للزينة بسبب أزهاره المستمرة وتحمله للحرارة."
    },
    "hi": {
      "common_name": "मेडागास्कर सदाबहार",
      "description": "Catharanthus roseus मेडागास्कर का मूल पौधा है। यह लंबे समय तक फूल देने और गर्मी सहन करने के लिए जाना जाता है।"
    },
    "zh": {
      "common_name": "长春花",
      "description": "Catharanthus roseus 原产于马达加斯加。因花期长且耐热而广泛栽培。"
    },
    "ja": {
      "common_name": "ニチニチソウ",
      "description": "Catharanthus roseus はマダガスカル原産の植物です。長い開花期と耐暑性で知られています。"
    },
    "ko": {
      "common_name": "일일초",
      "description": "Catharanthus roseus는 마다가스카르 원산의 식물입니다. 긴 개화 기간과 더위에 강한 특징이 있습니다."
    }
  },
