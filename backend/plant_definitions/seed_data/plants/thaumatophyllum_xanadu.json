  "translations": {
    "en": {
      "common_name": "Thaumatophyllum Xanadu",
      "description": "Thaumatophyllum xanadu is a tropical plant native to Brazil. It has large, glossy leaves and thrives in humid environments."
    },
    "pl": {
      "common_name": "Thaumatophyllum xanadu",
      "description": "Thaumatophyllum xanadu to roślina tropikalna pochodząca z Brazylii. Posiada duże, błyszczące liście i rośnie w wilgotnym środowisku."
    },
    "de": {
      "common_name": "Thaumatophyllum xanadu",
      "description": "Thaumatophyllum xanadu ist eine tropische Pflanze aus Brasilien. Sie hat große, glänzende Blätter und gedeiht in feuchten Umgebungen."
    },
    "fr": {
      "common_name": "Thaumatophyllum xanadu",
      "description": "Thaumatophyllum xanadu est une plante tropicale originaire du Brésil. Elle possède de grandes feuilles brillantes et prospère dans des environnements humides."
    },
    "it": {
      "common_name": "Thaumatophyllum xanadu",
      "description": "Thaumatophyllum xanadu è una pianta tropicale originaria del Brasile. Ha grandi foglie lucide e cresce in ambienti umidi."
    },
    "es": {
      "common_name": "Thaumatophyllum xanadu",
      "description": "Thaumatophyllum xanadu es una planta tropical originaria de Brasil. Tiene grandes hojas brillantes y prospera en ambientes húmedos."
    },
    "pt": {
      "common_name": "Thaumatophyllum xanadu",
      "description": "Thaumatophyllum xanadu é uma planta tropical nativa do Brasil. Possui grandes folhas brilhantes e cresce em ambientes úmidos."
    },
    "ar": {
      "common_name": "ثوماتوفيلوم كسينادو",
      "description": "ثوماتوفيلوم كسينادو هو نبات استوائي موطنه البرازيل. لديه أوراق كبيرة لامعة وينمو في البيئات الرطبة."
    },
    "hi": {
      "common_name": "थौमेटोफायलम एक्सानाडू",
      "description": "थौमेटोफायलम एक्सानाडू एक उष्णकटिबंधीय पौधा है जो ब्राजील का मूल निवासी है। इसमें बड़ी, चमकदार पत्तियाँ होती हैं और यह नम वातावरण में उगता है।"
    },
    "zh": {
      "common_name": "塔马托菲勒姆·仙杜",
      "description": "塔马托菲勒姆·仙杜是原产于巴西的热带植物。它拥有大而光滑的叶子，喜欢湿润的环境。"
    },
    "ja": {
      "common_name": "タウマトフィルム・ザナドゥ",
      "description": "タウマトフィルム・ザナドゥはブラジル原産の熱帯植物です。大きな光沢のある葉を持ち、湿った環境で育ちます。"
    },
    "ko": {
      "common_name": "타우마토필룸 자나두",
