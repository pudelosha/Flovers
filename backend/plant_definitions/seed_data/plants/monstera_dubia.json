  "translations": {
    "en": {
      "common_name": "Shingle plant",
      "description": "Monstera dubia is a tropical climbing aroid native to Central and South America. Juvenile leaves lie flat against surfaces, while mature plants develop larger foliage when allowed to climb."
    },
    "pl": {
      "common_name": "Monstera dachówkowata",
      "description": "Monstera dubia to tropikalne pnącze pochodzące z Ameryki Środkowej i Południowej. Młode liście przylegają do podpór, a starsze rosną większe przy wspinaniu."
    },
    "de": {
      "common_name": "Schindelblatt",
      "description": "Monstera dubia ist eine tropische Kletterpflanze aus Mittel- und Südamerika. Junge Blätter liegen flach an, ältere entwickeln sich größer beim Klettern."
    },
    "fr": {
      "common_name": "Monstera en bardeaux",
      "description": "Monstera dubia est une plante grimpante tropicale d’Amérique centrale et du Sud. Les jeunes feuilles plaquent au support avant de devenir plus grandes."
    },
    "it": {
      "common_name": "Monstera a scandole",
      "description": "Monstera dubia è una pianta tropicale rampicante originaria dell’America Centrale e Meridionale. Le foglie giovani aderiscono ai supporti."
    },
    "es": {
      "common_name": "Monstera teja",
      "description": "Monstera dubia es una planta tropical trepadora de América Central y del Sur. Las hojas jóvenes crecen pegadas a superficies."
    },
    "pt": {
      "common_name": "Monstera telhada",
      "description": "Monstera dubia é uma planta tropical trepadeira da América Central e do Sul. As folhas jovens aderem aos suportes."
    },
    "ar": {
      "common_name": "مونستيرا دوبيا",
      "description": "Monstera dubia نبات استوائي متسلق من أمريكا الوسطى والجنوبية. الأوراق الصغيرة تنمو ملاصقة للدعامات."
    },
    "hi": {
      "common_name": "शिंगल प्लांट",
      "description": "Monstera dubia मध्य और दक्षिण अमेरिका का उष्णकटिबंधीय चढ़ने वाला पौधा है। इसकी युवा पत्तियाँ सतह से चिपकी रहती हैं।"
    },
    "zh": {
      "common_name": "瓦片龟背竹",
      "description": "Monstera dubia 原产于中南美洲的热带攀援植物。幼叶贴附生长，成熟后叶片变大。"
    },
    "ja": {
      "common_name": "シングルプラント",
      "description": "Monstera dubia は中南米原産の熱帯性つる植物です。幼葉は面に密着して成長します。"
    },
    "ko": {
      "common_name": "신글 몬스테라",
      "description": "Monstera dubia는 중남미 원산의 열대성 덩굴식물입니다. 어린 잎은 표면에 밀착해 자랍니다."
    }
  },
