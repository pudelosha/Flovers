  "translations": {
    "en": {
      "common_name": "Kirk's Sowthistle",
      "description": "Sonchus kirkii is a perennial herb native to New Zealand. It thrives in coastal environments and produces bright yellow flowers."
    },
    "pl": {
      "common_name": "Słonecznik Kirk'a",
      "description": "Sonchus kirkii to roślina wieloletnia pochodząca z Nowej Zelandii. Rośnie w środowiskach przybrzeżnych i wytwarza jasne żółte kwiaty."
    },
    "de": {
      "common_name": "Kirk's Faltblatt",
      "description": "Sonchus kirkii ist ein mehrjähriges Kraut aus Neuseeland. Es wächst in Küstengebieten und produziert leuchtend gelbe Blumen."
    },
    "fr": {
      "common_name": "Chardon de Kirk",
      "description": "Sonchus kirkii est une herbe vivace originaire de Nouvelle-Zélande. Elle pousse dans des environnements côtiers et produit des fleurs jaunes éclatantes."
    },
    "it": {
      "common_name": "Sonchus di Kirk",
      "description": "Sonchus kirkii è un'erba perenne originaria della Nuova Zelanda. Cresce in ambienti costieri e produce fiori gialli brillanti."
    },
    "es": {
      "common_name": "Cardo de Kirk",
      "description": "Sonchus kirkii es una planta perenne originaria de Nueva Zelanda. Crece en ambientes costeros y produce flores amarillas brillantes."
    },
    "pt": {
      "common_name": "Cardo de Kirk",
      "description": "Sonchus kirkii é uma planta perene originária da Nova Zelândia. Cresce em ambientes costeiros e produz flores amarelas brilhantes."
    },
    "ar": {
      "common_name": "شوك كيرك",
      "description": "Sonchus kirkii هو نبات معمر من نيوزيلندا. ينمو في البيئات الساحلية ويعطي زهورًا صفراء زاهية."
    },
    "hi": {
      "common_name": "किर्क का सोनचुस",
      "description": "Sonchus kirkii एक वार्षिक जड़ी-बूटी है जो न्यूज़ीलैंड की है। यह तटीय वातावरण में पनपता है और उज्जवल पीले फूल उत्पन्न करता है।"
    },
    "zh": {
      "common_name": "柯克的蓬蒿",
      "description": "Sonchus kirkii 是一种原产于新西兰的多年生草本植物。它生长在沿海环境中，开出明亮的黄色花朵。"
    },
    "ja": {
      "common_name": "カークのソウスィスル",
      "description": "Sonchus kirkii はニュージーランド原産の多年草です。沿岸の環境に生育し、明るい黄色の花を咲かせます。"
    },
    "ko": {
      "common_name": "커크 소우시슬",
      "description": "Sonchus kirkii는 뉴질랜드 원산의 다년생 풀입니다. 해안 환경에서 자라며 밝은 노란색 꽃을 피웁니다."
    }
  },
