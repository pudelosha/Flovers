  "translations": {
    "en": {
      "common_name": "Star Jasmine",
      "description": "Star Jasmine is an evergreen climbing vine with fragrant white flowers. It prefers moderate sunlight and well-drained soil."
    },
    "pl": {
      "common_name": "Jaśmin gwiazdkowy",
      "description": "Jaśmin gwiazdkowy to zimozielna roślina pnąca z pachnącymi białymi kwiatami. Preferuje umiarkowane światło i dobrze przepuszczalną glebę."
    },
    "de": {
      "common_name": "Sterndost",
      "description": "Sterndost ist eine immergrüne Kletterpflanze mit duftenden weißen Blüten. Sie bevorzugt mittleres Licht und gut durchlässigen Boden."
    },
    "fr": {
      "common_name": "Jasmin étoilé",
      "description": "Le jasmin étoilé est une plante grimpante à feuilles persistantes avec des fleurs blanches parfumées. Elle préfère une lumière modérée et un sol bien drainé."
    },
    "it": {
      "common_name": "Gelsomino stellato",
      "description": "Il gelsomino stellato è una pianta rampicante sempreverde con fiori bianchi profumati. Preferisce luce media e terreno ben drenato."
    },
    "es": {
      "common_name": "Jazmín estrellado",
      "description": "El jazmín estrellado es una planta trepadora perenne con flores blancas fragantes. Prefiere luz moderada y suelo bien drenado."
    },
    "pt": {
      "common_name": "Jasmim estrela",
      "description": "O jasmim estrela é uma planta trepadora perene com flores brancas perfumadas. Prefere luz moderada e solo bem drenado."
    },
    "ar": {
      "common_name": "ياسمين نجمي",
      "description": "الياسمين النجمي هو نبات متسلق دائم الخضرة مع أزهار بيضاء عطرة. يفضل الضوء المعتدل والتربة جيدة التصريف."
    },
    "hi": {
      "common_name": "स्टार जैस्मिन",
      "description": "स्टार जैस्मिन एक सदाबहार लता है जिसमें सुगंधित सफेद फूल होते हैं। यह मध्यम रोशनी और अच्छी जल निकासी वाली मिट्टी पसंद करता है।"
    },
    "zh": {
      "common_name": "星形茉莉",
      "description": "星形茉莉是一种常绿攀缘植物，开有香气扑鼻的白色花朵。它喜欢中等光照和排水良好的土壤。"
    },
    "ja": {
      "common_name": "スタージャスミン",
      "description": "スタージャスミンは香りの良い白い花を持つ常緑のつる植物です。中程度の光と排水性の良い土壌を好みます。"
    },
    "ko": {
      "common_name": "별 자스민",
      "description": "별 자스민은 향기로운 흰 꽃을 가진 상록 덩굴 식물입니다. 중간 정도의 빛과 배수가 좋은 토양을 선호합니다."
    }
  },
