  "translations": {
    "en": {
      "common_name": "Gollum Jade",
      "description": "A unique succulent with tubular, finger-like leaves. It thrives in bright, indirect light and requires minimal water."
    },
    "pl": {
      "common_name": "Jade Gollum",
      "description": "Unikalny sukulent o rurkowatych, palczastych liściach. Rośnie najlepiej w jasnym, pośrednim świetle i wymaga minimalnego podlewania."
    },
    "de": {
      "common_name": "Gollum-Jade",
      "description": "Ein einzigartiger Sukkulent mit röhrenförmigen, fingerähnlichen Blättern. Er gedeiht bei hellem, indirektem Licht und benötigt wenig Wasser."
    },
    "fr": {
      "common_name": "Jade Gollum",
      "description": "Un succulent unique avec des feuilles tubulaires ressemblant à des doigts. Il prospère à la lumière indirecte et nécessite peu d'eau."
    },
    "it": {
      "common_name": "Gollum Jade",
      "description": "Un succulento unico con foglie tubolari simili a dita. Cresce bene in luce indiretta e richiede poca acqua."
    },
    "es": {
      "common_name": "Jade Gollum",
      "description": "Un suculento único con hojas tubulares similares a dedos. Crece bien con luz indirecta y necesita poca agua."
    },
    "pt": {
      "common_name": "Jade Gollum",
      "description": "Uma suculenta única com folhas tubulares semelhantes a dedos. Cresce bem com luz indireta e precisa de pouca água."
    },
    "ar": {
      "common_name": "جولدوم جاد",
      "description": "نبات عصاري فريد من نوعه بأوراق أنبوبية تشبه الأصابع. ينمو في ضوء غير مباشر ويتطلب كمية قليلة من الماء."
    },
    "hi": {
      "common_name": "गोलम जेड",
      "description": "एक अद्वितीय मांसल पौधा जिसकी ट्यूबलर, उंगलियों जैसी पत्तियाँ होती हैं। यह उज्ज्वल, अप्रत्यक्ष प्रकाश में पनपता है और कम पानी की आवश्यकता होती है."
    },
    "zh": {
      "common_name": "高卢翡翠",
      "description": "一种独特的多肉植物，具有管状、类似手指的叶子。它在明亮的间接光照下生长，需要较少的水分。"
    },
    "ja": {
      "common_name": "ゴルムジェイド",
      "description": "指のような管状の葉を持つユニークな多肉植物です。間接光でよく育ち、少量の水分を必要とします。"
    },
    "ko": {
      "common_name": "골룸 제이드",
      "description": "손가락처럼 튜브 모양의 독특한 다육 식물입니다. 밝은 간접광에서 잘 자라며 적은 물을 필요로 합니다."
    }
  },
