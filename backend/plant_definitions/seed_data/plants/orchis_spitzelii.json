  "translations": {
    "en": {
      "common_name": "Spitzel's Orchid",
      "description": "Orchis spitzelii is a species of orchid native to Europe. It thrives in moderate light and requires regular watering, avoiding overwatering."
    },
    "pl": {
      "common_name": "Orkidea Spitzela",
      "description": "Orchis spitzelii to gatunek orchidei występujący w Europie. Wymaga umiarkowanego światła i regularnego podlewania, unikając przelania."
    },
    "de": {
      "common_name": "Spitzel Orchidee",
      "description": "Orchis spitzelii ist eine Orchideenart, die in Europa beheimatet ist. Sie benötigt mittleres Licht und regelmäßiges Gießen, ohne Überwässerung."
    },
    "fr": {
      "common_name": "Orchidée de Spitzel",
      "description": "Orchis spitzelii est une espèce d'orchidée originaire d'Europe. Elle préfère une lumière modérée et un arrosage régulier, sans excès."
    },
    "it": {
      "common_name": "Orchidea di Spitzel",
      "description": "Orchis spitzelii è una specie di orchidea originaria dell'Europa. Cresce meglio con luce moderata e richiede annaffiature regolari, evitando ristagni d'acqua."
    },
    "es": {
      "common_name": "Orquídea de Spitzel",
      "description": "Orchis spitzelii es una especie de orquídea originaria de Europa. Necesita luz moderada y riego regular, evitando el exceso de agua."
    },
    "pt": {
      "common_name": "Orquídea de Spitzel",
      "description": "Orchis spitzelii é uma espécie de orquídea nativa da Europa. Prefere luz moderada e exige rega regular, evitando o excesso de água."
    },
    "ar": {
      "common_name": "أوركيد سباتزل",
      "description": "Orchis spitzelii هو نوع من الأوركيد موطنه أوروبا. ينمو في ضوء متوسط ويحتاج إلى الري المنتظم مع تجنب الإفراط في الري."
    },
    "hi": {
      "common_name": "स्पिट्ज़ल की आर्किड",
      "description": "Orchis spitzelii एक आर्किड प्रजाति है जो यूरोप की मूल है। इसे मध्यम रोशनी और नियमित पानी की आवश्यकता होती है, अत्यधिक पानी देने से बचना चाहिए।"
    },
    "zh": {
      "common_name": "斯皮策尔兰",
      "description": "Orchis spitzelii 是一种原产于欧洲的兰花。它需要适度的光照和定期浇水，避免过度浇水。"
    },
    "ja": {
      "common_name": "スピッツェルのラン",
      "description": "Orchis spitzelii はヨーロッパ原産のランの一種です。中程度の光と定期的な水やりが必要で、過剰な水やりを避けるべきです。"
    },
    "ko": {
      "common_name": "스피첼의 난초",
      "description": "Orchis spitzelii는 유럽 원산의 난초입니다. 중간 정도의 빛과 정기적인 물주기가 필요하며 과도한 물주기를 피해야 합니다."
    }
  },
