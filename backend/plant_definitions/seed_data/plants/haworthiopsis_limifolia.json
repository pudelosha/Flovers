  "translations": {
    "en": {
      "common_name": "Thread-leaf Haworthia",
      "description": "Haworthiopsis limifolia is a small succulent known for its thread-like leaves. It thrives in medium light with low watering needs."
    },
    "pl": {
      "common_name": "Haworthia nitkowata",
      "description": "Haworthiopsis limifolia to mały sukulent o nitkowatych liściach. Rośnie w średnim świetle, potrzebując niewiele wody."
    },
    "de": {
      "common_name": "Faden-Haworthia",
      "description": "Haworthiopsis limifolia ist ein kleiner Sukkulent mit fadenartigen Blättern. Sie gedeiht bei mittlerem Licht und benötigt wenig Wasser."
    },
    "fr": {
      "common_name": "Haworthia à feuilles filiformes",
      "description": "Haworthiopsis limifolia est une petite succulente avec des feuilles filiformes. Elle prospère sous une lumière moyenne avec peu d'arrosage."
    },
    "it": {
      "common_name": "Haworthia a foglie filiformi",
      "description": "Haworthiopsis limifolia è una piccola succulenta con foglie filiformi. Cresce con luce media e necessità di annaffiature basse."
    },
    "es": {
      "common_name": "Haworthia de hojas filiformes",
      "description": "Haworthiopsis limifolia es una pequeña suculenta conocida por sus hojas filiformes. Crece con luz media y requiere poco riego."
    },
    "pt": {
      "common_name": "Haworthia de folhas filiformes",
      "description": "Haworthiopsis limifolia é uma pequena suculenta conhecida por suas folhas finas. Cresce com luz média e pouca água."
    },
    "ar": {
      "common_name": "هاوورثيا ذات الأوراق الخيطية",
      "description": "Haworthiopsis limifolia هي نبات عصاري صغير ذو أوراق خيطية. يزدهر في الضوء المتوسط مع احتياجات منخفضة من الري."
    },
    "hi": {
      "common_name": "थ्रेड-लीफ हावोर्थिया",
      "description": "Haworthiopsis limifolia एक छोटा मांसल पौधा है जो अपनी धागे जैसी पत्तियों के लिए जाना जाता है। यह मध्यम प्रकाश में अच्छी तरह बढ़ता है और कम पानी की आवश्यकता होती है।"
    },
    "zh": {
      "common_name": "细叶哈沃西亚",
      "description": "Haworthiopsis limifolia 是一种小型多肉植物，因其线状叶片而闻名。它在中等光照下生长，水分需求较低。"
    },
    "ja": {
      "common_name": "スレッドリーフ・ハウォルシア",
      "description": "Haworthiopsis limifolia は糸状の葉を持つ小さな多肉植物です。中程度の光で成長し、少ない水分を必要とします。"
    },
    "ko": {
      "common_name": "실 리프 하워시아",
      "description": "Haworthiopsis limifolia는 실처럼 얇은 잎을 가진 작은 다육 식물입니다. 중간 정도의 빛에서 자라며 물을 적게 필요로 합니다."
    }
  },
