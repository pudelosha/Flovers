  "translations": {
    "en": {
      "common_name": "Balsam",
      "description": "Impatiens balsamina is a flowering plant native to Asia. It thrives in moderate sunlight and requires regular watering."
    },
    "pl": {
      "common_name": "Balsam",
      "description": "Impatiens balsamina to roślina kwitnąca pochodząca z Azji. Rośnie najlepiej w umiarkowanym świetle i wymaga regularnego podlewania."
    },
    "de": {
      "common_name": "Balsam",
      "description": "Impatiens balsamina ist eine blühende Pflanze, die in Asien beheimatet ist. Sie gedeiht bei mäßigem Licht und benötigt regelmäßiges Gießen."
    },
    "fr": {
      "common_name": "Balsam",
      "description": "Impatiens balsamina est une plante à fleurs originaire d'Asie. Elle prospère sous une lumière modérée et nécessite un arrosage régulier."
    },
    "it": {
      "common_name": "Balsamo",
      "description": "Impatiens balsamina è una pianta da fiore originaria dell'Asia. Cresce bene in luce moderata e necessita di annaffiature regolari."
    },
    "es": {
      "common_name": "Balsamina",
      "description": "Impatiens balsamina es una planta con flores originaria de Asia. Crece bien con luz moderada y requiere riego regular."
    },
    "pt": {
      "common_name": "Balsamina",
      "description": "Impatiens balsamina é uma planta com flores nativa da Ásia. Desenvolve-se bem sob luz moderada e requer rega regular."
    },
    "ar": {
      "common_name": "بالمسات",
      "description": "إمباتينس بالسامينا هي نبات مزهر موطنه آسيا. ينمو في الضوء المعتدل ويتطلب سقاية منتظمة."
    },
    "hi": {
      "common_name": "बैल्सम",
      "description": "इंपैटियन्स बालसामिना एक फूलों वाली पौधा है जो एशिया का मूल है। यह मध्यम रोशनी में अच्छी तरह से बढ़ता है और नियमित पानी की आवश्यकता होती है।"
    },
    "zh": {
      "common_name": "凤仙花",
      "description": "凤仙花（Impatiens balsamina）是一种原产于亚洲的开花植物。它在中等光照下茁壮成长，并需要定期浇水。"
    },
    "ja": {
      "common_name": "バルサム",
      "description": "インパティエンス・バルサミナはアジア原産の花を咲かせる植物です。適度な光の中で育ち、定期的な水やりが必要です。"
    },
    "ko": {
      "common_name": "발삼",
      "description": "임페이티엔스 발사미나는 아시아 원산의 꽃 식물입니다. 중간 정도의 빛에서 잘 자라며 정기적인 물주기가 필요합니다."
    }
  },
