  "translations": {
    "en": {
      "common_name": "Christmas orchid",
      "description": "Laelia anceps is an epiphytic orchid native to Mexico and Central America. It produces large lavender flowers in winter and prefers bright light with good air movement."
    },
    "pl": {
      "common_name": "Laelia zimowa",
      "description": "Laelia anceps to epifityczny storczyk z Meksyku i Ameryki Środkowej. Kwitnie zimą dużymi lawendowymi kwiatami i wymaga jasnego stanowiska."
    },
    "de": {
      "common_name": "Weihnachts-Laelie",
      "description": "Laelia anceps ist eine epiphytische Orchidee aus Mexiko und Mittelamerika. Sie blüht im Winter und benötigt viel Licht sowie gute Luftzirkulation."
    },
    "fr": {
      "common_name": "Laelia de Noël",
      "description": "Laelia anceps est une orchidée épiphyte originaire du Mexique et d’Amérique centrale. Elle fleurit en hiver et apprécie une lumière vive."
    },
    "it": {
      "common_name": "Laelia invernale",
      "description": "Laelia anceps è un’orchidea epifita del Messico e America Centrale. Fiorisce in inverno e richiede luce intensa e buona aerazione."
    },
    "es": {
      "common_name": "Laelia navideña",
      "description": "Laelia anceps es una orquídea epífita de México y Centroamérica. Florece en invierno y necesita luz brillante y riego regolare."
    },
    "pt": {
      "common_name": "Laélia-de-inverno",
      "description": "Laelia anceps é uma orquídea epífita do México e América Central. Floresce no inverno e prefere luz intensa e boa ventilação."
    },
    "ar": {
      "common_name": "أوركيد عيد الميلاد",
      "description": "Laelia anceps أوركيد هوائي موطنه المكسيك وأمريكا الوسطى. يزهر شتاءً ويحتاج إلى ضوء ساطع وتهوية جيدة."
    },
    "hi": {
      "common_name": "क्रिसमस ऑर्किड",
      "description": "Laelia anceps मेक्सिको और मध्य अमेरिका की एक एपिफाइटिक ऑर्किड है। यह सर्दियों में खिलती है और तेज़ रोशनी पसंद करती है।"
    },
    "zh": {
      "common_name": "冬花卡特兰",
      "description": "Laelia anceps 原产于墨西哥和中美洲，是一种附生兰花。它在冬季开花，喜明亮散射光。"
    },
    "ja": {
      "common_name": "ラエリア・アンセプス",
      "description": "ラエリア・アンセプスはメキシコ原産の着生ランです。冬に開花し、明るい環境を好みます。"
    },
    "ko": {
      "common_name": "라엘리아 안셉스",
      "description": "Laelia anceps는 멕시코와 중미 원산의 착생 난초입니다. 겨울에 꽃이 피며 밝은 빛을 선호합니다."
    }
  },
