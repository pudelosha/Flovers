  "translations": {
    "en": {
      "common_name": "Soapwort",
      "description": "Soapwort is a hardy herb known for its soap-like qualities. It thrives in moderate conditions and requires basic care."
    },
    "pl": {
      "common_name": "Mydlnica lekarska",
      "description": "Mydlnica lekarska to wytrzymała roślina znana ze swoich właściwości mydlnych. Rośnie w umiarkowanych warunkach i wymaga podstawowej opieki."
    },
    "de": {
      "common_name": "Seifenkraut",
      "description": "Seifenkraut ist ein robuster Kraut, bekannt für seine seifenähnlichen Eigenschaften. Es gedeiht unter mäßigen Bedingungen und benötigt einfache Pflege."
    },
    "fr": {
      "common_name": "Saponaire officinale",
      "description": "La saponaire officinale est une plante robuste, connue pour ses propriétés savonneuses. Elle prospère dans des conditions modérées et nécessite des soins de base."
    },
    "it": {
      "common_name": "Saponaria officinale",
      "description": "La saponaria officinale è un'erba resistente, conosciuta per le sue qualità simili al sapone. Cresce in condizioni moderate e richiede cure basilari."
    },
    "es": {
      "common_name": "Saponaria officinalis",
      "description": "La saponaria officinalis es una hierba resistente conocida por sus cualidades similares al jabón. Crece en condiciones moderadas y requiere cuidados básicos."
    },
    "pt": {
      "common_name": "Saponária",
      "description": "A saponária é uma erva resistente conhecida pelas suas qualidades semelhantes ao sabão. Cresce em condições moderadas e requer cuidados básicos."
    },
    "ar": {
      "common_name": "سابوناريا",
      "description": "السابوناريا هي عشبة قوية معروفة بخصائصها الشبيهة بالصابون. تنمو في ظروف معتدلة وتتطلب رعاية أساسية."
    },
    "hi": {
      "common_name": "सोपवर्ट",
      "description": "सोपवर्ट एक कठोर पौधा है जिसे साबुन जैसे गुणों के लिए जाना जाता है। यह मध्यम परिस्थितियों में उगता है और बुनियादी देखभाल की आवश्यकता होती है।"
    },
    "zh": {
      "common_name": "皂草",
      "description": "皂草是一种坚韧的草本植物，以其类似肥皂的特性而闻名。它在适度的条件下茁壮成长，需要基本的护理。"
    },
    "ja": {
      "common_name": "ソープワート",
      "description": "ソープワートは、その石鹸のような特性で知られる丈夫なハーブです。中程度の条件で育ち、基本的な手入れが必要です。"
    },
    "ko": {
      "common_name": "비누풀",
      "description": "비누풀은 비누와 유사한 특성으로 알려진 강한 허브입니다. 중간 정도의 조건에서 자라며 기본적인 관리가 필요합니다."
    }
  },
