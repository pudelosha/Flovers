  "translations": {
    "en": {
      "common_name": "Sage",
      "description": "Sage is a popular herb known for its aromatic leaves. It thrives in full sun and well-drained soil."
    },
    "pl": {
      "common_name": "Szałwia",
      "description": "Szałwia to popularna roślina z aromatycznymi liśćmi. Wymaga pełnego słońca i dobrze przepuszczalnej gleby."
    },
    "de": {
      "common_name": "Salbei",
      "description": "Salbei ist ein beliebtes Kraut mit aromatischen Blättern. Es gedeiht in voller Sonne und gut durchlässigem Boden."
    },
    "fr": {
      "common_name": "Sauge",
      "description": "La sauge est une herbe populaire avec des feuilles aromatiques. Elle préfère le plein soleil et un sol bien drainé."
    },
    "it": {
      "common_name": "Salvia",
      "description": "La salvia è un'erba popolare con foglie aromatiche. Cresce bene al sole pieno e in terreni ben drenati."
    },
    "es": {
      "common_name": "Salvia",
      "description": "La salvia es una hierba popular con hojas aromáticas. Crece bien a pleno sol y en suelos bien drenados."
    },
    "pt": {
      "common_name": "Sálvia",
      "description": "A sálvia é uma erva popular com folhas aromáticas. Cresce bem em pleno sol e em solo bem drenado."
    },
    "ar": {
      "common_name": "المريمية",
      "description": "المريمية هي عشبة شائعة ذات أوراق عطرية. تنمو في الشمس الكاملة والتربة جيدة التصريف."
    },
    "hi": {
      "common_name": "शहतूत",
      "description": "शहतूत एक लोकप्रिय जड़ी-बूटी है जिसमें सुगंधित पत्तियाँ होती हैं। यह पूर्ण सूर्य और अच्छी जल निकासी वाली मिट्टी में उगती है।"
    },
    "zh": {
      "common_name": "鼠尾草",
      "description": "鼠尾草是一种常见的草本植物，具有芳香的叶子。它在充足的阳光和良好的排水土壤中茁壮成长。"
    },
    "ja": {
      "common_name": "セージ",
      "description": "セージは香り高い葉を持つ人気のあるハーブです。日当たりの良い場所と排水の良い土壌で育ちます。"
    },
    "ko": {
      "common_name": "세이지",
      "description": "세이지는 향기로운 잎을 가진 인기 있는 허브입니다. 완전한 햇빛과 배수가 좋은 토양에서 잘 자랍니다."
    }
  },
