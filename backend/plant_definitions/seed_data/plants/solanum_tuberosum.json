  "translations": {
    "en": {
      "common_name": "Potato",
      "description": "The potato is a starchy tuber from the plant Solanum tuberosum. It's widely grown and consumed globally."
    },
    "pl": {
      "common_name": "Ziemniak",
      "description": "Ziemniak to skrobiowa bulwa rośliny Solanum tuberosum. Jest szeroko uprawiany i spożywany na całym świecie."
    },
    "de": {
      "common_name": "Kartoffel",
      "description": "Die Kartoffel ist eine stärkehaltige Knolle der Pflanze Solanum tuberosum. Sie wird weltweit angebaut und verzehrt."
    },
    "fr": {
      "common_name": "Pomme de terre",
      "description": "La pomme de terre est un tubercule amidonné de la plante Solanum tuberosum. Elle est cultivée et consommée dans le monde entier."
    },
    "it": {
      "common_name": "Patata",
      "description": "La patata è un tubero amidaceo della pianta Solanum tuberosum. È ampiamente coltivata e consumata in tutto il mondo."
    },
    "es": {
      "common_name": "Papa",
      "description": "La papa es un tubérculo con almidón de la planta Solanum tuberosum. Se cultiva y consume ampliamente en todo el mundo."
    },
    "pt": {
      "common_name": "Batata",
      "description": "A batata é um tubérculo rico em amido da planta Solanum tuberosum. É amplamente cultivada e consumida em todo o mundo."
    },
    "ar": {
      "common_name": "بطاطا",
      "description": "البطاطا هي درنة نشوية من نبات Solanum tuberosum. تزرع وتستهلك على نطاق واسع في جميع أنحاء العالم."
    },
    "hi": {
      "common_name": "आलू",
      "description": "आलू एक स्टार्ची कंद है जो पौधे Solanum tuberosum से आता है। यह दुनिया भर में उगाया और खाया जाता है।"
    },
    "zh": {
      "common_name": "土豆",
      "description": "土豆是来自 Solanum tuberosum 植物的含淀粉块茎。它在全球范围内广泛种植和消费。"
    },
    "ja": {
      "common_name": "ジャガイモ",
      "description": "ジャガイモは、植物Solanum tuberosumからのでんぷん質の塊茎です。世界中で広く栽培され、消費されています。"
    },
    "ko": {
      "common_name": "감자",
      "description": "감자는 Solanum tuberosum 식물의 전분 덩어리입니다. 전 세계적으로 널리 재배되고 소비됩니다."
    }
  },
