  "translations": {
    "en": {
      "common_name": "Mistletoe",
      "description": "Mistletoe is a parasitic plant found on trees. It has a slow growth rate and requires moderate care."
    },
    "pl": {
      "common_name": "Jemioła",
      "description": "Jemioła to roślina pasożytnicza, która rośnie na drzewach. Charakteryzuje się wolnym tempem wzrostu i umiarkowaną pielęgnacją."
    },
    "de": {
      "common_name": "Mistel",
      "description": "Die Mistel ist eine parasitäre Pflanze, die auf Bäumen wächst. Sie hat ein langsames Wachstum und benötigt mäßige Pflege."
    },
    "fr": {
      "common_name": "Gui",
      "description": "Le gui est une plante parasitaire trouvée sur les arbres. Elle a un taux de croissance lent et nécessite des soins modérés."
    },
    "it": {
      "common_name": "Vischio",
      "description": "Il vischio è una pianta parassitaria che cresce sugli alberi. Ha una crescita lenta e richiede cure moderate."
    },
    "es": {
      "common_name": "Muérdago",
      "description": "El muérdago es una planta parasitaria que crece sobre los árboles. Tiene un crecimiento lento y requiere cuidados moderados."
    },
    "pt": {
      "common_name": "Visco",
      "description": "O visco é uma planta parasitária encontrada em árvores. Tem uma taxa de crescimento lenta e exige cuidados moderados."
    },
    "ar": {
      "common_name": "البلوط",
      "description": "البلوط هو نبات طفيلي ينمو على الأشجار. لديه معدل نمو بطيء ويتطلب رعاية معتدلة."
    },
    "hi": {
      "common_name": "तुलसी",
      "description": "तुलसी एक परजीवी पौधा है जो पेड़ों पर उगता है। इसका विकास धीमा है और इसे मध्यम देखभाल की आवश्यकता है।"
    },
    "zh": {
      "common_name": "槲寄生",
      "description": "槲寄生是一种寄生植物，生长在树木上。它的生长速度较慢，需要适度的护理。"
    },
    "ja": {
      "common_name": "ヤドリギ",
      "description": "ヤドリギは木に生える寄生植物です。成長が遅く、適度なケアが必要です。"
    },
    "ko": {
      "common_name": "겨우살이",
      "description": "겨우살이는 나무에 자라는 기생 식물입니다. 성장 속도가 느리고 적당한 관리가 필요합니다."
    }
  },
