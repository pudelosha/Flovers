  "translations": {
    "en": {
      "common_name": "Purple Shamrock",
      "description": "A striking plant with purple, heart-shaped leaves and small white flowers. Known for its easy care and vibrant foliage."
    },
    "pl": {
      "common_name": "Fiołek Trójkątny",
      "description": "Roślina o purpurowych, sercowatych liściach i drobnych białych kwiatach. Łatwa w pielęgnacji."
    },
    "de": {
      "common_name": "Lila Glücksklee",
      "description": "Eine Pflanze mit lila, herzförmigen Blättern und kleinen weißen Blumen. Einfach zu pflegen."
    },
    "fr": {
      "common_name": "Trèfle pourpre",
      "description": "Plante à feuilles en forme de cœur de couleur pourpre et petites fleurs blanches. Facile à entretenir."
    },
    "it": {
      "common_name": "Trifoglio Viola",
      "description": "Una pianta con foglie a forma di cuore di colore viola e piccoli fiori bianchi. Facile da curare."
    },
    "es": {
      "common_name": "Trébol Púrpura",
      "description": "Planta con hojas en forma de corazón de color púrpura y pequeñas flores blancas. Fácil de cuidar."
    },
    "pt": {
      "common_name": "Trevo Roxo",
      "description": "Planta com folhas roxas em forma de coração e pequenas flores brancas. Fácil de cuidar."
    },
    "ar": {
      "common_name": "الكلأ الأرجواني",
      "description": "نبات بأوراق على شكل قلب بلون أرجواني وزهور بيضاء صغيرة. سهل العناية."
    },
    "hi": {
      "common_name": "बैंगनी शेमरॉक",
      "description": "बैंगनी, दिल के आकार की पत्तियों और छोटे सफेद फूलों वाला पौधा। देखभाल में आसान।"
    },
    "zh": {
      "common_name": "紫色三叶草",
      "description": "一种具有紫色心形叶子和小白色花朵的植物。易于照料。"
    },
    "ja": {
      "common_name": "パープルシャムロック",
      "description": "紫色のハート型の葉と小さな白い花を持つ植物。お手入れが簡単です。"
    },
    "ko": {
      "common_name": "보라색 클로버",
      "description": "보라색 하트 모양의 잎과 작은 흰색 꽃을 가진 식물. 관리가 쉽습니다."
    }
  },
