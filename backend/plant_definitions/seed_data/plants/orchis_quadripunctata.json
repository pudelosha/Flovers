  "translations": {
    "en": {
      "common_name": "Four-Spotted Orchid",
      "description": "Orchis quadripunctata is a beautiful orchid native to the Mediterranean region. It produces striking pink flowers with distinct spots."
    },
    "pl": {
      "common_name": "Orchidea czteroplamista",
      "description": "Orchis quadripunctata to piękna orchidea pochodząca z regionu Morza Śródziemnego. Wytwarza uderzające różowe kwiaty z wyraźnymi plamkami."
    },
    "de": {
      "common_name": "Vierfleck-Orchidee",
      "description": "Die Orchis quadripunctata ist eine wunderschöne Orchidee aus dem Mittelmeerraum. Sie produziert auffällige rosa Blüten mit markanten Flecken."
    },
    "fr": {
      "common_name": "Orchidée à quatre taches",
      "description": "Orchis quadripunctata est une magnifique orchidée originaire de la région méditerranéenne. Elle produit des fleurs roses frappantes avec des taches distinctes."
    },
    "it": {
      "common_name": "Orchidea a quattro macchie",
      "description": "Orchis quadripunctata è una bellissima orchidea originaria della regione mediterranea. Produce fiori rosa evidenti con macchie distintive."
    },
    "es": {
      "common_name": "Orquídea de cuatro manchas",
      "description": "Orchis quadripunctata es una hermosa orquídea originaria de la región mediterránea. Produce flores rosadas llamativas con manchas distintivas."
    },
    "pt": {
      "common_name": "Orquídea de quatro manchas",
      "description": "Orchis quadripunctata é uma linda orquídea nativa da região mediterrânea. Produz flores rosadas impressionantes com manchas distintas."
    },
    "ar": {
      "common_name": "أوركيد أربع بقع",
      "description": "Orchis quadripunctata هي أوركيد جميلة موطنها منطقة البحر الأبيض المتوسط. تنتج أزهارًا وردية لافتة للنظر مع بقع مميزة."
    },
    "hi": {
      "common_name": "चार धब्बेदार ऑर्किड",
      "description": "Orchis quadripunctata एक सुंदर ऑर्किड है जो भूमध्यसागरीय क्षेत्र का मूल निवासी है। यह विशेष धब्बों के साथ आकर्षक गुलाबी फूल उत्पन्न करता है."
    },
    "zh": {
      "common_name": "四点兰花",
      "description": "Orchis quadripunctata 是一种美丽的兰花，原产于地中海地区。它开出具有明显斑点的粉色花朵。"
    },
    "ja": {
      "common_name": "四斑蘭",
      "description": "Orchis quadripunctata は地中海地域原産の美しいランです。特徴的な斑点のあるピンクの花を咲かせます。"
    },
    "ko": {
      "common_name": "네 점 난초",
      "description": "Orchis quadripunctata는 지중해 지역 원산지의 아름다운 난초입니다. 눈에 띄는 반점이 있는 분홍색 꽃을 피웁니다."
    }
  },
