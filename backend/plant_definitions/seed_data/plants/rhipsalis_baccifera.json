  "translations": {
    "en": {
      "common_name": "Mistletoe Cactus",
      "description": "Rhipsalis baccifera is an epiphytic cactus native to tropical regions. It has slender, pendulous stems and small white berries."
    },
    "pl": {
      "common_name": "Kaktus jemiołowy",
      "description": "Rhipsalis baccifera to epifitowy kaktus występujący w tropikach. Ma cienkie, zwisające łodygi i małe białe jagody."
    },
    "de": {
      "common_name": "Mistelnkaktus",
      "description": "Rhipsalis baccifera ist ein epiphytischer Kaktus aus tropischen Regionen. Er hat dünne, hängende Stängel und kleine weiße Beeren."
    },
    "fr": {
      "common_name": "Cactus de gui",
      "description": "Rhipsalis baccifera est un cactus épiphyte originaire des régions tropicales. Il possède des tiges fines et pendantes avec de petites baies blanches."
    },
    "it": {
      "common_name": "Cactus vischio",
      "description": "Rhipsalis baccifera è un cactus epifita originario delle regioni tropicali. Ha fusti sottili e penduli con piccole bacche bianche."
    },
    "es": {
      "common_name": "Cactus muérdago",
      "description": "Rhipsalis baccifera es un cactus epífito originario de las regiones tropicales. Tiene tallos delgados y colgantes con pequeñas bayas blancas."
    },
    "pt": {
      "common_name": "Cacto visco",
      "description": "Rhipsalis baccifera é um cacto epífito nativo das regiões tropicais. Possui caules finos e pendentes, com pequenas frutas brancas."
    },
    "ar": {
      "common_name": "صبار الدبق",
      "description": "Rhipsalis baccifera هو صبار نباتي نشأ في المناطق الاستوائية. له سيقان رقيقة ومتدلية وثمار بيضاء صغيرة."
    },
    "hi": {
      "common_name": "मिस्टलेटो कैक्टस",
      "description": "Rhipsalis baccifera एक एपिफाइटिक कैक्टस है जो उष्णकटिबंधीय क्षेत्रों का मूल निवासी है। इसके पतले, लटकते हुए तने होते हैं और छोटे सफेद बेरी होते हैं।"
    },
    "zh": {
      "common_name": "槲寄生仙人掌",
      "description": "Rhipsalis baccifera 是一种原产于热带地区的附生仙人掌。它有细长的悬挂茎和小白色浆果。"
    },
    "ja": {
      "common_name": "ミスルトウサボテン",
      "description": "Rhipsalis baccifera は熱帯地方に自生する着生サボテンです。細く垂れ下がる茎と小さな白い実を持っています。"
    },
    "ko": {
      "common_name": "겨우살이 선인장",
      "description": "Rhipsalis baccifera는 열대 지역 원산지의 착생 선인장입니다. 가늘고 처진 줄기와 작은 흰색 열매를 가지고 있습니다."
    }
  },
