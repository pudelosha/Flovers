  "translations": {
    "en": {
      "common_name": "Scarlet Runner Bean",
      "description": "Phaseolus coccineus is a fast-growing climbing bean. It produces vibrant red flowers and edible beans."
    },
    "pl": {
      "common_name": "Fasola czerwona",
      "description": "Phaseolus coccineus to szybko rosnąca roślina pnąca. Wytwarza intensywnie czerwone kwiaty i jadalne fasole."
    },
    "de": {
      "common_name": "Rote Bohnenranke",
      "description": "Phaseolus coccineus ist eine schnell wachsende Kletterbohne. Sie produziert leuchtend rote Blüten und essbare Bohnen."
    },
    "fr": {
      "common_name": "Haricot Grimpant Rouge",
      "description": "Phaseolus coccineus est une plante grimpante à croissance rapide. Elle produit des fleurs rouges vives et des haricots comestibles."
    },
    "it": {
      "common_name": "Fagiolo rampicante rosso",
      "description": "Phaseolus coccineus è una pianta rampicante a crescita rapida. Produce fiori rossi vivaci e fagioli commestibili."
    },
    "es": {
      "common_name": "Frijol trepador escarlata",
      "description": "Phaseolus coccineus es una planta trepadora de rápido crecimiento. Produce flores rojas brillantes y frijoles comestibles."
    },
    "pt": {
      "common_name": "Feijão trepador escarlate",
      "description": "Phaseolus coccineus é uma planta trepadora de crescimento rápido. Produz flores vermelhas vibrantes e feijões comestíveis."
    },
    "ar": {
      "common_name": "فاصوليا متسلقة حمراء",
      "description": "Phaseolus coccineus هو نبات متسلق سريع النمو. ينتج أزهار حمراء زاهية وفاصوليا صالحة للأكل."
    },
    "hi": {
      "common_name": "लाल चढ़ाई बीन्स",
      "description": "Phaseolus coccineus एक तेज़ी से बढ़ने वाली चढ़ाई वाली बीन्स है। यह चमकीले लाल फूल और खाने योग्य बीन्स उत्पन्न करती है।"
    },
    "zh": {
      "common_name": "红色爬豆",
      "description": "Phaseolus coccineus 是一种快速生长的爬藤豆类植物。它开出鲜艳的红花，并结出可食用的豆类。"
    },
    "ja": {
      "common_name": "スカーレットランナービーン",
      "description": "Phaseolus coccineus は、速く成長する登攀植物です。鮮やかな赤い花を咲かせ、食用の豆を実らせます。"
    },
    "ko": {
      "common_name": "스칼렛 러너 빈",
      "description": "Phaseolus coccineus는 빠르게 자라는 덩굴콩입니다. 선명한 빨간 꽃과 식용 콩을 생산합니다."
    }
  },
