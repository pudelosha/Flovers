  "translations": {
    "en": {
      "common_name": "Summer Savory",
      "description": "Summer savory is an aromatic herb, commonly used in culinary dishes. It thrives in sunny, well-drained soils."
    },
    "pl": {
      "common_name": "Majeranek letni",
      "description": "Majeranek letni to aromatyczna roślina stosowana w kuchni. Wymaga słonecznych, dobrze przepuszczalnych gleb."
    },
    "de": {
      "common_name": "Sommerbohnenkraut",
      "description": "Sommerbohnenkraut ist ein aromatisches Kraut, das häufig in der Küche verwendet wird. Es gedeiht in sonnigen, gut durchlässigen Böden."
    },
    "fr": {
      "common_name": "Sarriette d'été",
      "description": "La sarriette d'été est une herbe aromatique, utilisée en cuisine. Elle prospère dans des sols bien drainés et ensoleillés."
    },
    "it": {
      "common_name": "Santoreggia",
      "description": "La santoreggia è un'erba aromatica utilizzata in cucina. Cresce in terreni ben drenati e soleggiati."
    },
    "es": {
      "common_name": "Satureja",
      "description": "La satureja es una hierba aromática utilizada en la cocina. Crece en suelos bien drenados y soleados."
    },
    "pt": {
      "common_name": "Satureia",
      "description": "A satureia é uma erva aromática usada na culinária. Desenvolve-se em solos bem drenados e ensolarados."
    },
    "ar": {
      "common_name": "زعتر صيفي",
      "description": "الزعتر الصيفي هو عشب عطري يُستخدم في الطهي. يزدهر في التربة جيدة التصريف والمشمسة."
    },
    "hi": {
      "common_name": "गर्मी का सैवोरी",
      "description": "गर्मी का सैवोरी एक खुशबूदार जड़ी-बूटी है, जिसका उपयोग रसोई में किया जाता है। यह धूप वाली, अच्छी जल निकासी वाली मिट्टी में उगती है।"
    },
    "zh": {
      "common_name": "夏季香草",
      "description": "夏季香草是一种芳香的草本植物，常用于烹饪。它在阳光充足、排水良好的土壤中生长良好。"
    },
    "ja": {
      "common_name": "夏のサバリー",
      "description": "夏のサバリーは香り高いハーブで、料理に使われます。日当たりが良く、排水の良い土壌で育ちます。"
    },
    "ko": {
      "common_name": "여름 세이보리",
      "description": "여름 세이보리는 요리에 자주 사용되는 향기로운 허브입니다. 햇볕이 잘 드는 배수가 좋은 토양에서 자랍니다."
    }
  },
