  "translations": {
    "en": {
      "common_name": "Common privet",
      "description": "Ligustrum vulgare is a deciduous to semi-evergreen shrub native to Europe and western Asia. It is commonly used for hedges due to its dense growth and tolerance of pruning."
    },
    "pl": {
      "common_name": "Ligustr pospolity",
      "description": "Ligustrum vulgare to krzew liściasty lub półzimozielony pochodzący z Europy. Jest często sadzony na żywopłoty ze względu na szybki wzrost."
    },
    "de": {
      "common_name": "Gewöhnlicher Liguster",
      "description": "Ligustrum vulgare ist ein laubabwerfender Strauch aus Europa. Er wird häufig als Heckenpflanze verwendet und ist sehr schnittverträglich."
    },
    "fr": {
      "common_name": "Troène commun",
      "description": "Ligustrum vulgare est un arbuste originaire d’Europe. Il est apprécié pour les haies grâce à sa croissance dense et rapide."
    },
    "it": {
      "common_name": "Ligustro comune",
      "description": "Ligustrum vulgare è un arbusto europeo molto resistente. Viene spesso usato come siepe ornamentale."
    },
    "es": {
      "common_name": "Aligustre común",
      "description": "Ligustrum vulgare es un arbusto originario de Europa. Se utiliza ampliamente en setos por su rápido crecimiento."
    },
    "pt": {
      "common_name": "Alfeneiro-comum",
      "description": "Ligustrum vulgare é um arbusto europeu resistente. É muito usado em sebes devido à sua densidade."
    },
    "ar": {
      "common_name": "الليغستروم الشائع",
      "description": "Ligustrum vulgare شجيرة أصلها أوروبا. تُستخدم غالبًا في الأسيجة لقوتها وسرعة نموها."
    },
    "hi": {
      "common_name": "कॉमन प्रिवेट",
      "description": "Ligustrum vulgare यूरोप का एक झाड़ीदार पौधा है। इसे अक्सर बाड़ के रूप में लगाया जाता है।"
    },
    "zh": {
      "common_name": "欧洲女贞",
      "description": "Ligustrum vulgare 原产于欧洲，是一种常见的绿篱灌木。它生长快且耐修剪。"
    },
    "ja": {
      "common_name": "セイヨウイボタ",
      "description": "Ligustrum vulgareはヨーロッパ原産の低木です。生垣としてよく利用されます。"
    },
    "ko": {
      "common_name": "서양쥐똥나무",
      "description": "Ligustrum vulgare는 유럽 원산의 관목입니다. 생울타리로 널리 사용됩니다."
    }
  },
