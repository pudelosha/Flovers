  "translations": {
    "en": {
      "common_name": "Holly",
      "description": "Holly is a popular evergreen shrub or tree with spiny, glossy leaves and bright red berries. It is often used in winter decorations and as a hedge."
    },
    "pl": {
      "common_name": "Ostrokrzew",
      "description": "Ostrokrzew to popularny krzew lub drzewo z wiecznie zielonymi liśćmi i błyszczącymi, kolczastymi liśćmi oraz czerwonymi jagodami."
    },
    "de": {
      "common_name": "Stechpalme",
      "description": "Die Stechpalme ist ein beliebter immergrüner Strauch oder Baum mit stacheligen, glänzenden Blättern und roten Beeren."
    },
    "fr": {
      "common_name": "Houx",
      "description": "Le houx est un arbuste ou arbre à feuilles persistantes avec des feuilles brillantes et épineuses et des baies rouges vives."
    },
    "it": {
      "common_name": "Agrifoglio",
      "description": "L'agrifoglio è un popolare arbusto o albero sempreverde con foglie lucide e spine e bacche rosse."
    },
    "es": {
      "common_name": "Acebo",
      "description": "El acebo es un arbusto o árbol siempreverde con hojas espinosas y brillantes y bayas rojas."
    },
    "pt": {
      "common_name": "Azevinho",
      "description": "O azevinho é um arbusto ou árvore popular de folhas perenes, com folhas brilhantes e espinhosas e bagas vermelhas."
    },
    "ar": {
      "common_name": "الهدب",
      "description": "الهدب هو شجيرة أو شجرة دائمة الخضرة بأوراق لامعة وشائكة وثمار حمراء."
    },
    "hi": {
      "common_name": "हॉली",
      "description": "हॉली एक लोकप्रिय सदाबहार झाड़ी या पेड़ है जिसमें कांटेदार, चमकदार पत्तियाँ और उज्ज्वल लाल बेरी होती है।"
    },
    "zh": {
      "common_name": "冬青",
      "description": "冬青是一种常绿灌木或树木，具有刺状、光泽的叶子和鲜艳的红色浆果。"
    },
    "ja": {
      "common_name": "ヒイラギ",
      "description": "ヒイラギは、鋭い光沢のある葉と鮮やかな赤いベリーを持つ人気のある常緑樹木です。"
    },
    "ko": {
      "common_name": "호랑가시나무",
      "description": "호랑가시나무는 가시가 있는 반짝이는 잎과 밝은 빨간색 열매를 가진 인기 있는 상록수입니다."
    }
  },
