  "translations": {
    "en": {
      "common_name": "Dahlia",
      "description": "Dahlia pinnata is a tuberous flowering plant grown for its large, colorful blooms. It prefers full sun, rich soil, and regular watering during the growing season."
    },
    "pl": {
      "common_name": "Dalia",
      "description": "Dahlia pinnata to bulwiasta roślina ozdobna o dużych kwiatach. Wymaga pełnego słońca, żyznej gleby i regularnego podlewania."
    },
    "de": {
      "common_name": "Dahlie",
      "description": "Dahlia pinnata ist eine Knollenpflanze mit auffälligen Blüten. Sie wächst am besten in voller Sonne und nährstoffreicher, gut drainierter Erde."
    },
    "fr": {
      "common_name": "Dahlia",
      "description": "Dahlia pinnata est une plante à tubercules appréciée pour ses fleurs voyantes. Elle demande du soleil et un arrosage régulier en saison."
    },
    "it": {
      "common_name": "Dalia",
      "description": "Dahlia pinnata è una pianta tuberosa ornamentale dai fiori grandi. Predilige pieno sole, terreno fertile e annaffiature regolari."
    },
    "es": {
      "common_name": "Dalia",
      "description": "Dahlia pinnata es una planta tuberosa muy ornamental. Necesita sol pleno, suelo rico y riego frecuente durante el crecimiento."
    },
    "pt": {
      "common_name": "Dália",
      "description": "Dahlia pinnata é uma planta tuberosa conhecida pelas flores vistosas. Requer sol pleno, solo fértil e rega regular."
    },
    "ar": {
      "common_name": "الداليا",
      "description": "نبات الداليا ذو درنات مزهرة بألوان زاهية. يحتاج إلى شمس كاملة وتربة غنية وري منتظم."
    },
    "hi": {
      "common_name": "डाहलिया",
      "description": "डाहलिया पिन्नाटा एक कंदीय पुष्पीय पौधा है। इसे भरपूर धूप, उपजाऊ मिट्टी और नियमित पानी की आवश्यकता होती है।"
    },
    "zh": {
      "common_name": "大丽花",
      "description": "大丽花是一种块根开花植物，以花色丰富著称。生长期需要充足阳光和定期浇水。"
    },
    "ja": {
      "common_name": "ダリア",
      "description": "ダリアは球根性の花卉で、大きな花が特徴です。日当たりと肥沃な土、水やりが重要です。"
    },
    "ko": {
      "common_name": "달리아",
      "description": "달리아는 화려한 꽃을 피우는 구근 식물입니다. 충분한 햇빛과 비옥한 토양, 규칙적인 물주기가 필요합니다."
    }
  },
