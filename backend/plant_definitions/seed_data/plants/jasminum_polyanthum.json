  "translations": {
    "en": {
      "common_name": "Pink jasmine",
      "description": "Jasminum polyanthum is a fast-growing evergreen climber known for fragrant pink buds and white flowers. It prefers bright light and evenly moist soil during active growth."
    },
    "pl": {
      "common_name": "Jaśmin różowy",
      "description": "Jasminum polyanthum to szybko rosnące pnącze o pachnących różowych pąkach i białych kwiatach. Lubi jasne stanowiska i umiarkowanie wilgotną glebę."
    },
    "de": {
      "common_name": "Rosa Jasmin",
      "description": "Jasminum polyanthum ist eine schnell wachsende Kletterpflanze mit duftenden Blüten. Sie benötigt viel Licht und gleichmäßig feuchte Erde."
    },
    "fr": {
      "common_name": "Jasmin rose",
      "description": "Jasminum polyanthum est une plante grimpante vigoureuse aux fleurs très parfumées. Elle apprécie une lumière vive et un arrosage régulier."
    },
    "it": {
      "common_name": "Gelsomino rosa",
      "description": "Jasminum polyanthum è un rampicante sempreverde dai fiori profumati. Cresce bene con molta luce e terreno mantenuto umido."
    },
    "es": {
      "common_name": "Jazmín rosa",
      "description": "Jasminum polyanthum es una trepadora de rápido crecimiento con flores fragantes. Requiere buena luz y riego regular en crecimiento."
    },
    "pt": {
      "common_name": "Jasmim-rosa",
      "description": "Jasminum polyanthum é uma trepadeira vigorosa com flores perfumadas. Prefere luz intensa e solo levemente úmido."
    },
    "ar": {
      "common_name": "الياسمين الوردي",
      "description": "الياسمين الوردي نبات متسلق سريع النمو ذو أزهار عطرية. يحتاج إلى ضوء جيد وري منتظم."
    },
    "hi": {
      "common_name": "पिंक जैस्मिन",
      "description": "Jasminum polyanthum एक तेज़ी से बढ़ने वाली बेल है जिसमें सुगंधित फूल होते हैं। इसे तेज़ रोशनी और नियमित पानी पसंद है।"
    },
    "zh": {
      "common_name": "粉红茉莉",
      "description": "Jasminum polyanthum 是生长迅速的攀援植物，花香浓郁。需要充足光照和均匀浇水。"
    },
    "ja": {
      "common_name": "ピンクジャスミン",
      "description": "Jasminum polyanthum は香りの強い花を咲かせるつる性植物です。明るい場所と適度な水やりを好みます。"
    },
    "ko": {
      "common_name": "핑크 자스민",
      "description": "Jasminum polyanthum은 향기로운 꽃을 가진 빠르게 자라는 덩굴식물입니다. 밝은 빛과 규칙적인 물주기가 필요합니다."
    }
  },
