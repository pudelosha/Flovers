  "translations": {
    "en": {
      "common_name": "Cast Iron Plant",
      "description": "Aspidistra elatior is a durable houseplant known for its tolerance of low light and neglect. It has dark green, leathery leaves and grows slowly. It is commonly used indoors."
    },
    "pl": {
      "common_name": "Aspidistra wyniosła",
      "description": "Aspidistra elatior to bardzo wytrzymała roślina doniczkowa tolerująca cień i zaniedbanie. Ma ciemnozielone, skórzaste liście. Rośnie powoli w warunkach domowych."
    },
    "de": {
      "common_name": "Schusterpalme",
      "description": "Aspidistra elatior ist eine robuste Zimmerpflanze, die wenig Licht verträgt. Sie besitzt ledrige, dunkelgrüne Blätter. Das Wachstum ist langsam."
    },
    "fr": {
      "common_name": "Plante de fonte",
      "description": "Aspidistra elatior est une plante d'intérieur très résistante supportant peu de lumière. Elle a des feuilles vert foncé et coriaces. Sa croissance est lente."
    },
    "it": {
      "common_name": "Pianta di ghisa",
      "description": "Aspidistra elatior è una pianta da interno molto resistente e tollerante alla scarsa luce. Presenta foglie verdi e coriacee. Cresce lentamente."
    },
    "es": {
      "common_name": "Planta de hierro",
      "description": "Aspidistra elatior es una planta de interior resistente que tolera poca luz. Tiene hojas verdes oscuras y firmes. Su crecimiento es lento."
    },
    "pt": {
      "common_name": "Planta de ferro",
      "description": "Aspidistra elatior é uma planta de interior muito resistente à pouca luz. Possui folhas verdes escuras e coriáceas. O crescimento é lento."
    },
    "ar": {
      "common_name": "نبات الحديد",
      "description": "Aspidistra elatior نبات منزلي شديد التحمل وقادر على العيش في إضاءة منخفضة. أوراقه خضراء داكنة وسميكة. نموه بطيء."
    },
    "hi": {
      "common_name": "कास्ट आयरन प्लांट",
      "description": "Aspidistra elatior एक मजबूत घरेलू पौधा है जो कम रोशनी सहन करता है। इसकी पत्तियाँ गहरे हरे रंग की होती हैं। इसकी वृद्धि धीमी होती है।"
    },
    "zh": {
      "common_name": "蜘蛛抱蛋",
      "description": "Aspidistra elatior 是一种非常耐阴的室内植物。叶片深绿色且坚韧。生长速度缓慢。"
    },
    "ja": {
      "common_name": "ハラン",
      "description": "Aspidistra elatior は耐陰性の高い丈夫な観葉植物です。濃い緑色の葉を持ちます。成長はゆっくりです。"
    },
    "ko": {
      "common_name": "아스피디스트라",
      "description": "Aspidistra elatior 는 약한 빛에서도 잘 자라는 튼튼한 실내 식물입니다. 잎은 진한 녹색입니다. 성장 속도는 느립니다."
    }
  },
