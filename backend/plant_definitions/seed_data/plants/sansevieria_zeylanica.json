  "translations": {
    "en": {
      "common_name": "Ceylon Bowstring Hemp",
      "description": "Sansevieria zeylanica is a hardy, low-maintenance plant. It thrives in bright, indirect light and requires infrequent watering."
    },
    "pl": {
      "common_name": "Lina Cejlońska",
      "description": "Sansevieria zeylanica to roślina odporna na zaniedbanie. Preferuje jasne, pośrednie światło i rzadkie podlewanie."
    },
    "de": {
      "common_name": "Ceylon-Bogenhanf",
      "description": "Sansevieria zeylanica ist eine pflegeleichte Pflanze. Sie gedeiht bei hellem, indirektem Licht und benötigt nur wenig Wasser."
    },
    "fr": {
      "common_name": "Chanvre de Ceylan",
      "description": "Sansevieria zeylanica est une plante résistante. Elle préfère la lumière indirecte et un arrosage modéré."
    },
    "it": {
      "common_name": "Canapa di Ceylon",
      "description": "Sansevieria zeylanica è una pianta resistente. Predilige la luce indiretta e richiede poca acqua."
    },
    "es": {
      "common_name": "Cáñamo de Ceilán",
      "description": "Sansevieria zeylanica es una planta resistente. Prefiere luz indirecta y riego poco frecuente."
    },
    "pt": {
      "common_name": "Cânhamo de Ceilão",
      "description": "Sansevieria zeylanica é uma planta resistente. Prefere luz indireta e rega moderada."
    },
    "ar": {
      "common_name": "حبل قوس سيلان",
      "description": "Sansevieria zeylanica هي نبتة مقاومة. تفضل الضوء غير المباشر والري المعتدل."
    },
    "hi": {
      "common_name": "सीलोन बोवस्ट्रिंग हैम्प",
      "description": "Sansevieria zeylanica एक मजबूत पौधा है। यह अप्रत्यक्ष प्रकाश पसंद करता है और कम पानी की आवश्यकता होती है।"
    },
    "zh": {
      "common_name": "锡兰弓弦麻",
      "description": "Sansevieria zeylanica 是一种坚韧的植物。它喜欢间接光，并且需要较少的水分。"
    },
    "ja": {
      "common_name": "セイロンボウストリングヘンプ",
      "description": "Sansevieria zeylanica は手入れが簡単な植物です。間接光を好み、少ない水分で育ちます。"
    },
    "ko": {
      "common_name": "실론 보우스트링 햄프",
      "description": "Sansevieria zeylanica는 관리가 쉬운 식물입니다. 간접적인 빛을 선호하고 적은 물을 필요로 합니다."
    }
  },
