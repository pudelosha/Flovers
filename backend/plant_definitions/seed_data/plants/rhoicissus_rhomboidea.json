  "translations": {
    "en": {
      "common_name": "Grape Ivy",
      "description": "Rhoicissus rhomboidea is a fast-growing, attractive climber. It thrives in moderate light and requires regular watering."
    },
    "pl": {
      "common_name": "Winorośl liściasta",
      "description": "Rhoicissus rhomboidea to szybko rosnąca roślina pnąca. Rośnie dobrze przy umiarkowanym świetle i wymaga regularnego podlewania."
    },
    "de": {
      "common_name": "Weinblatt-Efeu",
      "description": "Rhoicissus rhomboidea ist eine schnell wachsende Kletterpflanze. Sie gedeiht bei mittlerem Licht und benötigt regelmäßige Bewässerung."
    },
    "fr": {
      "common_name": "Lierre de raisin",
      "description": "Rhoicissus rhomboidea est une plante grimpante à croissance rapide. Elle préfère une lumière modérée et nécessite un arrosage régulier."
    },
    "it": {
      "common_name": "Edera d'uva",
      "description": "Rhoicissus rhomboidea è una rampicante in rapida crescita. Cresce bene alla luce moderata e ha bisogno di irrigazione regolare."
    },
    "es": {
      "common_name": "Hiedra de uva",
      "description": "Rhoicissus rhomboidea es una planta trepadora de rápido crecimiento. Prefiere luz moderada y requiere riego regular."
    },
    "pt": {
      "common_name": "Hera de uva",
      "description": "Rhoicissus rhomboidea é uma trepadeira de crescimento rápido. Prefere luz moderada e precisa de rega regular."
    },
    "ar": {
      "common_name": "لبلاب العنب",
      "description": "Rhoicissus rhomboidea هو نبات متسلق ينمو بسرعة. ينمو جيدًا في الضوء المعتدل ويحتاج إلى الري المنتظم."
    },
    "hi": {
      "common_name": "अंगूर का आइवी",
      "description": "Rhoicissus rhomboidea एक तेज़ी से बढ़ने वाला बेलदार पौधा है। यह मध्यम रोशनी में बढ़ता है और नियमित पानी की आवश्यकता होती है।"
    },
    "zh": {
      "common_name": "葡萄常春藤",
      "description": "Rhoicissus rhomboidea是一种快速生长的攀缘植物。它在中等光照下生长，并需要定期浇水。"
    },
    "ja": {
      "common_name": "グレープアイビー",
      "description": "Rhoicissus rhomboideaは成長が速いつる植物です。中程度の光で成長し、定期的な水やりが必要です。"
    },
    "ko": {
      "common_name": "포도 아이비",
      "description": "Rhoicissus rhomboidea는 빠르게 자라는 덩굴식물입니다. 중간 정도의 빛에서 잘 자라며 정기적인 물주기가 필요합니다."
    }
  },
