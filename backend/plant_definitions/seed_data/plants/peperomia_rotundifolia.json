  "translations": {
    "en": {
      "common_name": "Trailing Jade",
      "description": "Peperomia rotundifolia is a small, trailing plant known for its round, glossy leaves. It thrives in moderate light and prefers well-draining soil."
    },
    "pl": {
      "common_name": "Jade zwisający",
      "description": "Peperomia rotundifolia to mała, zwisająca roślina o okrągłych, błyszczących liściach. Rośnie w umiarkowanym świetle i preferuje dobrze przepuszczalną glebę."
    },
    "de": {
      "common_name": "Hängendes Jadepflänzchen",
      "description": "Peperomia rotundifolia ist eine kleine, hängende Pflanze mit runden, glänzenden Blättern. Sie gedeiht bei mittlerem Licht und bevorzugt gut durchlässigen Boden."
    },
    "fr": {
      "common_name": "Jade traînant",
      "description": "Peperomia rotundifolia est une petite plante grimpante avec des feuilles rondes et brillantes. Elle préfère un éclairage modéré et un sol bien drainé."
    },
    "it": {
      "common_name": "Jade pendente",
      "description": "Peperomia rotundifolia è una piccola pianta rampicante con foglie rotonde e lucide. Cresce con luce moderata e preferisce un terreno ben drenato."
    },
    "es": {
      "common_name": "Jade colgante",
      "description": "Peperomia rotundifolia es una pequeña planta colgante con hojas redondas y brillantes. Prefiere luz moderada y un suelo bien drenado."
    },
    "pt": {
      "common_name": "Jade pendente",
      "description": "Peperomia rotundifolia é uma planta pequena e pendente com folhas redondas e brilhantes. Prefere luz moderada e solo bem drenado."
    },
    "ar": {
      "common_name": "جاد مائل",
      "description": "Peperomia rotundifolia هي نبات صغير مائل ذو أوراق دائرية لامعة. يفضل الضوء المعتدل والتربة جيدة التصريف."
    },
    "hi": {
      "common_name": "लटकता हुआ जेड",
      "description": "Peperomia rotundifolia एक छोटा, लटकता हुआ पौधा है जिसकी गोल, चमकदार पत्तियां होती हैं। यह मध्यम रोशनी में पनपता है और अच्छी तरह से नाली वाली मिट्टी को पसंद करता है।"
    },
    "zh": {
      "common_name": "垂吊玉树",
      "description": "Peperomia rotundifolia 是一种小型垂挂植物，具有圆形光滑的叶子。它喜欢中等光照和排水良好的土壤。"
    },
    "ja": {
      "common_name": "トレーリングジェイド",
      "description": "Peperomia rotundifoliaは、小さな垂れ下がる植物で、丸い光沢のある葉を持っています。適度な光と排水性の良い土壌を好みます。"
    },
    "ko": {
      "common_name": "추적하는 제이드",
      "description": "Peperomia rotundifolia는 작은 덩굴 식물로 둥글고 광택 있는 잎을 가집니다. 이 식물은 적당한 빛과 배수가 잘되는 토양에서 잘 자랍니다."
    }
  },
