  "translations": {
    "en": {
      "common_name": "Oregano",
      "description": "Oregano is a fragrant herb used in cooking and medicine. It thrives in full sunlight and well-drained soil."
    },
    "pl": {
      "common_name": "Oregano",
      "description": "Oregano to aromatyczne zioło wykorzystywane w kuchni i medycynie. Rośnie w pełnym słońcu i dobrze przepuszczalnej glebie."
    },
    "de": {
      "common_name": "Oregano",
      "description": "Oregano ist ein duftendes Kraut, das in der Küche und Medizin verwendet wird. Es gedeiht in voller Sonne und gut durchlässigem Boden."
    },
    "fr": {
      "common_name": "Origan",
      "description": "L'origan est une herbe parfumée utilisée en cuisine et en médecine. Il prospère en plein soleil et dans un sol bien drainé."
    },
    "it": {
      "common_name": "Origano",
      "description": "L'origano è un'erba profumata usata in cucina e in medicina. Cresce al sole e in un terreno ben drenato."
    },
    "es": {
      "common_name": "Orégano",
      "description": "El orégano es una hierba fragante utilizada en la cocina y la medicina. Crece a pleno sol y en suelos bien drenados."
    },
    "pt": {
      "common_name": "Orégão",
      "description": "O orégão é uma erva aromática usada na culinária e na medicina. Cresce em pleno sol e em solo bem drenado."
    },
    "ar": {
      "common_name": "أوريجانو",
      "description": "الأوريجانو هو عشب عطر يستخدم في الطهي والطب. ينمو في الشمس الكاملة وفي التربة جيدة الصرف."
    },
    "hi": {
      "common_name": "ओरेगैनो",
      "description": "ओरेगैनो एक सुगंधित जड़ी-बूटी है जिसका उपयोग खाना पकाने और चिकित्सा में होता है। यह पूरी धूप और अच्छी जल निकासी वाली मिट्टी में उगता है।"
    },
    "zh": {
      "common_name": "牛至",
      "description": "牛至是一种芳香的草本植物，用于烹饪和医学。它在完全阳光和排水良好的土壤中生长。"
    },
    "ja": {
      "common_name": "オレガノ",
      "description": "オレガノは料理や医療に使用される香りの良いハーブです。完全な日光と排水の良い土壌で育ちます。"
    },
    "ko": {
      "common_name": "오레가노",
      "description": "오레가노는 요리와 의학에 사용되는 향긋한 허브입니다. 완전한 햇빛과 잘 배수되는 토양에서 자랍니다."
    }
  },
