  "translations": {
    "en": {
      "common_name": "Oncidium orchid",
      "description": "Oncidium sphacelatum is a species of orchid native to tropical America. It is commonly found growing as an epiphyte in rainforests."
    },
    "pl": {
      "common_name": "Orchidea Oncidium",
      "description": "Oncidium sphacelatum to gatunek storczyka pochodzący z tropikalnej Ameryki. Rośnie głównie jako epifit w lasach deszczowych."
    },
    "de": {
      "common_name": "Oncidium-Orchidee",
      "description": "Oncidium sphacelatum ist eine Orchideenart, die in tropischem Amerika beheimatet ist. Es wächst hauptsächlich als Epiphyt in Regenwäldern."
    },
    "fr": {
      "common_name": "Orchidée Oncidium",
      "description": "Oncidium sphacelatum est une espèce d'orchidée originaire d'Amérique tropicale. Elle pousse principalement en tant qu'épiphyte dans les forêts tropicales."
    },
    "it": {
      "common_name": "Orchidea Oncidium",
      "description": "Oncidium sphacelatum è una specie di orchidea originaria dell'America tropicale. Cresce principalmente come epifita nelle foreste pluviali."
    },
    "es": {
      "common_name": "Orquídea Oncidium",
      "description": "Oncidium sphacelatum es una especie de orquídea originaria de América tropical. Se encuentra principalmente como epífita en los bosques lluviosos."
    },
    "pt": {
      "common_name": "Orquídea Oncidium",
      "description": "Oncidium sphacelatum é uma espécie de orquídea nativa da América tropical. Cresce principalmente como epífita em florestas tropicais."
    },
    "ar": {
      "common_name": "زهرة الأوركيد أونسيديام",
      "description": "Oncidium sphacelatum هي نوع من الأوركيد موطنها أمريكا الاستوائية. تنمو عادة كنوع من النباتات الطفيلية في الغابات الاستوائية."
    },
    "hi": {
      "common_name": "ऑन्सिडियम ऑर्किड",
      "description": "ऑन्सिडियम स्पैसलेटीम एक ऑर्किड प्रजाति है जो उष्णकटिबंधीय अमेरिका का मूल निवासी है। यह सामान्यत: वर्षावनों में एपिफाइट के रूप में पाया जाता है।"
    },
    "zh": {
      "common_name": "Oncidium兰花",
      "description": "Oncidium sphacelatum是一种原产于热带美洲的兰花。它主要作为附生植物生长在热带雨林中。"
    },
    "ja": {
      "common_name": "オンシディウム蘭",
      "description": "Oncidium sphacelatumは熱帯アメリカに自生するランの一種です。主に熱帯雨林で着生植物として生育します。"
    },
    "ko": {
      "common_name": "온시디움 난초",
      "description": "Oncidium sphacelatum은 열대 아메리카 원산의 난초 종입니다. 주로 열대 우림에서 착생 식물로 자랍니다."
    }
  },
