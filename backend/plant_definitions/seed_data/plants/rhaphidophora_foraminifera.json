  "translations": {
    "en": {
      "common_name": "Foraminer's Rhaphidophora",
      "description": "A climbing plant with fenestrated leaves. Prefers bright, indirect light and moderate humidity."
    },
    "pl": {
      "common_name": "Rhaphidophora foraminifera",
      "description": "Roślina wspinająca się o liściach z dziurkami. Preferuje jasne, pośrednie światło i umiarkowaną wilgotność."
    },
    "de": {
      "common_name": "Rhaphidophora foraminifera",
      "description": "Eine kletternde Pflanze mit fenestrierten Blättern. Bevorzugt helles, indirektes Licht und mäßige Luftfeuchtigkeit."
    },
    "fr": {
      "common_name": "Rhaphidophora foraminifera",
      "description": "Plante grimpante avec des feuilles fenestrées. Préfère la lumière indirecte et une humidité modérée."
    },
    "it": {
      "common_name": "Rhaphidophora foraminifera",
      "description": "Una pianta rampicante con foglie fenestrate. Preferisce luce indiretta e umidità moderata."
    },
    "es": {
      "common_name": "Rhaphidophora foraminifera",
      "description": "Planta trepadora con hojas fenestradas. Prefiere luz indirecta y humedad moderada."
    },
    "pt": {
      "common_name": "Rhaphidophora foraminifera",
      "description": "Planta trepadeira com folhas fenestradas. Prefere luz indireta e umidade moderada."
    },
    "ar": {
      "common_name": "Rhaphidophora foraminifera",
      "description": "نبات متسلق ذو أوراق مثقبة. يفضل الضوء غير المباشر والرطوبة المعتدلة."
    },
    "hi": {
      "common_name": "Rhaphidophora foraminifera",
      "description": "एक चढ़ाई करने वाला पौधा जिसकी पत्तियाँ छिद्रित होती हैं। यह उज्ज्वल, अप्रत्यक्ष प्रकाश और मध्यम आर्द्रता पसंद करता है।"
    },
    "zh": {
      "common_name": "Rhaphidophora foraminifera",
      "description": "一种爬藤植物，具有带孔的叶子。喜欢明亮的间接光和适度的湿度。"
    },
    "ja": {
      "common_name": "Rhaphidophora foraminifera",
      "description": "窓のある葉を持つツル植物。明るい間接光と適度な湿度を好む。"
    },
    "ko": {
      "common_name": "Rhaphidophora foraminifera",
      "description": "구멍이 있는 잎을 가진 덩굴 식물. 밝고 간접적인 빛과 적당한 습도를 선호합니다."
    }
