  "translations": {
    "en": {
      "common_name": "Watercress",
      "description": "Watercress is a fast-growing aquatic or semi-aquatic plant native to Europe and Asia. It thrives in cool, flowing water and is commonly grown for its edible leaves."
    },
    "pl": {
      "common_name": "Rzeżucha wodna",
      "description": "Rzeżucha wodna to szybko rosnąca roślina wodna lub bagienna. Preferuje chłodną, czystą wodę i jest uprawiana jako roślina jadalna."
    },
    "de": {
      "common_name": "Brunnenkresse",
      "description": "Brunnenkresse ist eine schnell wachsende Wasserpflanze. Sie wächst in kühlen, fließenden Gewässern und wird als Blattgemüse genutzt."
    },
    "fr": {
      "common_name": "Cresson de fontaine",
      "description": "Le cresson de fontaine est une plante aquatique à croissance rapide. Il pousse dans une eau fraîche et propre et est consommé comme légume-feuille."
    },
    "it": {
      "common_name": "Crescione d'acqua",
      "description": "Il crescione d'acqua è una pianta acquatica a crescita rapida. Predilige acque fresche ed è coltivato per le sue foglie commestibili."
    },
    "es": {
      "common_name": "Berro",
      "description": "El berro es una planta acuática de rápido crecimiento. Se desarrolla en aguas frescas y limpias y se consume como verdura."
    },
    "pt": {
      "common_name": "Agrião",
      "description": "O agrião é uma planta aquática de crescimento rápido. Prefere água corrente fresca e é cultivado como hortaliça."
    },
    "ar": {
      "common_name": "الجرجير المائي",
      "description": "الجرجير المائي نبات مائي سريع النمو. ينمو في مياه باردة ونظيفة ويُستخدم كخضار ورقي."
    },
    "hi": {
      "common_name": "वॉटरक्रेस",
      "description": "वॉटरक्रेस एक तेज़ी से बढ़ने वाला जलीय पौधा है। यह ठंडे और स्वच्छ पानी में उगता है और खाने योग्य होता है।"
    },
    "zh": {
      "common_name": "西洋菜",
      "description": "西洋菜是一种快速生长的水生植物。它生长在清凉的流水中，常作为蔬菜食用。"
    },
    "ja": {
      "common_name": "オランダガラシ",
      "description": "オランダガラシは成長の早い水生植物です。冷たくきれいな水で育ち、食用にされます。"
    },
    "ko": {
      "common_name": "워터크레스",
      "description": "워터크레스는 빠르게 자라는 수생 식물입니다. 서늘하고 깨끗한 물에서 자라며 식용으로 사용됩니다."
    }
  },
