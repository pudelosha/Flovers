  "translations": {
    "en": {
      "common_name": "Drummond's Phlox",
      "description": "Phlox drummondii is a vibrant flowering plant, popular in gardens. It thrives in full sun and well-drained soil."
    },
    "pl": {
      "common_name": "Floks Drummonda",
      "description": "Phlox drummondii to roślina ozdobna, popularna w ogrodach. Rośnie najlepiej w pełnym słońcu i dobrze przepuszczalnej glebie."
    },
    "de": {
      "common_name": "Drummonds Flieder",
      "description": "Phlox drummondii ist eine leuchtend blühende Pflanze, die in Gärten sehr beliebt ist. Sie gedeiht in voller Sonne und gut durchlässigem Boden."
    },
    "fr": {
      "common_name": "Phlox de Drummond",
      "description": "Le Phlox drummondii est une plante à fleurs vives, populaire dans les jardins. Elle prospère au soleil direct et dans un sol bien drainé."
    },
    "it": {
      "common_name": "Phlox di Drummond",
      "description": "Phlox drummondii è una pianta fiorita vivace, molto popolare nei giardini. Cresce bene al sole pieno e in terreni ben drenati."
    },
    "es": {
      "common_name": "Phlox de Drummond",
      "description": "Phlox drummondii es una planta con flores vibrantes, popular en jardines. Se desarrolla bien a pleno sol y en suelos bien drenados."
    },
    "pt": {
      "common_name": "Phlox de Drummond",
      "description": "Phlox drummondii é uma planta vibrante, popular em jardins. Cresce bem ao sol pleno e em solo bem drenado."
    },
    "ar": {
      "common_name": "فلوكس دروموندي",
      "description": "Phlox drummondii هي نبات مزهر لامع، شائع في الحدائق. ينمو في الشمس الكاملة والتربة جيدة التصريف."
    },
    "hi": {
      "common_name": "ड्रमंड का फ्लोक्स",
      "description": "Phlox drummondii एक जीवंत फूलों वाली पौधा है, जो बागवानी में लोकप्रिय है। यह पूरी धूप और अच्छी जल निकासी वाली मिट्टी में उगता है।"
    },
    "zh": {
      "common_name": "德拉蒙德风铃草",
      "description": "Phlox drummondii 是一种色彩鲜艳的开花植物，在花园中非常流行。它在全日照和排水良好的土壤中生长得很好。"
    },
    "ja": {
      "common_name": "ドラムンドフロックス",
      "description": "Phlox drummondii は鮮やかな花を咲かせる植物で、庭で人気があります。直射日光と排水性の良い土壌でよく育ちます。"
    },
    "ko": {
      "common_name": "드러먼드 플록스",
      "description": "Phlox drummondii는 화려한 꽃을 피우는 식물로 정원에서 인기가 있습니다. 직사광선과 배수가 좋은 토양에서 잘 자랍니다."
    }
  },
