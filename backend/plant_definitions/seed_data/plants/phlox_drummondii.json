  "translations": {
    "en": {
      "common_name": "Drummond's Phlox",
      "description": "Phlox drummondii is a colorful flowering plant native to Texas. It thrives in sunny, well-drained soils."
    },
    "pl": {
      "common_name": "Phlox Drummonda",
      "description": "Phlox drummondii to kolorowa roślina kwitnąca pochodząca z Teksasu. Lubi słoneczne, dobrze przepuszczalne gleby."
    },
    "de": {
      "common_name": "Drummonds Flammenblume",
      "description": "Phlox drummondii ist eine bunte Blume, die in Texas beheimatet ist. Sie bevorzugt sonnige, gut durchlässige Böden."
    },
    "fr": {
      "common_name": "Phlox de Drummond",
      "description": "Phlox drummondii est une plante colorée originaire du Texas. Elle préfère les sols bien drainés et ensoleillés."
    },
    "it": {
      "common_name": "Phlox di Drummond",
      "description": "Phlox drummondii è una pianta colorata originaria del Texas. Cresce bene in terreni ben drenati e soleggiati."
    },
    "es": {
      "common_name": "Phlox de Drummond",
      "description": "Phlox drummondii es una planta colorida originaria de Texas. Prefiere suelos bien drenados y soleados."
    },
    "pt": {
      "common_name": "Phlox de Drummond",
      "description": "Phlox drummondii é uma planta colorida nativa do Texas. Prefere solos bem drenados e ensolarados."
    },
    "ar": {
      "common_name": "فلوكس دروموند",
      "description": "فلوكس دروموند هو نبات مزهر ملون موطنه تكساس. يزدهر في التربة المشمسة والجيدة التصريف."
    },
    "hi": {
      "common_name": "ड्रमंड्स फ्लॉक्स",
      "description": "Phlox drummondii एक रंगीन फूलों वाला पौधा है जो टेक्सास का मूल निवासी है। यह धूप में उगने वाली, अच्छी जल निकासी वाली मिट्टी में अच्छे से बढ़ता है।"
    },
    "zh": {
      "common_name": "德拉蒙德长春花",
      "description": "Phlox drummondii 是一种色彩鲜艳的开花植物，原产于德克萨斯州。它喜欢阳光充足、排水良好的土壤。"
    },
    "ja": {
      "common_name": "ドラムモンドフロックス",
      "description": "Phlox drummondii はテキサス州原産の色鮮やかな花を咲かせる植物です。日当たりが良く、水はけの良い土壌を好みます。"
    },
    "ko": {
      "common_name": "드러먼드 플록스",
      "description": "Phlox drummondii는 텍사스에서 자생하는 다채로운 꽃을 가진 식물입니다. 햇볕이 잘 드는 배수가 좋은 토양에서 잘 자랍니다."
    }
  },
