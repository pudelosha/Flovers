  "translations": {
    "en": {
      "common_name": "Globeflower",
      "description": "Trollius europaeus is a perennial wildflower from moist meadows in Europe. It prefers moist, cool conditions and part shade to full shade."
    },
    "pl": {
      "common_name": "Kulisty kwiat",
      "description": "Trollius europaeus to bylina z wilgotnych łąk Europy. Preferuje wilgotne, chłodne warunki i półcień do cienia."
    },
    "de": {
      "common_name": "Kugelblume",
      "description": "Trollius europaeus ist eine mehrjährige Wildblume aus feuchten Wiesen Europas. Sie bevorzugt kühle, feuchte Standorte im Halbschatten."
    },
    "fr": {
      "common_name": "Trollius",
      "description": "Trollius europaeus est une vivace des prairies humides d'Europe. Elle préfère les sols frais et humide et une ombre partielle."
    },
    "it": {
      "common_name": "Fiore sferico",
      "description": "Trollius europaeus è un perenne dei prati umidi europei. Preferisce condizioni fresche e umide e mezzombra."
    },
    "es": {
      "common_name": "Flor esfera",
      "description": "Trollius europaeus es una perenne de praderas húmedas europeas. Prefiere condiciones frescas y húmedas y sombra parcial."
    },
    "pt": {
      "common_name": "Flor globular",
      "description": "Trollius europaeus é uma perene de prados húmidos da Europa. Prefere condições frescas e húmidas e sombra parcial."
    },
    "ar": {
      "common_name": "زهرة كروية",
      "description": "Trollius europaeus نبات معمر من المروج الرطبة في أوروبا. يفضل الظروف الباردة والرطبة والظل الجزئي."
    },
    "hi": {
      "common_name": "ग्लोबफ्लावर",
      "description": "Trollius europaeus यूरोप के नम मैदानी हिस्सों का एक पौधा है। यह ठंडी और नम स्थितियों में आंशिक छाया पसंद करता है."
    },
    "zh": {
      "common_name": "地球花",
      "description": "Trollius europaeus 是来自欧洲潮湿草地的多年生植物。它喜欢凉爽湿润的环境和部分阴影。"
    },
    "ja": {
      "common_name": "グローブフラワー",
      "description": "Trollius europaeus はヨーロッパの湿った草地の多年草です。 涼しく湿った条件と半日陰を好みます。"
    },
    "ko": {
      "common_name": "구형꽃",
      "description": "Trollius europaeus 는 유럽 습지 초원의 다년생 식물입니다. 시원하고 습한 환경과 반그늘을 선호합니다."
    }
  },
