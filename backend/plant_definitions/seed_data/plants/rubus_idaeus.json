  "translations": {
    "en": {
      "common_name": "Raspberry",
      "description": "Raspberries are small, red fruits that grow on shrubs. They are known for their sweet-tart flavor and are commonly used in desserts."
    },
    "pl": {
      "common_name": "Malina",
      "description": "Maliny to małe, czerwone owoce rosnące na krzewach. Są znane ze swojego słodko-kwaśnego smaku i często wykorzystywane w deserach."
    },
    "de": {
      "common_name": "Himbeere",
      "description": "Himbeeren sind kleine, rote Früchte, die an Sträuchern wachsen. Sie sind bekannt für ihren süß-sauren Geschmack und werden oft in Desserts verwendet."
    },
    "fr": {
      "common_name": "Framboise",
      "description": "Les framboises sont de petits fruits rouges qui poussent sur des buissons. Elles sont connues pour leur goût sucré-acidulé et sont souvent utilisées dans les desserts."
    },
    "it": {
      "common_name": "Lampone",
      "description": "I lamponi sono piccoli frutti rossi che crescono su arbusti. Sono noti per il loro sapore dolce-acido e sono spesso usati nei dolci."
    },
    "es": {
      "common_name": "Frambuesa",
      "description": "Las frambuesas son frutas rojas pequeñas que crecen en arbustos. Son conocidas por su sabor dulce y ácido y se utilizan comúnmente en postres."
    },
    "pt": {
      "common_name": "Framboesa",
      "description": "As framboesas são pequenas frutas vermelhas que crescem em arbustos. São conhecidas por seu sabor doce e azedo e são comumente usadas em sobremesas."
    },
    "ar": {
      "common_name": "توت العليق",
      "description": "توت العليق هو فاكهة حمراء صغيرة تنمو على الشجيرات. تشتهر بنكهتها الحلوة والحامضة وغالبًا ما تستخدم في الحلويات."
    },
    "hi": {
      "common_name": "रसभरी",
      "description": "रसभरी छोटे, लाल फल होते हैं जो झाड़ियों पर उगते हैं। इन्हें अपनी मीठी-खट्टी स्वाद के लिए जाना जाता है और आमतौर पर मिठाइयों में उपयोग किया जाता है."
    },
    "zh": {
      "common_name": "覆盆子",
      "description": "覆盆子是长在灌木丛中的小红果。它们因甜酸的味道而闻名，常用于甜点中。"
    },
    "ja": {
      "common_name": "ラズベリー",
      "description": "ラズベリーは低木に生える小さな赤い果実です。甘酸っぱい味で知られ、デザートに一般的に使われます。"
    },
    "ko": {
      "common_name": "산딸기",
      "description": "산딸기는 관목에서 자라는 작은 빨간 열매입니다. 달콤하고 시큼한 맛으로 유명하며 디저트에 자주 사용됩니다."
    }
  },
