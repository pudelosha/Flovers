  "translations": {
    "en": {
      "common_name": "Bird of Paradise",
      "description": "The Bird of Paradise is a striking plant known for its vibrant flowers. It thrives in full sun and requires moderate watering."
    },
    "pl": {
      "common_name": "Rajski Ptak",
      "description": "Rajski Ptak to roślina o imponujących kwiatach. Lubi pełne słońce i umiarkowane podlewanie."
    },
    "de": {
      "common_name": "Paradiesvogelblume",
      "description": "Die Paradiesvogelblume ist bekannt für ihre auffälligen Blumen. Sie gedeiht in voller Sonne und benötigt mäßiges Gießen."
    },
    "fr": {
      "common_name": "Oiseau de paradis",
      "description": "L'oiseau de paradis est une plante remarquable avec des fleurs vibrantes. Elle prospère sous plein soleil et nécessite un arrosage modéré."
    },
    "it": {
      "common_name": "Uccello del paradiso",
      "description": "L'uccello del paradiso è una pianta straordinaria con fiori vivaci. Cresce meglio al sole pieno e richiede un'irrigazione moderata."
    },
    "es": {
      "common_name": "Ave del paraíso",
      "description": "El ave del paraíso es una planta llamativa con flores vibrantes. Crece mejor con sol directo y requiere riego moderado."
    },
    "pt": {
      "common_name": "Ave do Paraíso",
      "description": "A Ave do Paraíso é uma planta notável com flores vibrantes. Ela cresce bem sob sol pleno e precisa de rega moderada."
    },
    "ar": {
      "common_name": "طائر الجنة",
      "description": "طائر الجنة هو نبات لافت للنظر مع زهور نابضة بالحياة. ينمو جيدًا تحت الشمس الكاملة ويتطلب سقاية معتدلة."
    },
    "hi": {
      "common_name": "स्वर्ग का पक्षी",
      "description": "स्वर्ग का पक्षी एक आकर्षक पौधा है जिसकी रंगीन फूल होती हैं। यह पूरी धूप में उगता है और मध्यम पानी की आवश्यकता होती है।"
    },
    "zh": {
      "common_name": "天堂鸟",
      "description": "天堂鸟是一种以鲜艳花朵著称的植物。它在充足阳光下茁壮成长，需要适度浇水。"
    },
    "ja": {
      "common_name": "極楽鳥花",
      "description": "極楽鳥花は鮮やかな花で知られる印象的な植物です。完全な日光の下で育ち、適度な水やりが必要です。"
    },
    "ko": {
      "common_name": "천국의 새",
      "description": "천국의 새는 화려한 꽃으로 유명한 인상적인 식물입니다. 이 식물은 완전한 햇빛에서 자라며 적당한 물주기가 필요합니다."
    }
  },
