  "translations": {
    "en": {
      "common_name": "Coleus",
      "description": "Coleus scutellarioides is a tropical foliage plant valued for its colorful leaves. It prefers bright indirect light and consistently moist soil indoors."
    },
    "pl": {
      "common_name": "Koleus",
      "description": "Coleus scutellarioides to tropikalna roślina ozdobna o barwnych liściach. Najlepiej rośnie w jasnym miejscu i wilgotnym podłożu."
    },
    "de": {
      "common_name": "Buntnessel",
      "description": "Coleus scutellarioides ist eine tropische Blattschmuckpflanze mit farbigen Blättern. Sie mag helles Licht und gleichmäßig feuchte Erde."
    },
    "fr": {
      "common_name": "Coléus",
      "description": "Coleus scutellarioides est une plante tropicale appréciée pour son feuillage coloré. Elle préfère une lumière vive et un sol humide."
    },
    "it": {
      "common_name": "Coleo",
      "description": "Coleus scutellarioides è una pianta tropicale ornamentale dalle foglie colorate. Richiede luce intensa e terreno costantemente umido."
    },
    "es": {
      "common_name": "Cóleo",
      "description": "Coleus scutellarioides es una planta tropical apreciada por su follaje colorido. Necesita buena luz y riego regular."
    },
    "pt": {
      "common_name": "Coleus",
      "description": "Coleus scutellarioides é uma planta tropical ornamental de folhas coloridas. Prefere luz indireta e solo sempre úmido."
    },
    "ar": {
      "common_name": "كوليوس",
      "description": "Coleus scutellarioides نبات استوائي يتميز بأوراقه الملونة. يفضل الضوء الساطع غير المباشر وتربة رطبة."
    },
    "hi": {
      "common_name": "कोलियस",
      "description": "Coleus scutellarioides एक उष्णकटिबंधीय सजावटी पौधा है। इसे तेज अप्रत्यक्ष रोशनी और नम मिट्टी पसंद है।"
    },
    "zh": {
      "common_name": "彩叶草",
      "description": "Coleus scutellarioides 是一种热带观叶植物，以彩色叶片著称。适合明亮散射光和湿润土壤。"
    },
    "ja": {
      "common_name": "コリウス",
      "description": "Coleus scutellarioides は色鮮やかな葉を持つ熱帯植物です。明るい間接光と湿った土を好みます。"
    },
    "ko": {
      "common_name": "콜레우스",
      "description": "Coleus scutellarioides 는 화려한 잎이 특징인 열대 식물입니다. 밝은 간접광과 촉촉한 토양을 선호합니다."
    }
