  "translations": {
    "en": {
      "common_name": "Parsley",
      "description": "Parsley is a popular herb used in cooking. It prefers moderate sunlight and well-drained soil."
    },
    "pl": {
      "common_name": "Pietruszka",
      "description": "Pietruszka to popularna roślina używana w kuchni. Woli umiarkowane światło słoneczne i dobrze przepuszczalną glebę."
    },
    "de": {
      "common_name": "Petersilie",
      "description": "Petersilie ist ein beliebtes Küchenkraut. Sie bevorzugt mäßiges Sonnenlicht und gut durchlässigen Boden."
    },
    "fr": {
      "common_name": "Persil",
      "description": "Le persil est une herbe populaire en cuisine. Il préfère une lumière modérée et un sol bien drainé."
    },
    "it": {
      "common_name": "Prezzemolo",
      "description": "Il prezzemolo è un'erba popolare in cucina. Preferisce la luce moderata e un terreno ben drenato."
    },
    "es": {
      "common_name": "Perejil",
      "description": "El perejil es una hierba popular en la cocina. Prefiere luz moderada y suelo bien drenado."
    },
    "pt": {
      "common_name": "Salsa",
      "description": "A salsa é uma erva popular na culinária. Prefere luz moderada e solo bem drenado."
    },
    "ar": {
      "common_name": "بقدونس",
      "description": "البقدونس هو عشب شائع في الطهي. يفضل الضوء المعتدل والتربة جيدة الصرف."
    },
    "hi": {
      "common_name": "धनिया",
      "description": "धनिया एक लोकप्रिय हर्ब है जिसका उपयोग खाना बनाने में होता है। यह मध्यम धूप और अच्छी जल निकासी वाली मिट्टी पसंद करता है।"
    },
    "zh": {
      "common_name": "香菜",
      "description": "香菜是厨房中常用的草药。它喜欢适度的阳光和排水良好的土壤。"
    },
    "ja": {
      "common_name": "パセリ",
      "description": "パセリは料理でよく使われるハーブです。適度な日光と排水の良い土壌を好みます。"
    },
    "ko": {
      "common_name": "파슬리",
      "description": "파슬리는 요리에서 자주 사용되는 허브입니다. 적당한 햇빛과 배수가 좋은 토양을 선호합니다."
    }
  },
