    "en": {
      "common_name": "Parsley",
      "description": "Parsley is a popular herb used in cooking. It prefers medium light and moderate watering."
    },
    "pl": {
      "common_name": "Pietruszka",
      "description": "Pietruszka to popularne zioło wykorzystywane w kuchni. Lubi średnie nasłonecznienie i umiarkowane podlewanie."
    },
    "de": {
      "common_name": "Petersilie",
      "description": "Petersilie ist ein beliebtes Kraut in der Küche. Sie bevorzugt mittleres Licht und mäßiges Gießen."
    },
    "fr": {
      "common_name": "Persil",
      "description": "Le persil est une herbe populaire en cuisine. Il préfère une lumière moyenne et un arrosage modéré."
    },
    "it": {
      "common_name": "Prezzemolo",
      "description": "Il prezzemolo è un'erba popolare in cucina. Preferisce una luce media e un'annaffiatura moderata."
    },
    "es": {
      "common_name": "Perejil",
      "description": "El perejil es una hierba popular en la cocina. Prefiere luz media y riego moderado."
    },
    "pt": {
      "common_name": "Salsa",
      "description": "A salsa é uma erva popular na culinária. Prefere luz média e rega moderada."
    },
    "ar": {
      "common_name": "بقدونس",
      "description": "البقدونس هو عشبة شائعة في الطهي. يفضل الضوء المتوسط والري المعتدل."
    },
    "hi": {
      "common_name": "धनिया",
      "description": "धनिया एक लोकप्रिय मसाला है। इसे मध्यम रोशनी और सामान्य पानी की आवश्यकता होती है।"
    },
    "zh": {
      "common_name": "香菜",
      "description": "香菜是常用于烹饪的草本植物。它喜欢中等光照和适度浇水。"
    },
    "ja": {
      "common_name": "パセリ",
      "description": "パセリは料理でよく使われるハーブです。中程度の光と適度な水やりを好みます。"
    },
    "ko": {
      "common_name": "파슬리",
      "description": "파슬리는 요리에 자주 사용되는 허브입니다. 중간 정도의 빛과 적당한 물주기를 선호합니다."
    }
