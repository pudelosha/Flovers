  "translations": {
    "en": {
      "common_name": "Slender spleenwort",
      "description": "Asplenium gracillimum is a delicate fern native to New Zealand forests. It prefers bright, indirect light and consistently moist soil indoors."
    },
    "pl": {
      "common_name": "Zanokcica smukła",
      "description": "Asplenium gracillimum to delikatna paproć z Nowej Zelandii. Najlepiej rośnie w jasnym, rozproszonym świetle i stale wilgotnym podłożu."
    },
    "de": {
      "common_name": "Zarter Streifenfarn",
      "description": "Asplenium gracillimum ist ein feiner Farn aus Neuseeland. Er mag helles, indirektes Licht und gleichmäßig feuchte Erde."
    },
    "fr": {
      "common_name": "Doradille grêle",
      "description": "Asplenium gracillimum est une fougère délicate originaire de Nouvelle-Zélande. Elle apprécie une lumière indirecte et un sol toujours légèrement humide."
    },
    "it": {
      "common_name": "Asplenio sottile",
      "description": "Asplenium gracillimum è una felce delicata della Nuova Zelanda. Predilige luce indiretta e terreno costantemente umido."
    },
    "es": {
      "common_name": "Culantrillo esbelto",
      "description": "Asplenium gracillimum es un helecho delicado de Nueva Zelanda. Requiere luz indirecta y sustrato uniformemente húmedo."
    },
    "pt": {
      "common_name": "Asplênio esguio",
      "description": "Asplenium gracillimum é uma samambaia delicada da Nova Zelândia. Gosta de luz indireta e solo sempre levemente úmido."
    },
    "ar": {
      "common_name": "سرخس أسبلينيوم رفيع",
      "description": "Asplenium gracillimum سرخس دقيق موطنه نيوزيلندا. يفضل الضوء غير المباشر والتربة الرطبة باستمرار."
    },
    "hi": {
      "common_name": "पतला स्प्लीनवॉर्ट",
      "description": "Asplenium gracillimum न्यूज़ीलैंड का एक नाज़ुक फ़र्न है। इसे अप्रत्यक्ष रोशनी और लगातार नम मिट्टी पसंद है।"
    },
    "zh": {
      "common_name": "纤细铁角蕨",
      "description": "Asplenium gracillimum 原产于新西兰，是一种精致的蕨类植物。室内养护需明亮散射光和持续湿润的土壤。"
    },
    "ja": {
      "common_name": "ホソバチャセンシダ",
      "description": "Asplenium gracillimum はニュージーランド原産の繊細なシダです。明るい間接光と適度に湿った土を好みます。"
    },
    "ko": {
      "common_name": "가는아스플레니움",
      "description": "Asplenium gracillimum은 뉴질랜드 원산의 섬세한 양치식물입니다. 밝은 간접광과 지속적으로 촉촉한 토양을 선호합니다."
    }
  },
