  "translations": {
    "en": {
      "common_name": "Bilberry",
      "description": "Vaccinium myrtillus is a low-growing deciduous shrub native to European forests and heathlands. It produces edible dark blue berries and prefers acidic, moist soils in cool climates."
    },
    "pl": {
      "common_name": "Borówka czarna",
      "description": "Vaccinium myrtillus to niski krzew liściasty rosnący naturalnie w lasach i na wrzosowiskach Europy. Wytwarza jadalne, ciemnoniebieskie owoce i wymaga kwaśnej, wilgotnej gleby."
    },
    "de": {
      "common_name": "Heidelbeere",
      "description": "Vaccinium myrtillus ist ein niedriger Laubstrauch aus europäischen Wäldern und Heiden. Er trägt essbare blaue Beeren und wächst am besten in sauren, feuchten Böden."
    },
    "fr": {
      "common_name": "Myrtille",
      "description": "Vaccinium myrtillus est un petit arbuste caduc originaire des forêts européennes. Il produit des baies comestibles et préfère les sols acides et frais."
    },
    "it": {
      "common_name": "Mirtillo nero",
      "description": "Vaccinium myrtillus è un arbusto basso originario delle foreste europee. Produce bacche commestibili e cresce bene in terreni acidi e umidi."
    },
    "es": {
      "common_name": "Arándano europeo",
      "description": "Vaccinium myrtillus es un arbusto bajo nativo de bosques europeos. Da frutos comestibles y requiere suelos ácidos y húmedos."
    },
    "pt": {
      "common_name": "Mirtilo",
      "description": "Vaccinium myrtillus é um arbusto baixo nativo de florestas europeias. Produz frutos comestíveis e prefere solos ácidos e úmidos."
    },
    "ar": {
      "common_name": "توت بري أوروبي",
      "description": "Vaccinium myrtillus شجيرة منخفضة موطنها الغابات الأوروبية. تنتج ثمارًا صالحة للأكل وتنمو في تربة حمضية رطبة."
    },
    "hi": {
      "common_name": "यूरोपीय ब्लूबेरी",
      "description": "Vaccinium myrtillus एक छोटी झाड़ी है जो यूरोपीय वनों में पाई जाती है। यह खाने योग्य फल देती है और अम्लीय नम मिट्टी पसंद करती है।"
    },
    "zh": {
      "common_name": "欧洲越橘",
      "description": "Vaccinium myrtillus 是原产于欧洲森林的低矮灌木。它结可食用的蓝色果实，适合酸性湿润土壤。"
    },
    "ja": {
      "common_name": "ビルベリー",
      "description": "Vaccinium myrtillus はヨーロッパ原産の低木です。食用になる青い果実をつけ、酸性で湿った土壌を好みます。"
    },
    "ko": {
      "common_name": "빌베리",
      "description": "Vaccinium myrtillus는 유럽 숲이 원산지인 낮은 관목입니다. 식용 가능한 열매를 맺으며 산성의 습한 토양을 선호합니다."
    }
  },
