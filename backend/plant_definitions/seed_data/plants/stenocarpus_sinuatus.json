  "translations": {
    "en": {
      "common_name": "Firewheel Tree",
      "description": "The Firewheel Tree is a beautiful flowering tree native to Australia. It is known for its stunning red-orange flowers."
    },
    "pl": {
      "common_name": "Drzewo Ognistego Koła",
      "description": "Drzewo Ognistego Koła to piękne drzewo kwitnące, pochodzące z Australii. Znane jest z imponujących czerwono-pomarańczowych kwiatów."
    },
    "de": {
      "common_name": "Feuerradbaum",
      "description": "Der Feuerradbaum ist ein schöner blühender Baum aus Australien, bekannt für seine auffälligen rot-orangefarbenen Blüten."
    },
    "fr": {
      "common_name": "Arbre à roue de feu",
      "description": "L'Arbre à roue de feu est un bel arbre à fleurs originaire d'Australie, connu pour ses magnifiques fleurs rouge-orange."
    },
    "it": {
      "common_name": "Albero Ruota di Fuoco",
      "description": "L'Albero Ruota di Fuoco è un bellissimo albero da fiore originario dell'Australia, noto per i suoi fiori rosso-arancio."
    },
    "es": {
      "common_name": "Árbol Rueda de Fuego",
      "description": "El Árbol Rueda de Fuego es un hermoso árbol con flores originario de Australia, conocido por sus flores rojas y naranjas."
    },
    "pt": {
      "common_name": "Árvore Roda de Fogo",
      "description": "A Árvore Roda de Fogo é uma bela árvore florida nativa da Austrália, conhecida por suas flores vermelhas e laranjas."
    },
    "ar": {
      "common_name": "شجرة عجلة النار",
      "description": "شجرة عجلة النار هي شجرة مزهرة جميلة موطنها الأصلي أستراليا، ومعروفة بأزهارها الحمراء البرتقالية المدهشة."
    },
    "hi": {
      "common_name": "आग का पहिया पेड़",
      "description": "आग का पहिया पेड़ एक सुंदर फूलों वाला पेड़ है जो ऑस्ट्रेलिया का मूल निवासी है। यह अपने लाल-नारंगी फूलों के लिए जाना जाता है।"
    },
    "zh": {
      "common_name": "火轮树",
      "description": "火轮树是一种美丽的开花树，原产于澳大利亚，以其红橙色的花朵而闻名。"
    },
    "ja": {
      "common_name": "ファイヤーホイールツリー",
      "description": "ファイヤーホイールツリーはオーストラリア原産の美しい花を持つ木で、鮮やかな赤橙色の花で知られています。"
    },
    "ko": {
      "common_name": "화염바퀴나무",
      "description": "화염바퀴나무는 호주 원산지의 아름다운 꽃나무로, 붉은 주황색 꽃으로 유명합니다."
    }
  },
