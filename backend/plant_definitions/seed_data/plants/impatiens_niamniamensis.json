  "translations": {
    "en": {
      "common_name": "Parrot Plant",
      "description": "Impatiens niamniamensis is a tropical plant known for its parrot-shaped red and yellow flowers. It grows best indoors in bright, indirect light and consistently moist soil. Warm temperatures and stable care support healthy flowering."
    },
    "pl": {
      "common_name": "Niecierpek papuzi",
      "description": "Impatiens niamniamensis to tropikalna roślina o kwiatach przypominających papugi. Najlepiej rośnie w jasnym, rozproszonym świetle i stale wilgotnym podłożu. Wymaga ciepła i regularnej pielęgnacji."
    },
    "de": {
      "common_name": "Papageien-Impatiens",
      "description": "Impatiens niamniamensis ist eine tropische Pflanze mit papageienförmigen Blüten. Sie bevorzugt helles, indirektes Licht und gleichmäßig feuchte Erde. Warme Temperaturen fördern gesundes Wachstum."
    },
    "fr": {
      "common_name": "Plante perroquet",
      "description": "Impatiens niamniamensis est une plante tropicale aux fleurs en forme de perroquet. Elle aime une lumière vive sans soleil direct et un sol constamment humide. La chaleur favorise sa floraison."
    },
    "it": {
      "common_name": "Pianta pappagallo",
      "description": "Impatiens niamniamensis è una pianta tropicale dai fiori simili a pappagalli. Cresce bene con luce indiretta e terreno sempre umido. Predilige temperature calde e stabili."
    },
    "es": {
      "common_name": "Planta loro",
      "description": "Impatiens niamniamensis es una planta tropical con flores que parecen loros. Necesita luz indirecta brillante y sustrato húmedo de forma constante. El calor favorece su desarrollo."
    },
    "pt": {
      "common_name": "Planta papagaio",
      "description": "Impatiens niamniamensis é uma planta tropical com flores em forma de papagaio. Prefere luz indireta intensa e solo sempre úmido. Temperaturas quentes ajudam no florescimento."
    },
    "ar": {
      "common_name": "نبات الببغاء",
      "description": "Impatiens niamniamensis نبات استوائي يتميز بأزهار تشبه الببغاء. ينمو جيدًا في ضوء ساطع غير مباشر وتربة رطبة باستمرار. يحتاج إلى درجات حرارة دافئة."
    },
    "hi": {
      "common_name": "पैरट प्लांट",
      "description": "Impatiens niamniamensis एक उष्णकटिबंधीय पौधा है जिसके फूल तोते जैसे दिखते हैं। यह तेज़ अप्रत्यक्ष रोशनी और नम मिट्टी में अच्छा बढ़ता है। गर्म तापमान इसके लिए उपयुक्त हैं।"
    },
    "zh": {
      "common_name": "鹦鹉凤仙花",
      "description": "Impatiens niamniamensis 是一种热带植物，花朵形似鹦鹉。适合在明亮的散射光和持续湿润的土壤中生长。温暖环境有助于开花。"
    },
    "ja": {
      "common_name": "オウムホウセンカ",
      "description": "Impatiens niamniamensis はオウムの形をした花が特徴の熱帯植物です。明るい間接光と湿った土壌を好みます。暖かい環境でよく育ちます。"
    },
    "ko": {
      "common_name": "앵무 임파첸스",
      "description": "Impatiens niamniamensis는 앵무새 모양의 꽃을 가진 열대 식물입니다. 밝은 간접광과 항상 촉촉한 흙에서 잘 자랍니다. 따뜻한 온도가 필요합니다."
    }
  },
