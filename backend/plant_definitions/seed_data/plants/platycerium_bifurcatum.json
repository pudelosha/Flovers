  "translations": {
    "en": {
      "common_name": "Staghorn Fern",
      "description": "The Staghorn Fern is an epiphytic plant with large, bifurcated fronds. It thrives in warm, humid environments and is often found growing on tree trunks."
    },
    "pl": {
      "common_name": "Pterygium jelenie",
      "description": "Pterygium jelenie to epifityczna roślina z dużymi, rozgałęzionymi liśćmi. Rośnie w ciepłych, wilgotnych warunkach, często na pniach drzew."
    },
    "de": {
      "common_name": "Hirschhornfarn",
      "description": "Der Hirschhornfarn ist eine epiphytische Pflanze mit großen, verzweigten Blättern. Er gedeiht in warmen, feuchten Umgebungen und wächst oft auf Baumstämmen."
    },
    "fr": {
      "common_name": "Fougère cerf",
      "description": "La fougère cerf est une plante épiphyte avec de grandes frondes bifurquées. Elle prospère dans des environnements chauds et humides, souvent sur les troncs d'arbres."
    },
    "it": {
      "common_name": "Felce corna di cervo",
      "description": "La felce corna di cervo è una pianta epifita con grandi fronde biforcate. Cresce in ambienti caldi e umidi, spesso sui tronchi degli alberi."
    },
    "es": {
      "common_name": "Helecho cuerno de alce",
      "description": "El helecho cuerno de alce es una planta epífita con grandes frondas bifurcadas. Crece en ambientes cálidos y húmedos, a menudo sobre los troncos de los árboles."
    },
    "pt": {
      "common_name": "Samambaia chifre de alce",
      "description": "A samambaia chifre de alce é uma planta epífita com grandes frondes bifurcadas. Cresce em ambientes quentes e úmidos, frequentemente sobre troncos de árvores."
    },
    "ar": {
      "common_name": "سرخس قرن الأيل",
      "description": "سرخس قرن الأيل هو نبات إبيفيتي ذو أوراق متفرعة كبيرة. ينمو في بيئات دافئة ورطبة وغالبًا ما ينمو على جذوع الأشجار."
    },
    "hi": {
      "common_name": "गांठ फर्न",
      "description": "गांठ फर्न एक एपिफाइटिक पौधा है जिसकी बड़ी, bifurcated पत्तियां होती हैं। यह गर्म, आर्द्र वातावरण में पनपता है और अक्सर वृक्षों की छाल पर उगता है।"
    },
    "zh": {
      "common_name": "鹿角蕨",
      "description": "鹿角蕨是一种附生植物，具有大型分叉的叶子。它在温暖湿润的环境中生长，通常生长在树干上。"
    },
    "ja": {
      "common_name": "鹿角シダ",
      "description": "鹿角シダは大きな分岐した葉を持つ着生植物です。温暖で湿度の高い環境で繁茂し、しばしば木の幹に生えています。"
    },
    "ko": {
      "common_name": "사슴뿔고사리",
      "description": "사슴뿔고사리는 큰 갈라진 잎을 가진 착생 식물입니다. 따뜻하고 습한 환경에서 자라며 종종 나무의 줄기에 자랍니다."
    }
  },
