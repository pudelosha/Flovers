  "translations": {
    "en": {
      "common_name": "Boston Ivy",
      "description": "Boston Ivy is a fast-growing climbing plant. It thrives in full sun and well-draining soil, with moderate watering."
    },
    "pl": {
      "common_name": "Bluszcz Daleki",
      "description": "Bluszcz Daleki to szybko rosnąca roślina pnąca. Wymaga pełnego słońca i dobrze przepuszczalnej gleby, umiarkowanego podlewania."
    },
    "de": {
      "common_name": "Klettereiche",
      "description": "Die Klettereiche ist eine schnell wachsende Kletterpflanze. Sie gedeiht in voller Sonne und gut durchlässigem Boden bei mäßigem Gießen."
    },
    "fr": {
      "common_name": "Lierre de Boston",
      "description": "Le lierre de Boston est une plante grimpante à croissance rapide. Elle prospère en plein soleil et dans un sol bien drainé avec un arrosage modéré."
    },
    "it": {
      "common_name": "Edera di Boston",
      "description": "L'edera di Boston è una pianta rampicante a crescita rapida. Cresce bene in pieno sole e in terreni ben drenati con irrigazione moderata."
    },
    "es": {
      "common_name": "Hiedra de Boston",
      "description": "La hiedra de Boston es una planta trepadora de rápido crecimiento. Prosperará a pleno sol y en suelos bien drenados con riego moderado."
    },
    "pt": {
      "common_name": "Herbácea de Boston",
      "description": "A herácea de Boston é uma planta trepadeira de crescimento rápido. Ela prospera à plena luz solar e em solo bem drenado com rega moderada."
    },
    "ar": {
      "common_name": "اللبلاب بوسطن",
      "description": "اللبلاب بوسطن هو نبات متسلق سريع النمو. يزدهر في الشمس الكاملة والتربة جيدة التصريف مع الري المعتدل."
    },
    "hi": {
      "common_name": "बॉस्टन आइवी",
      "description": "बॉस्टन आइवी एक तेज़ी से बढ़ने वाली चढ़ाई करने वाली पौधा है। यह पूर्ण सूर्य और अच्छी जल निकासी वाली मिट्टी में, मध्यम पानी के साथ अच्छा होता है।"
    },
    "zh": {
      "common_name": "波士顿常春藤",
      "description": "波士顿常春藤是一种快速生长的攀缘植物。它在全日照和良好排水的土壤中生长良好，适量浇水。"
    },
    "ja": {
      "common_name": "ボストンアイビー",
      "description": "ボストンアイビーは成長が速いつる植物です。完全な日光と排水の良い土壌で、適度な水やりが必要です。"
    },
    "ko": {
      "common_name": "보스턴 아이비",
      "description": "보스턴 아이비는 빠르게 자라는 덩굴 식물입니다. 완전한 햇볕과 잘 배수되는 토양에서 자주 물을 주어야 잘 자랍니다."
    }
  },
