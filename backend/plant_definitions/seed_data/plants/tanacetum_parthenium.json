  "translations": {
    "en": {
      "common_name": "Feverfew",
      "description": "Feverfew is a perennial herb used in traditional medicine. It thrives in moderate light and well-drained soil."
    },
    "pl": {
      "common_name": "Parapet",
      "description": "Parapet to wieloletnia roślina lecznicza. Rośnie w średnim świetle i dobrze przepuszczalnej glebie."
    },
    "de": {
      "common_name": "Fieberkraut",
      "description": "Fieberkraut ist ein mehrjähriges Kraut, das in der traditionellen Medizin verwendet wird. Es wächst bei mäßigem Licht und gut durchlässigem Boden."
    },
    "fr": {
      "common_name": "Parthénium",
      "description": "Le Parthénium est une plante vivace utilisée en médecine traditionnelle. Elle pousse dans une lumière modérée et un sol bien drainé."
    },
    "it": {
      "common_name": "Tanaceto",
      "description": "Il Tanaceto è una pianta perenne usata nella medicina tradizionale. Cresce in luce media e in terreni ben drenati."
    },
    "es": {
      "common_name": "Tanacetum",
      "description": "El Tanacetum es una planta perenne utilizada en medicina tradicional. Crece en luz moderada y en suelos bien drenados."
    },
    "pt": {
      "common_name": "Tanacetum",
      "description": "O Tanacetum é uma planta perene usada na medicina tradicional. Cresce em luz moderada e solo bem drenado."
    },
    "ar": {
      "common_name": "التاناسيتوم",
      "description": "التاناسيتوم هو نبات معمر يستخدم في الطب التقليدي. ينمو في ضوء معتدل وتربة جيدة التصريف."
    },
    "hi": {
      "common_name": "फीवरफ्यू",
      "description": "फीवरफ्यू एक पारंपरिक औषधि में उपयोग होने वाली बारहमासी जड़ी-बूटी है। यह मध्यम रोशनी और अच्छी जल निकासी वाली मिट्टी में उगता है।"
    },
    "zh": {
      "common_name": "凤仙花",
      "description": "凤仙花是一种多年生草本植物，常用于传统医学。它生长在中等光照和良好排水的土壤中。"
    },
    "ja": {
      "common_name": "フェバーフュー",
      "description": "フェバーフューは伝統的な薬草として使われる多年生植物です。中程度の光と良好な排水の土壌で育ちます。"
    },
    "ko": {
      "common_name": "페버퓨",
      "description": "페버퓨는 전통 의학에서 사용되는 다년생 식물입니다. 중간 정도의 빛과 잘 배수되는 토양에서 자랍니다."
    }
  },
