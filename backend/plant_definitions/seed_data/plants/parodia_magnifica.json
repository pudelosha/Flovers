  "translations": {
    "en": {
      "common_name": "Magnificent Parodia",
      "description": "A slow-growing cactus native to South America. It requires full sun and well-draining soil."
    },
    "pl": {
      "common_name": "Magnificent Parodia",
      "description": "Wolno rosnący kaktus pochodzący z Ameryki Południowej. Wymaga pełnego słońca i dobrze przepuszczalnej gleby."
    },
    "de": {
      "common_name": "Magnificent Parodia",
      "description": "Ein langsam wachsender Kaktus aus Südamerika. Er benötigt volle Sonne und gut durchlässigen Boden."
    },
    "fr": {
      "common_name": "Parodia magnifique",
      "description": "Un cactus à croissance lente originaire d'Amérique du Sud. Il nécessite un ensoleillement direct et un sol bien drainé."
    },
    "it": {
      "common_name": "Parodia magnifica",
      "description": "Un cactus a crescita lenta originario del Sud America. Richiede pieno sole e terreno ben drenato."
    },
    "es": {
      "common_name": "Parodia magnífica",
      "description": "Un cactus de crecimiento lento originario de América del Sur. Requiere sol pleno y suelo bien drenado."
    },
    "pt": {
      "common_name": "Parodia magnífica",
      "description": "Um cacto de crescimento lento originário da América do Sul. Precisa de sol pleno e solo bem drenado."
    },
    "ar": {
      "common_name": "باروديا رائعة",
      "description": "صبار ينمو ببطء من أمريكا الجنوبية. يحتاج إلى شمس كاملة وتربة جيدة التصريف."
    },
    "hi": {
      "common_name": "magnificent Parodia",
      "description": "दक्षिण अमेरिका का एक धीमी वृद्धि वाला कैक्टस। इसे पूरी धूप और अच्छी जल निकासी वाली मिट्टी की आवश्यकता होती है।"
    },
    "zh": {
      "common_name": "壮丽的巴罗迪亚",
      "description": "一种生长缓慢的仙人掌，原产于南美。它需要充足的阳光和排水良好的土壤。"
    },
    "ja": {
      "common_name": "豪華なパロディア",
      "description": "南アメリカ原産の遅い成長をするサボテン。直射日光と排水性の良い土を必要とします。"
    },
    "ko": {
      "common_name": "Magnificent Parodia",
      "description": "남미 원산지의 느리게 자라는 선인장. 직사광선과 배수가 좋은 토양이 필요합니다."
    }
  },
