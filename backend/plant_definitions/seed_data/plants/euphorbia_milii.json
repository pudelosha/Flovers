  "translations": {
    "en": {
      "common_name": "Crown of Thorns",
      "description": "A spiny, succulent shrub known for its bright, colorful flowers. Tolerant of dry conditions."
    },
    "pl": {
      "common_name": "Korona cierniowa",
      "description": "Krępy, mięsisty krzew z jaskrawymi, kolorowymi kwiatami. Toleruje suche warunki."
    },
    "de": {
      "common_name": "Dornkrone",
      "description": "Ein stacheliger, sukkulenter Strauch mit leuchtend bunten Blüten. Toleriert trockene Bedingungen."
    },
    "fr": {
      "common_name": "Couronne d'épines",
      "description": "Un buisson épineux et succulente, connu pour ses fleurs colorées et vives. Tolère les conditions sèches."
    },
    "it": {
      "common_name": "Corona di spine",
      "description": "Un cespuglio spinato e succulento, noto per i suoi fiori colorati e vivaci. Resistente alle condizioni di siccità."
    },
    "es": {
      "common_name": "Corona de espinas",
      "description": "Un arbusto espinoso y suculento conocido por sus flores brillantes y coloridas. Tolera las condiciones secas."
    },
    "pt": {
      "common_name": "Coroa de espinhos",
      "description": "Um arbusto espinhoso e suculento, conhecido por suas flores brilhantes e coloridas. Tolerante às condições secas."
    },
    "ar": {
      "common_name": "تاج الشوك",
      "description": "شجيرة شوكية وعصارية معروفة بزهورها الملونة الزاهية. تتحمل الظروف الجافة."
    },
    "hi": {
      "common_name": "कांटे का मुकुट",
      "description": "एक कांटेदार, मांसल झाड़ी जो अपनी चमकीली, रंगीन फूलों के लिए जानी जाती है। सूखी परिस्थितियों में सहनशील।"
    },
    "zh": {
      "common_name": "荆棘之冠",
      "description": "一种多刺的多肉灌木，以其鲜艳的花朵而闻名。耐干旱条件。"
    },
    "ja": {
      "common_name": "トゲの王冠",
      "description": "鮮やかな色の花で知られるトゲのある多肉植物の茂み。乾燥した条件に耐性あり。"
    },
    "ko": {
      "common_name": "가시 왕관",
      "description": "밝고 화려한 꽃으로 유명한 가시가 있는 다육 식물 덤불. 건조한 환경에서 잘 자람."
    }
  },
