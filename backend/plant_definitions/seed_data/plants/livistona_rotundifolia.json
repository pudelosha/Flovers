  "translations": {
    "en": {
      "common_name": "Round-leafed Livistona",
      "description": "Livistona rotundifolia is a slow-growing, tropical palm. It thrives in bright, indirect light and prefers moderate watering."
    },
    "pl": {
      "common_name": "Livistona o okrągłych liściach",
      "description": "Livistona rotundifolia to powoli rosnąca palma tropikalna. Preferuje jasne, pośrednie światło i umiarkowane podlewanie."
    },
    "de": {
      "common_name": "Rundblättrige Livistona",
      "description": "Livistona rotundifolia ist eine langsam wachsende tropische Palme. Sie bevorzugt helles, indirektes Licht und mäßiges Gießen."
    },
    "fr": {
      "common_name": "Livistona à feuilles rondes",
      "description": "Livistona rotundifolia est un palmier à croissance lente. Il préfère la lumière indirecte et un arrosage modéré."
    },
    "it": {
      "common_name": "Livistona a foglie rotonde",
      "description": "Livistona rotundifolia è una palma a crescita lenta. Preferisce la luce indiretta e annaffiature moderate."
    },
    "es": {
      "common_name": "Livistona de hojas redondas",
      "description": "Livistona rotundifolia es una palma de crecimiento lento. Prefiere luz indirecta y riego moderado."
    },
    "pt": {
      "common_name": "Livistona de folhas arredondadas",
      "description": "Livistona rotundifolia é uma palma de crescimento lento. Prefere luz indireta e rega moderada."
    },
    "ar": {
      "common_name": "ليفستونا ذات الأوراق المستديرة",
      "description": "ليفستونا روتونديفوليا هي نبتة نخيلية بطيئة النمو. تفضل الضوء غير المباشر والري المعتدل."
    },
    "hi": {
      "common_name": "गोल पत्तियों वाली लिविस्टोना",
      "description": "लिविस्टोना रोटुंडीफोलिया एक धीमी वृद्धि करने वाला उष्णकटिबंधीय हथेली है। यह उज्ज्वल, अप्रत्यक्ष प्रकाश में बढ़ता है और मध्यम पानी पसंद करता है।"
    },
    "zh": {
      "common_name": "圆叶棕榈",
      "description": "圆叶棕榈是一种生长缓慢的热带棕榈。它喜欢明亮的间接光和适度的浇水。"
    },
    "ja": {
      "common_name": "丸葉リヴィストナ",
      "description": "リヴィストナ・ロトゥンディフォリアは遅く成長する熱帯性のヤシです。明るい間接光と適度な水やりを好みます。"
    },
    "ko": {
      "common_name": "둥근잎 리비스토나",
      "description": "리비스토나 로툰디폴리아는 느리게 자라는 열대 야자수입니다. 밝고 간접적인 빛과 적당한 급수를 선호합니다."
    }
  },
