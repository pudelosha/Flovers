  "translations": {
    "en": {
      "common_name": "Brazilian Purple Sage",
      "description": "A flowering plant known for its purple blooms and attractive foliage. It thrives in medium light and requires moderate watering."
    },
    "pl": {
      "common_name": "Szałwia brazylijska",
      "description": "Roślina kwitnąca, znana z fioletowych kwiatów i atrakcyjnego ulistnienia. Wymaga średniego światła i umiarkowanego podlewania."
    },
    "de": {
      "common_name": "Brasilianische Purpursalbei",
      "description": "Eine blühende Pflanze, die für ihre lila Blüten und attraktives Laub bekannt ist. Sie gedeiht bei mittlerem Licht und benötigt mäßiges Gießen."
    },
    "fr": {
      "common_name": "Sauge pourpre brésilienne",
      "description": "Une plante à fleurs connue pour ses fleurs violettes et son feuillage attrayant. Elle prospère à la lumière moyenne et nécessite un arrosage modéré."
    },
    "it": {
      "common_name": "Salvia viola brasiliana",
      "description": "Una pianta da fiore conosciuta per i suoi fiori viola e il fogliame attraente. Cresce bene con luce media e richiede irrigazione moderata."
    },
    "es": {
      "common_name": "Salvia morada brasileña",
      "description": "Planta con flores conocidas por sus flores moradas y su follaje atractivo. Requiere luz media y riego moderado."
    },
    "pt": {
      "common_name": "Sálvia roxa brasileira",
      "description": "Uma planta com flores conhecidas por suas flores roxas e folhagem atraente. Cresce bem com luz média e precisa de rega moderada."
    },
    "ar": {
      "common_name": "المريمية البنفسجية البرازيلية",
      "description": "نبات مزهر يعرف بأزهاره البنفسجية وأوراقه الجذابة. يزدهر في الضوء المتوسط ويتطلب ريًا معتدلًا."
    },
    "hi": {
      "common_name": "ब्राज़ीलियाई बैंगनी शालवी",
      "description": "एक फूलों वाली पौधों जिसे उसके बैंगनी फूलों और आकर्षक पत्तियों के लिए जाना जाता है। यह मध्यम प्रकाश में पनपता है और इसमें मध्यम पानी की आवश्यकता होती है।"
    },
    "zh": {
      "common_name": "巴西紫色鼠尾草",
      "description": "一种以紫色花朵和吸引人的叶子而闻名的开花植物。它在中等光照下茁壮成长，并需要适量的浇水。"
    },
    "ja": {
      "common_name": "ブラジル紫サルビア",
      "description": "紫色の花と魅力的な葉を持つことで知られる開花植物。中程度の光で育ち、適度な水やりが必要です。"
    },
    "ko": {
      "common_name": "브라질 자주색 세이지",
      "description": "자주색 꽃과 매력적인 잎으로 알려진 꽃 식물입니다. 중간 정도의 빛에서 잘 자라며 적당한 물주기가 필요합니다."
    }
  },
