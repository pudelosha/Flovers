  "translations": {
    "en": {
      "common_name": "Black Hellebore",
      "description": "Helleborus niger is a perennial plant known for its striking dark flowers. It thrives in shaded environments and requires moderate care."
    },
    "pl": {
      "common_name": "Czarny ciemiernik",
      "description": "Helleborus niger to roślina wieloletnia, znana z ciemnych kwiatów. Rośnie w zacienionych miejscach i wymaga umiarkowanej opieki."
    },
    "de": {
      "common_name": "Schwarze Nieswurz",
      "description": "Helleborus niger ist eine mehrjährige Pflanze, die für ihre dunklen Blüten bekannt ist. Sie gedeiht in schattigen Umgebungen und erfordert mäßige Pflege."
    },
    "fr": {
      "common_name": "Hellébore noire",
      "description": "Helleborus niger est une plante vivace connue pour ses fleurs sombres. Elle prospère dans des environnements ombragés et nécessite des soins modérés."
    },
    "it": {
      "common_name": "Elleboro nero",
      "description": "Helleborus niger è una pianta perenne nota per i suoi fiori scuri. Cresce in ambienti ombreggiati e richiede una cura moderata."
    },
    "es": {
      "common_name": "Helleboro negro",
      "description": "Helleborus niger es una planta perenne conocida por sus flores oscuras. Crece en entornos sombreados y requiere cuidados moderados."
    },
    "pt": {
      "common_name": "Hellebore negro",
      "description": "Helleborus niger é uma planta perene conhecida por suas flores escuras. Cresce em ambientes sombreados e requer cuidados moderados."
    },
    "ar": {
      "common_name": "الهيلوبور الأسود",
      "description": "Helleborus niger هو نبات معمر معروف بأزهاره الداكنة. ينمو في بيئات مظللة ويتطلب رعاية معتدلة."
    },
    "hi": {
      "common_name": "काले हेलीबोर",
      "description": "Helleborus niger एक बहुवर्षीय पौधा है जो अपने गहरे फूलों के लिए प्रसिद्ध है। यह छायादार वातावरण में उगता है और इसे मध्यम देखभाल की आवश्यकता होती है।"
    },
    "zh": {
      "common_name": "黑色圣诞玫瑰",
      "description": "Helleborus niger 是一种多年生植物，以其深色的花朵而闻名。它在阴凉环境中生长，需要适度的护理。"
    },
    "ja": {
      "common_name": "ブラックヘレボルス",
      "description": "Helleborus nigerは、暗い花で知られる多年草です。日陰の環境で育ち、適度な手入れが必要です。"
    },
    "ko": {
      "common_name": "검은 헬리보르스",
      "description": "Helleborus niger는 어두운 꽃으로 유명한 다년생 식물입니다. 그늘진 환경에서 자라며 적당한 관리가 필요합니다."
    }
  },
