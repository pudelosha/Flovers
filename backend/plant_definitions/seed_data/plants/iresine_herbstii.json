  "translations": {
    "en": {
      "common_name": "Bloodleaf",
      "description": "Herbst's Bloodleaf is a tropical foliage plant with striking red leaves. It prefers bright, indirect light and consistently moist soil, and benefits from regular feeding."
    },
    "pl": {
      "common_name": "Iresine krwista",
      "description": "Iresine herbstii to tropikalna roślina ozdobna o czerwonych liściach. Wymaga jasnego światła pośredniego i wilgotnej gleby oraz korzyści z regularnego nawożenia."
    },
    "de": {
      "common_name": "Blutblatt",
      "description": "Iresine herbstii ist eine tropische Zierpflanze mit roten Blättern. Sie bevorzugt helles indirektes Licht und gleichmäßig feuchte Erde sowie regelmäßige Düngung."
    },
    "fr": {
      "common_name": "Feuille de sang",
      "description": "Iresine herbstii est une plante tropicale à feuilles rouges. Elle préfère une lumière indirecte vive et un sol uniformément humide avec engrais régulier."
    },
    "it": {
      "common_name": "Foglia di sangue",
      "description": "Iresine herbstii è una pianta tropicale con foglie rosse. Predilige luce indiretta brillante e terreno umido con concimazioni regolari."
    },
    "es": {
      "common_name": "Hoja de sangre",
      "description": "Iresine herbstii es una planta tropical de hojas rojas. Prefiere luz indirecta brillante y suelo húmedo con fertilización regular."
    },
    "pt": {
      "common_name": "Folha de sangue",
      "description": "Iresine herbstii é uma planta tropical de folhas vermelhas. Prefere luz indireta brilhante e solo úmido com adubação regular."
    },
    "ar": {
      "common_name": "ورقة دم",
      "description": "Iresine herbstii هي نبتة استوائية بأوراق حمراء. تفضل ضوءًا غير مباشر مشرق وتربة رطبة مع تغذية منتظمة."
    },
    "hi": {
      "common_name": "ब्लडलीफ",
      "description": "Iresine herbstii एक उष्णकटिबंधीय लाल पत्तों वाला पौधा है। यह उज्ज्वल अप्रत्यक्ष प्रकाश और नम मिट्टी पसंद करता है।"
    },
    "zh": {
      "common_name": "血叶草",
      "description": "Iresine herbstii 是一种热带红叶植物。喜明亮间接光和保持湿润的土壤，定期施肥有益。"
    },
    "ja": {
      "common_name": "ブラッドリーフ",
      "description": "Iresine herbstii は赤い葉の熱帯植物です。明るい間接光と湿った土を好み、定期的な施肥が有益です。"
    },
    "ko": {
      "common_name": "블러드리프",
      "description": "Iresine herbstii 는 빨간 잎의 열대 식물입니다. 밝은 간접광과 촉촉한 흙을 좋아하며 정기 비료가 도움이 됩니다."
    }
  },
