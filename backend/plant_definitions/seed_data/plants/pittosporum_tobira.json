  "translations": {
    "en": {
      "common_name": "Japanese Pittosporum",
      "description": "Pittosporum tobira is a small evergreen shrub. It is native to East Asia and popular for its fragrant flowers."
    },
    "pl": {
      "common_name": "Pittosporum japoński",
      "description": "Pittosporum tobira to niewielki krzew zimozielony. Pochodzi z Azji Wschodniej i jest popularny ze względu na swoje pachnące kwiaty."
    },
    "de": {
      "common_name": "Japanischer Pittosporum",
      "description": "Pittosporum tobira ist ein kleiner immergrüner Strauch. Er ist in Ostasien beheimatet und für seine duftenden Blumen bekannt."
    },
    "fr": {
      "common_name": "Pittosporum japonais",
      "description": "Pittosporum tobira est un petit arbuste à feuillage persistant. Il est originaire d'Asie de l'Est et apprécié pour ses fleurs parfumées."
    },
    "it": {
      "common_name": "Pittosporum giapponese",
      "description": "Pittosporum tobira è un piccolo cespuglio sempreverde. È originario dell'Asia orientale ed è popolare per i suoi fiori profumati."
    },
    "es": {
      "common_name": "Pittosporum japonés",
      "description": "Pittosporum tobira es un pequeño arbusto perenne. Es originario del este de Asia y es popular por sus flores fragantes."
    },
    "pt": {
      "common_name": "Pittosporum japonês",
      "description": "Pittosporum tobira é um pequeno arbusto perene. É originário do Leste Asiático e popular por suas flores perfumadas."
    },
    "ar": {
      "common_name": "بيتوسبوروم ياباني",
      "description": "Pittosporum tobira هو شجيرة دائمة الخضرة صغيرة. وهو موطنه شرق آسيا ويشهر بزهوره العطرة."
    },
    "hi": {
      "common_name": "जापानी पिटोसपोरोम",
      "description": "Pittosporum tobira एक छोटा सदाबहार झाड़ी है। यह पूर्वी एशिया का मूल निवासी है और इसकी खुशबूदार फूलों के लिए प्रसिद्ध है।"
    },
    "zh": {
      "common_name": "日本木犀",
      "description": "Pittosporum tobira 是一种小型常绿灌木。它原产于东亚，以其芳香的花朵而闻名。"
    },
    "ja": {
      "common_name": "ジャパニーズピットスポルム",
      "description": "Pittosporum tobira は小さな常緑低木で、東アジア原産で香りの良い花で人気があります。"
    },
    "ko": {
      "common_name": "일본 피토스포럼",
      "description": "Pittosporum tobira는 작은 상록 관목입니다. 동아시아가 원산지이며 향기로운 꽃으로 유명합니다."
    }
