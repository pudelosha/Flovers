  "translations": {
    "en": {
      "common_name": "Bashful Primrose",
      "description": "Primula obconica is a perennial flowering plant native to China. It thrives in cool, moist conditions with medium sunlight."
    },
    "pl": {
      "common_name": "Przypołudnik wstydliwy",
      "description": "Primula obconica to roślina wieloletnia pochodząca z Chin. Wymaga chłodnych, wilgotnych warunków oraz średniego nasłonecznienia."
    },
    "de": {
      "common_name": "Schüchterne Primel",
      "description": "Primula obconica ist eine mehrjährige Blume aus China. Sie gedeiht in kühlen, feuchten Bedingungen bei mittlerer Sonneneinstrahlung."
    },
    "fr": {
      "common_name": "Primevère timide",
      "description": "Primula obconica est une plante vivace originaire de Chine. Elle prospère dans des conditions fraîches et humides avec une lumière moyenne."
    },
    "it": {
      "common_name": "Primula timida",
      "description": "Primula obconica è una pianta perenne originaria della Cina. Cresce in condizioni fresche e umide con luce solare media."
    },
    "es": {
      "common_name": "Primavera tímida",
      "description": "Primula obconica es una planta perenne originaria de China. Crece en condiciones frescas y húmedas con luz media."
    },
    "pt": {
      "common_name": "Primula tímida",
      "description": "Primula obconica é uma planta perene originária da China. Ela cresce em condições frescas e úmidas com luz média."
    },
    "ar": {
      "common_name": "زهرة الربيع الخجولة",
      "description": "Primula obconica هي نبات معمّر موطنه الصين. ينمو في ظروف باردة ورطبة مع ضوء متوسط."
    },
    "hi": {
      "common_name": "संकोची प्राइमुला",
      "description": "Primula obconica एक बारहमासी फूल वाला पौधा है जो चीन का मूल निवासी है। यह ठंडे और नम वातावरण में और मध्यम धूप में बढ़ता है।"
    },
    "zh": {
      "common_name": "腼腆的报春花",
      "description": "Primula obconica 是一种多年生开花植物，原产于中国。它在凉爽、潮湿的环境中生长，光照适中。"
    },
    "ja": {
      "common_name": "恥ずかしがり屋のサクラソウ",
      "description": "Primula obconicaは中国原産の多年草です。涼しく湿った環境で中程度の光を好んで成長します。"
    },
    "ko": {
      "common_name": "수줍은 제비꽃",
      "description": "Primula obconica는 중국 원산의 다년생 꽃 식물입니다. 차갑고 습한 환경에서 중간 정도의 햇빛을 받으며 자랍니다."
    }
  },
