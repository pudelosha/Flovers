  "translations": {
    "en": {
      "common_name": "Matanzas Melocactus",
      "description": "Melocactus matanzus is a slow-growing cactus native to Cuba. It requires high light and medium watering intervals."
    },
    "pl": {
      "common_name": "Melokaktus Matanzas",
      "description": "Melocactus matanzus to wolno rosnący kaktus pochodzący z Kuby. Wymaga wysokiego nasłonecznienia i średnich odstępów w podlewaniu."
    },
    "de": {
      "common_name": "Matanzas-Melocactus",
      "description": "Melocactus matanzus ist ein langsam wachsender Kaktus aus Kuba. Er benötigt viel Licht und mittlere Bewässerungsintervalle."
    },
    "fr": {
      "common_name": "Melocactus de Matanzas",
      "description": "Le Melocactus matanzus est un cactus à croissance lente originaire de Cuba. Il nécessite une lumière intense et des intervalles d'arrosage moyens."
    },
    "it": {
      "common_name": "Melocactus di Matanzas",
      "description": "Il Melocactus matanzus è un cactus a crescita lenta originario di Cuba. Richiede una forte illuminazione e intervalli di annaffiatura medi."
    },
    "es": {
      "common_name": "Melocactus de Matanzas",
      "description": "El Melocactus matanzus es un cactus de crecimiento lento originario de Cuba. Requiere luz intensa e intervalos de riego medios."
    },
    "pt": {
      "common_name": "Melocactus de Matanzas",
      "description": "Melocactus matanzus é um cacto de crescimento lento originário de Cuba. Exige luz alta e intervalos de rega médios."
    },
    "ar": {
      "common_name": "ميلوككتوس ماتانزاس",
      "description": "Melocactus matanzus هو صبار بطيء النمو موطنه كوبا. يتطلب ضوءًا عاليًا وفترات سقي متوسطة."
    },
    "hi": {
      "common_name": "मैटांज़ास मेलोकैक्टस",
      "description": "Melocactus matanzus एक धीमी गति से बढ़ने वाला कैक्टस है जो क्यूबा का मूल निवासी है। इसे उच्च रोशनी और मध्यम पानी की आवश्यकता होती है।"
    },
    "zh": {
      "common_name": "马坦萨斯美洲柱",
      "description": "Melocactus matanzus 是一种生长缓慢的仙人掌，原产于古巴。它需要高强度的光照和中等的浇水间隔。"
    },
    "ja": {
      "common_name": "マタンザス・メロカクタス",
      "description": "Melocactus matanzusはキューバ原産の成長が遅いサボテンです。高い光と中程度の水やりの間隔が必要です。"
    },
    "ko": {
      "common_name": "마탄자스 멜로카쿠투스",
      "description": "Melocactus matanzus는 쿠바 원산지의 느리게 자라는 선인장입니다. 높은 햇빛과 중간 정도의 물 주기가 필요합니다."
    }
  },
