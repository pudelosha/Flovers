  "translations": {
    "en": {
      "common_name": "Umbrella Fig",
      "description": "Ficus umbellata is a tropical shrub native to Africa. It thrives in medium light and moderate watering."
    },
    "pl": {
      "common_name": "Figowiec parasolowy",
      "description": "Ficus umbellata to tropikalny krzew pochodzący z Afryki. Rośnie dobrze w średnim świetle i umiarkowanym podlewaniu."
    },
    "de": {
      "common_name": "Schirmfeige",
      "description": "Ficus umbellata ist ein tropischer Strauch aus Afrika. Er gedeiht bei mittlerem Licht und mäßigem Gießen."
    },
    "fr": {
      "common_name": "Figuier parapluie",
      "description": "Ficus umbellata est un arbuste tropical originaire d'Afrique. Il prospère à lumière moyenne et arrosage modéré."
    },
    "it": {
      "common_name": "Ficus ombrello",
      "description": "Ficus umbellata è un arbusto tropicale originario dell'Africa. Cresce bene con luce media e annaffiature moderate."
    },
    "es": {
      "common_name": "Higuera paraguas",
      "description": "Ficus umbellata es un arbusto tropical originario de África. Crece bien con luz media y riego moderado."
    },
    "pt": {
      "common_name": "Ficus sombrinha",
      "description": "Ficus umbellata é um arbusto tropical originário da África. Desenvolve-se bem com luz média e rega moderada."
    },
    "ar": {
      "common_name": "توتumbrella",
      "description": "Ficus umbellata هو شجيرة استوائية موطنها أفريقيا. يزدهر في الضوء المتوسط وسقي معتدل."
    },
    "hi": {
      "common_name": "छाता फिकस",
      "description": "Ficus umbellata एक उष्णकटिबंधीय झाड़ी है जो अफ्रीका का मूल है। यह मध्यम रोशनी और हल्के पानी में अच्छे से बढ़ता है।"
    },
    "zh": {
      "common_name": "伞形榕",
      "description": "Ficus umbellata 是一种原产于非洲的热带灌木。它在中等光照和适量浇水的条件下茁壮成长。"
    },
    "ja": {
      "common_name": "アンブレラフィカス",
      "description": "Ficus umbellata はアフリカ原産の熱帯低木です。中程度の光と適度な水やりでよく育ちます。"
    },
    "ko": {
      "common_name": "우산피쿠스",
      "description": "Ficus umbellata는 아프리카 원산의 열대 관목입니다. 중간 정도의 빛과 적당한 물주기로 잘 자랍니다."
    }
