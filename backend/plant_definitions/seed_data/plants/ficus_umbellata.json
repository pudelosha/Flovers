  "translations": {
    "en": {
      "common_name": "Umbrella Fig",
      "description": "Ficus umbellata is a tropical fig with large, heart-shaped leaves. It grows naturally in West Africa and adapts well to indoor conditions with stable warmth and light."
    },
    "pl": {
      "common_name": "Figowiec parasolowy",
      "description": "Ficus umbellata to tropikalny figowiec o dużych, sercowatych liściach. Pochodzi z Afryki Zachodniej i dobrze rośnie w warunkach domowych."
    },
    "de": {
      "common_name": "Schirm-Feige",
      "description": "Ficus umbellata ist eine tropische Feige mit großen, herzförmigen Blättern. Sie stammt aus Westafrika und eignet sich für helle Innenräume."
    },
    "fr": {
      "common_name": "Figuier parapluie",
      "description": "Ficus umbellata est un figuier tropical aux grandes feuilles en forme de cœur. Originaire d’Afrique de l’Ouest, il se cultive bien en intérieur lumineux."
    },
    "it": {
      "common_name": "Fico ombrello",
      "description": "Il Ficus umbellata è un fico tropicale con grandi foglie a forma di cuore. Proviene dall’Africa occidentale e si adatta agli ambienti interni."
    },
    "es": {
      "common_name": "Higuera sombrilla",
      "description": "Ficus umbellata es una higuera tropical de hojas grandes y acorazonadas. Es originaria de África occidental y se cultiva como planta de interior."
    },
    "pt": {
      "common_name": "Figueira-umbela",
      "description": "Ficus umbellata é uma figueira tropical com folhas grandes em forma de coração. É nativa da África Ocidental e adequada para interiores."
    },
    "ar": {
      "common_name": "فيكس المظلة",
      "description": "فيكس أومبيلياتا نبات استوائي بأوراق كبيرة على شكل قلب. موطنه غرب أفريقيا ويناسب الزراعة الداخلية."
    },
    "hi": {
      "common_name": "अम्ब्रेला फाइकस",
      "description": "फिकस अम्बेलाटा एक उष्णकटिबंधीय पौधा है जिसकी पत्तियाँ बड़ी और दिल के आकार की होती हैं। यह पश्चिमी अफ्रीका का मूल निवासी है।"
    },
    "zh": {
      "common_name": "伞叶榕",
      "description": "伞叶榕是一种热带榕树，具有大型心形叶片。原产于西非，适合室内明亮环境。"
    },
    "ja": {
      "common_name": "アンブレラフィカス",
      "description": "フィカス・ウンベラータは大きなハート形の葉を持つ熱帯性のイチジクです。西アフリカ原産で室内向きです。"
    },
    "ko": {
      "common_name": "엄벨라타 고무나무",
      "description": "피쿠스 움벨라타는 큰 하트 모양 잎을 가진 열대 식물입니다. 서아프리카 원산으로 실내 재배에 적합합니다."
    }
  },
