  "translations": {
    "en": {
      "common_name": "Treasure flower",
      "description": "Gazania rigens is a sun-loving flowering plant native to southern Africa. It produces large, colorful daisy-like flowers and tolerates drought well."
    },
    "pl": {
      "common_name": "Gazania",
      "description": "Gazania rigens to roślina kwitnąca pochodząca z Afryki Południowej. Lubi pełne słońce i dobrze znosi suszę."
    },
    "de": {
      "common_name": "Mittagsgold",
      "description": "Gazania rigens stammt aus dem südlichen Afrika. Sie blüht reich in voller Sonne und ist trockenheitsresistent."
    },
    "fr": {
      "common_name": "Gazanie",
      "description": "Gazania rigens est originaire d'Afrique australe. Elle apprécie le plein soleil et résiste bien à la sécheresse."
    },
    "it": {
      "common_name": "Gazania",
      "description": "Gazania rigens è una pianta originaria dell'Africa meridionale. Fiorisce al sole ed è resistente alla siccità."
    },
    "es": {
      "common_name": "Gazania",
      "description": "Gazania rigens es nativa del sur de África. Prefiere sol pleno y requiere poco riego."
    },
    "pt": {
      "common_name": "Gazânia",
      "description": "Gazania rigens é originária do sul da África. Gosta de sol pleno e tolera bem a seca."
    },
    "ar": {
      "common_name": "غازانيا",
      "description": "نبات Gazania rigens موطنه جنوب أفريقيا. يفضل الشمس الكاملة ويتحمل الجفاف."
    },
    "hi": {
      "common_name": "गज़ानिया",
      "description": "Gazania rigens दक्षिणी अफ्रीका का पौधा है। यह तेज धूप और कम पानी में अच्छी तरह बढ़ता है।"
    },
    "zh": {
      "common_name": "勋章菊",
      "description": "Gazania rigens 原产于非洲南部。喜全日照，耐旱性强。"
    },
    "ja": {
      "common_name": "ガザニア",
      "description": "Gazania rigens は南アフリカ原産の植物です。日当たりを好み、乾燥に強いです。"
    },
    "ko": {
      "common_name": "가자니아",
      "description": "Gazania rigens는 남아프리카 원산의 식물입니다. 강한 햇빛과 건조에 강합니다."
    }
  },
